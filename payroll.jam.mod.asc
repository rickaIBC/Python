 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine: do_logs
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine displays the leave request logs.
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2019-04-29      I. Lane         CR20190018
   JPLTEXT=//                      Created.
   JPLTEXT=//      2020-12-23      E. Ramos        ENHC0036106
   JPLTEXT=//                      Save changes when navigating to the logs\
 tab, just like the other tabs do.
   JPLTEXT=//      2025-01-23      A. McCormick    DFCT0014136
   JPLTEXT=//                      Added call to update_comments() to ensure\
 the comments are saved with the timesheet.
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=proc do_logs ()
   JPLTEXT={
   JPLTEXT=        if (force_save_flag == gbl$TRUE)
   JPLTEXT=             UsrMadeChg = 1
   JPLTEXT=
   JPLTEXT=        // must clear so do_valid_screen functions properly
   JPLTEXT=        save_flag = 0
   JPLTEXT=        edit_reqd = 0
   JPLTEXT=
   JPLTEXT=        if ( do_valid_screen() )
   JPLTEXT=        {
   JPLTEXT=                return (-1)
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        if (save_flag == 1 )
   JPLTEXT=             UsrMadeChg = 1
   JPLTEXT=
   JPLTEXT=        //Save payroll data.
   JPLTEXT=        if ( update_comments() )
   JPLTEXT=        {
   JPLTEXT=                return (-1)
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        if ( update_time() )
   JPLTEXT=        {
   JPLTEXT=                return (-1)
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        // Ensure the weekly and daily totals are displayed\
 correctly
   JPLTEXT=        call get_daily_totals()
   JPLTEXT=
   JPLTEXT=        send bundle "timesheet_logs" data active_empid,\
 active_empname, status_array, pp, ppyear, period_total
   JPLTEXT=
   JPLTEXT=        call sm_jform("timesheet_logs")
   JPLTEXT=        return (0)
   JPLTEXT=} //end proc do_leave()
   JPLTEXT=
