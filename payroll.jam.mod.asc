 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine:   set_NumOcc
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine sets the number of occurrences in the
   JPLTEXT=//              synchronized selection group in the entry grid.
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2016-06-20      E. Ramos        CR20160062
   JPLTEXT=//                      Show week 1 and week 2 on the same screen.
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=proc set_NumOcc ()
   JPLTEXT={
   JPLTEXT=        vars occ(10), on_occ(10)
   JPLTEXT=
   JPLTEXT=        w1NumOcc = ut_max(w1grid->num_occurrences,\
 w1grid->onscreen_rows)
   JPLTEXT=        w2NumOcc = ut_max(w2grid->num_occurrences,\
 w2grid->onscreen_rows)
   JPLTEXT=
   JPLTEXT=        return (0)
   JPLTEXT=} //end proc set_NumOcc()
   JPLTEXT=
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine:   do_add_row
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine adds an occurrence to the grid above\
 the current grid occurrence.
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2005-05-31      E. Ramos        CR20050039
   JPLTEXT=//                      Add support for cost structure components.
   JPLTEXT=//      2007-02-21      T. Peace        PR20060893
   JPLTEXT=//                      Add payroll_flag indicator on calls to
   JPLTEXT=//                      load_default_leave procedure.
   JPLTEXT=//      2016-06-20      E. Ramos        CR20160062
   JPLTEXT=//                      Support displaying both weeks on the same\
 screen.
   JPLTEXT=//      2018-02-16      E. Ramos        PR20180035
   JPLTEXT=//                      Insert row below instead of above.
   JPLTEXT=//      2018-04-05      E. Ramos        PR20180111
   JPLTEXT=//                      Increase number of rows in grid when row is\
 inserted.
   JPLTEXT=//      2019-02-04      E. Ramos        CR20180124
   JPLTEXT=//                      Switch to checkboxes.
   JPLTEXT=//      2019-03-01      E. Ramos        CR20180124 REWORK
   JPLTEXT=//                      Update selection group when a row is added.
   JPLTEXT=//                      Call ut_cb_workaround()
   JPLTEXT=//      2019-03-27      E. Ramos        PR20190062
   JPLTEXT=//                      When necessary, fall back and obtain row\
 number from selected_row_num.
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=proc do_add_row ()
   JPLTEXT={
   JPLTEXT=        vars grid_occ(10)
   JPLTEXT=        vars week_num
   JPLTEXT=        call sm_n_clear_array("err_msg_dl")
   JPLTEXT=        call sm_n_clear_array("err_img")
   JPLTEXT=        err_msg_idx = 1
   JPLTEXT=
   JPLTEXT=        add_del_flag = gbl$TRUE
   JPLTEXT=        if ( !security_flag )
   JPLTEXT=        {
   JPLTEXT=                unauthorized_action = gbl$TRUE
   JPLTEXT=                return(0)
   JPLTEXT=        }
   JPLTEXT=        week_num = selected_week_num
   JPLTEXT=
   JPLTEXT=        if (w:(week_num)group->num_occurrences > 1)
   JPLTEXT=        {
   JPLTEXT=                call ut5_msg_information( "Select a single row and\
 then click Add Row to have a new row added underneath." )
   JPLTEXT=                return
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        grid_occ = w:(week_num)group[1]
   JPLTEXT=
   JPLTEXT=        if (grid_occ < 1)
   JPLTEXT=        {
   JPLTEXT=                grid_occ = selected_row_num
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        call sm_i_ioccur("w:(week_num)hrcode", grid_occ + 1, 1)
   JPLTEXT=        w:(week_num)grid->grid_current_occ = grid_occ + 1
   JPLTEXT=        call ut_move_row_inputNameWithFocus(1)
   JPLTEXT=        call ut_increase_grid_rows("w:(week_num)grid")
   JPLTEXT=
   JPLTEXT=        // Set default leave codes (disabled fields are not\
 transmitted from the browser to the server)
   JPLTEXT=        call load_default_leave(gbl$TRUE)
   JPLTEXT=
   JPLTEXT=        call do_valid_screen()
   JPLTEXT=        call set_NumOcc()
   JPLTEXT=
   JPLTEXT=        // Update the selection group to hold the added row
   JPLTEXT=        w:(week_num)group[1] = grid_occ + 1
   JPLTEXT=
   JPLTEXT=        return (0)
   JPLTEXT=} //end proc do_add_row()
   JPLTEXT=
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine:   do_del_row
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine deletes an occurrence from the grid if\
 not an empty "visible" row.
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2005-05-31      E. Ramos        CR20050039
   JPLTEXT=//                      Add support for cost structure components.
   JPLTEXT=//      2007-02-21      T. Peace        PR20060893
   JPLTEXT=//                      Add payroll_flag indicator on calls to
   JPLTEXT=//                      load_default_leave procedure.
   JPLTEXT=//      2007-12-17      T. Peace        PR20070812
   JPLTEXT=//                      Ensure deletion of timesheet row triggers\
 timesheet save.
   JPLTEXT=//      2008-04-24      T. Peace        PR20080212
   JPLTEXT=//                      Ensure that a save operation after row\
 deletion
   JPLTEXT=//                      forces the status reset to UNV.
   JPLTEXT=//      2010-09-30      T. Peace        PR20100336
   JPLTEXT=//                      Skip validation if there is no time entered\
 on row being deleted.
   JPLTEXT=//                      This will prevent status being reset to UNV\
 if there is no relevant change.
   JPLTEXT=//      2014-11-07      E. Ramos        PR20140482 REWORK 2
   JPLTEXT=//                      Ensure weekly and daily totals are\
 presented correctly after using the scissors icon.
   JPLTEXT=//      2016-06-20      E. Ramos        CR20160062
   JPLTEXT=//                      Support displaying both weeks on the same\
 screen.
   JPLTEXT=//      2019-02-04      E. Ramos        CR20180124
   JPLTEXT=//                      Switch to checkboxes and allow deleting\
 multiple rows.
   JPLTEXT=//      2019-03-01      E. Ramos        CR20180124 REWORK
   JPLTEXT=//                      Shorten message to avoid truncation.
   JPLTEXT=//                      Call ut_cb_workaround().
   JPLTEXT=//      2019-03-27      E. Ramos        PR20190062
   JPLTEXT=//                      Avoid calling_ut_cb_workaround.
   JPLTEXT=//      2019-09-03      E. Ramos        ENHC0035628
   JPLTEXT=//                      Clear daystotal when row is deleted.
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=proc do_del_row ()
   JPLTEXT={
   JPLTEXT=        vars grid_occ(10)
   JPLTEXT=        vars skip_validation_flag(10)
   JPLTEXT=        vars wgrid_row_time, wk_grid_row_time
   JPLTEXT=        vars i week_num num_rows
   JPLTEXT=        vars r noval_rows
   JPLTEXT=        vars first_sel_row
   JPLTEXT=
   JPLTEXT=        call sm_n_clear_array("err_msg_dl")
   JPLTEXT=        call sm_n_clear_array("err_img")
   JPLTEXT=        ed_error_count = 0
   JPLTEXT=        err_msg_idx    = 1
   JPLTEXT=
   JPLTEXT=        add_del_flag = gbl$TRUE
   JPLTEXT=        del_row_flag = gbl$TRUE
   JPLTEXT=
   JPLTEXT=        force_save_flag = gbl$TRUE
   JPLTEXT=
   JPLTEXT=        if ( !security_flag )
   JPLTEXT=        {
   JPLTEXT=                unauthorized_action = gbl$TRUE
   JPLTEXT=                return(0)
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        week_num = selected_week_num
   JPLTEXT=
   JPLTEXT=        num_rows = w:(week_num)group->num_occurrences
   JPLTEXT=
   JPLTEXT=        if (w:(week_num)group->num_occurrences < 1)
   JPLTEXT=        {
   JPLTEXT=                call ut5_msg_warning( "Select one or more rows and\
 then click Delete Row(s) %N to have the selected row(s) deleted." )
   JPLTEXT=                return
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        // Save occurrence number of the first row selected for\
 deletion
   JPLTEXT=        first_sel_row = w:(week_num)group[1]
   JPLTEXT=
   JPLTEXT=        if (first_sel_row > 1)
   JPLTEXT=        {
   JPLTEXT=                first_sel_row = first_sel_row - 1
   JPLTEXT=                call ut_move_row_inputNameWithFocus(-1)
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        noval_rows = 0
   JPLTEXT=
   JPLTEXT=        skip_validation_flag = gbl$FALSE
   JPLTEXT=
   JPLTEXT=        for r = num_rows while r>0 step -1
   JPLTEXT=        {
   JPLTEXT=                grid_occ = w:(week_num)group[r]
   JPLTEXT=                w:(week_num)hrcode[grid_occ] = "DEL"
   JPLTEXT=                w:(week_num)daystotal[grid_occ] = ""
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        for r = num_rows while r>0 step -1
   JPLTEXT=        {
   JPLTEXT=                grid_occ = w:(week_num)group[r]
   JPLTEXT=
   JPLTEXT=                wgrid_row_time =\
 ut_hhmm2mm(w:(week_num)day_1[grid_occ])
   JPLTEXT=                wgrid_row_time = wgrid_row_time +\
 ut_hhmm2mm(w:(week_num)day_2[grid_occ])
   JPLTEXT=                wgrid_row_time = wgrid_row_time +\
 ut_hhmm2mm(w:(week_num)day_3[grid_occ])
   JPLTEXT=                wgrid_row_time = wgrid_row_time +\
 ut_hhmm2mm(w:(week_num)day_4[grid_occ])
   JPLTEXT=                wgrid_row_time = wgrid_row_time +\
 ut_hhmm2mm(w:(week_num)day_5[grid_occ])
   JPLTEXT=                wgrid_row_time = wgrid_row_time +\
 ut_hhmm2mm(w:(week_num)day_6[grid_occ])
   JPLTEXT=                wgrid_row_time = wgrid_row_time +\
 ut_hhmm2mm(w:(week_num)day_7[grid_occ])
   JPLTEXT=
   JPLTEXT=                wk_grid_row_time =\
 ut_hhmm2mm(w:(week_num)day_1[grid_occ])
   JPLTEXT=                wk_grid_row_time = wk_grid_row_time +\
 ut_hhmm2mm(w:(week_num)day_2[grid_occ])
   JPLTEXT=                wk_grid_row_time = wk_grid_row_time +\
 ut_hhmm2mm(w:(week_num)day_3[grid_occ])
   JPLTEXT=                wk_grid_row_time = wk_grid_row_time +\
 ut_hhmm2mm(w:(week_num)day_4[grid_occ])
   JPLTEXT=                wk_grid_row_time = wk_grid_row_time +\
 ut_hhmm2mm(w:(week_num)day_5[grid_occ])
   JPLTEXT=                wk_grid_row_time = wk_grid_row_time +\
 ut_hhmm2mm(w:(week_num)day_6[grid_occ])
   JPLTEXT=                wk_grid_row_time = wk_grid_row_time +\
 ut_hhmm2mm(w:(week_num)day_7[grid_occ])
   JPLTEXT=
   JPLTEXT=                if (wgrid_row_time) == 0
   JPLTEXT=                {
   JPLTEXT=                     if (wk_grid_row_time) == 0
   JPLTEXT=                     {
   JPLTEXT=                            noval_rows = noval_rows + 1
   JPLTEXT=                     }
   JPLTEXT=                }
   JPLTEXT=
   JPLTEXT=                // Clear days total
   JPLTEXT=                w:(week_num)daystotal[:grid_occ]=""
   JPLTEXT=
   JPLTEXT=                // Remove row
   JPLTEXT=                call sm_i_doccur("w:(week_num)hrcode", :grid_occ,\
 1)
   JPLTEXT=
   JPLTEXT=                // Remove entry from selection group
   JPLTEXT=                call sm_i_doccur("w:(week_num)group", r, 1)
   JPLTEXT=
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        // Put checkmark on the first row selected for deletion\
 (which has been cleared by now)
   JPLTEXT=        if (week_num==1)
   JPLTEXT=                w1group[1] = first_sel_row
   JPLTEXT=        if (week_num==2)
   JPLTEXT=                w2group[1] = first_sel_row
   JPLTEXT=
   JPLTEXT=        if noval_rows = num_rows
   JPLTEXT=                skip_validation_flag = gbl$TRUE
   JPLTEXT=
   JPLTEXT=        // Set default leave codes (disabled fields are not\
 transmitted from the browser to the server)
   JPLTEXT=        call load_default_leave(gbl$TRUE)
   JPLTEXT=
   JPLTEXT=        if (skip_validation_flag == gbl$FALSE)
   JPLTEXT=             call do_valid_screen()
   JPLTEXT=
   JPLTEXT=        call set_NumOcc()
   JPLTEXT=
   JPLTEXT=        if (num_rows==1)
   JPLTEXT=                call ut5_msg_information( "Row deleted." )
   JPLTEXT=        else
   JPLTEXT=                call ut5_msg_information( num_rows ## " rows\
 deleted." )
   JPLTEXT=
   JPLTEXT=        // Set flag so that clicking on the SAVE button will save\
 the time sheet
   JPLTEXT=        UsrMadeChg = 1
   JPLTEXT=
   JPLTEXT=        // Ensure the weekly and daily totals are displayed\
 correctly
   JPLTEXT=        call get_daily_totals()
   JPLTEXT=
   JPLTEXT=        call get_week_totals()
   JPLTEXT=
   JPLTEXT=        return (0)
   JPLTEXT=
   JPLTEXT=} //end proc do_del_row()
   JPLTEXT=
