 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine:   select_comments
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine performs the selection for the\
 comments section of the screen
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2024-08-15      A. McCormick    ENHC0037149
   JPLTEXT=//                      Created from time_comments.jam
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=proc select_comments()
   JPLTEXT={
   JPLTEXT=        vars alias_stmt, pre_stmt, sql_stmt, post_stmt
   JPLTEXT=        vars retval,i, num_occ
   JPLTEXT=        retval = 0
   JPLTEXT=
   JPLTEXT=        if(ppyear == 0)
   JPLTEXT=                return 0
   JPLTEXT=
   JPLTEXT=        if (!amendment)
   JPLTEXT=        {
   JPLTEXT=                c_pp = pp
   JPLTEXT=                c_ppyear = ppyear
   JPLTEXT=                c_process_pp = ""
   JPLTEXT=                c_process_ppyear = ""
   JPLTEXT=                c_process_seq = ""
   JPLTEXT=                c_satstat2 = ""
   JPLTEXT=        }
   JPLTEXT=        else
   JPLTEXT=        {
   JPLTEXT=                c_pp = prior_tna_data [idx$PAY_CORR_PP]
   JPLTEXT=                c_ppyear = prior_tna_data [idx$PAY_CORR_YEAR]
   JPLTEXT=                c_process_pp = pp
   JPLTEXT=                c_process_ppyear = ppyear
   JPLTEXT=                c_process_seq = prior_tna_data[idx$PAY_CURR_SEQ]
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        call sm_n_clear_array("emp_comment")
   JPLTEXT=        call sm_n_clear_array("emp_updated_by")
   JPLTEXT=        call sm_n_clear_array("emp_update_date")
   JPLTEXT=        call sm_n_clear_array("tk_comment")
   JPLTEXT=        call sm_n_clear_array("tk_updated_by")
   JPLTEXT=        call sm_n_clear_array("tk_update_date")
   JPLTEXT=
   JPLTEXT=        if (!amendment)
   JPLTEXT=        {
   JPLTEXT=                if !(ppyear > 0 && pp > 0 )
   JPLTEXT=                {
   JPLTEXT=                        return retval
   JPLTEXT=                }
   JPLTEXT=                alias_stmt = "ts_status"
   JPLTEXT=                sql_stmt = "select satstat2 from ppstat where\
 ppyear = :c_ppyear and pp = :c_pp and empid = :+active_empid"
   JPLTEXT=                retval = ut_dbms_sql(alias_stmt, sql_stmt)
   JPLTEXT=                if(retval == 0)
   JPLTEXT=                        ts_status = 'UNV'
   JPLTEXT=
   JPLTEXT=                alias_stmt = "emp_comment"
   JPLTEXT=                pre_stmt  = "select description from\
 timesheet_comments "
   JPLTEXT=                sql_stmt  = "where ppyear = :c_ppyear and pp =\
 :c_pp and empid = :+active_empid and role = 'EMPLOYEE' "
   JPLTEXT=                post_stmt = "order by seq_num"
   JPLTEXT=                retval = ut_dbms_big_sql (alias_stmt, pre_stmt,\
 sql_stmt, post_stmt)
   JPLTEXT=
   JPLTEXT=                alias_stmt = "emp_updated_by, emp_update_date"
   JPLTEXT=                pre_stmt  = "select B.empname,\
 to_char(A.update_date, 'MM/DD/YYYY HH::MI AM') from timesheet_comments A, "
   JPLTEXT=                sql_stmt  = "employee B where A.ppyear = :c_ppyear\
 and A.pp = :c_pp and A.empid = :+active_empid and A.role = 'EMPLOYEE' "
   JPLTEXT=                post_stmt = "and A.updated_by = B.empid (+) order\
 by update_date desc"
   JPLTEXT=                retval = ut_dbms_big_sql (alias_stmt, pre_stmt,\
 sql_stmt, post_stmt)
   JPLTEXT=
   JPLTEXT=                alias_stmt = "tk_comment"
   JPLTEXT=                pre_stmt  = "select description from\
 timesheet_comments "
   JPLTEXT=                sql_stmt  = "where ppyear = :c_ppyear and pp =\
 :c_pp and empid = :+active_empid and role = 'TIMEKEEPER' "
   JPLTEXT=                post_stmt = "order by seq_num"
   JPLTEXT=                retval = ut_dbms_big_sql (alias_stmt, pre_stmt,\
 sql_stmt, post_stmt)
   JPLTEXT=
   JPLTEXT=                alias_stmt = "tk_updated_by, tk_update_date"
   JPLTEXT=                pre_stmt  = "select B.empname,\
 to_char(A.update_date, 'MM/DD/YYYY HH::MI AM') from timesheet_comments A, "
   JPLTEXT=                sql_stmt  = "employee B where A.ppyear = :c_ppyear\
 and A.pp = :c_pp and A.empid = :+active_empid and A.role = 'TIMEKEEPER' "
   JPLTEXT=                post_stmt = "and A.updated_by = B.empid (+) order\
 by update_date desc"
   JPLTEXT=                retval = ut_dbms_big_sql (alias_stmt, pre_stmt,\
 sql_stmt, post_stmt)
   JPLTEXT=
   JPLTEXT=                call sm_n_copyarray("orig_emp_comment",\
 "emp_comment")
   JPLTEXT=                call sm_n_copyarray("orig_tk_comment",\
 "tk_comment")
   JPLTEXT=
   JPLTEXT=                emp_updated = emp_updated_by[1]
   JPLTEXT=                emp_u_date = emp_update_date[1]
   JPLTEXT=                tk_updated = tk_updated_by[1]
   JPLTEXT=                tk_u_date = tk_update_date[1]
   JPLTEXT=
   JPLTEXT=        }
   JPLTEXT=        else
   JPLTEXT=        {
   JPLTEXT=                alias_stmt = "ts_status"
   JPLTEXT=                sql_stmt = "select satstat2 from amend_ppstat where\
 ppyear = :c_ppyear and pp = :c_pp and process_ppyear = :c_process_ppyear and\
 process_seq = :c_process_seq"
   JPLTEXT=                sql_stmt = sql_stmt ## " and process_pp =\
 :c_process_pp and empid = :+active_empid"
   JPLTEXT=                retval = ut_dbms_sql(alias_stmt, sql_stmt)
   JPLTEXT=                if(retval == 0)
   JPLTEXT=                        ts_status = 'UNV'
   JPLTEXT=
   JPLTEXT=                alias_stmt = "emp_comment"
   JPLTEXT=                pre_stmt  = "select description from\
 amend_timesheet_comments "
   JPLTEXT=                sql_stmt  = "where ppyear = :c_ppyear and pp =\
 :c_pp and empid = :+active_empid and role = 'EMPLOYEE' "
   JPLTEXT=                post_stmt = "and process_ppyear = :c_process_ppyear\
 and process_pp = :c_process_pp and process_seq = :c_process_seq order by\
 seq_num"
   JPLTEXT=                retval = ut_dbms_big_sql (alias_stmt, pre_stmt,\
 sql_stmt, post_stmt)
   JPLTEXT=
   JPLTEXT=                alias_stmt = "emp_updated_by, emp_update_date"
   JPLTEXT=                pre_stmt  = "select B.empname,\
 to_char(A.update_date, 'MM/DD/YYYY HH::MI AM') from amend_timesheet_comments\
 A, "
   JPLTEXT=                sql_stmt  = "employee B where A.ppyear = :c_ppyear\
 and A.pp = :c_pp and A.empid = :+active_empid and A.role = 'EMPLOYEE' and\
 A.updated_by = B.empid (+) "
   JPLTEXT=                post_stmt = "and A.process_ppyear =\
 :c_process_ppyear and A.process_pp = :c_process_pp and A.process_seq =\
 :c_process_seq order by update_date desc"
   JPLTEXT=                retval = ut_dbms_big_sql (alias_stmt, pre_stmt,\
 sql_stmt, post_stmt)
   JPLTEXT=
   JPLTEXT=                alias_stmt = "tk_comment"
   JPLTEXT=                pre_stmt  = "select description from\
 amend_timesheet_comments "
   JPLTEXT=                sql_stmt  = "where ppyear = :c_ppyear and pp =\
 :c_pp and empid = :+active_empid and role = 'TIMEKEEPER' "
   JPLTEXT=                post_stmt = "and process_ppyear = :c_process_ppyear\
 and process_pp = :c_process_pp and process_seq = :c_process_seq order by\
 seq_num"
   JPLTEXT=                retval = ut_dbms_big_sql (alias_stmt, pre_stmt,\
 sql_stmt, post_stmt)
   JPLTEXT=
   JPLTEXT=                alias_stmt = "tk_updated_by, tk_update_date"
   JPLTEXT=                pre_stmt  = "select B.empname,\
 to_char(A.update_date, 'MM/DD/YYYY HH::MI AM') from amend_timesheet_comments\
 A, "
   JPLTEXT=                sql_stmt  = "employee B where A.ppyear = :c_ppyear\
 and A.pp = :c_pp and A.empid = :+active_empid and A.role = 'TIMEKEEPER' and\
 A.updated_by = B.empid (+) "
   JPLTEXT=                post_stmt = "and A.process_ppyear =\
 :c_process_ppyear and A.process_pp = :c_process_pp and A.process_seq =\
 :c_process_seq order by update_date desc"
   JPLTEXT=                retval = ut_dbms_big_sql (alias_stmt, pre_stmt,\
 sql_stmt, post_stmt)
   JPLTEXT=
   JPLTEXT=                call sm_n_copyarray("orig_emp_comment",\
 "emp_comment")
   JPLTEXT=                call sm_n_copyarray("orig_tk_comment",\
 "tk_comment")
   JPLTEXT=
   JPLTEXT=                emp_updated = emp_updated_by[1]
   JPLTEXT=                emp_u_date = emp_update_date[1]
   JPLTEXT=                tk_updated = tk_updated_by[1]
   JPLTEXT=                tk_u_date = tk_update_date[1]
   JPLTEXT=
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        return(retval)
   JPLTEXT=}
   JPLTEXT=
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine:   update_comments
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine performs the update routine for the\
 screen
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2024-08-15      A. McCormick    ENHC0037149
   JPLTEXT=//                      Created from time_comments.jam
   JPLTEXT=//      2025-01-23      A. McCormick    DFCT0014136
   JPLTEXT=//                      Moved data saved messages outside of this\
 function.
   JPLTEXT=//      2025-03-07      E. Ramos        DFCT0014226
   JPLTEXT=//                      Exit early if current user is not allowed\
 to change comments.
   JPLTEXT=//                      Prevents clearing comments in the database\
 when role is not allowed to edit comments.
   JPLTEXT=//      2025-04-15      T. Peace        DFCT0014160
   JPLTEXT=//                      Removed code checking if E-PAY-045 is
   JPLTEXT=//                      active in order to prevent running
   JPLTEXT=//                      edit checks if only comments are added.
   JPLTEXT=//                      This will be performed after row delete
   JPLTEXT=//                      operations are processed. 
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=proc update_comments()
   JPLTEXT={
   JPLTEXT=        vars retval(10)
   JPLTEXT=        vars sql_stmt(255)
   JPLTEXT=
   JPLTEXT=        // Determine if updating comments is NOT allowed (same\
 condition is used in proc refresh_screen to detect if time cannot be\
 changed).
   JPLTEXT=        if (((!amendment) && \\
   JPLTEXT=                ((tna_data [idx$ACTIVE_ROLE] == gbl$ROLE_CERTIFIER)\
 || \\
   JPLTEXT=                (tna_data [idx$ACTIVE_ROLE] ==\
 gbl$ROLE_ADMINISTRATOR) || \\
   JPLTEXT=                (ut_bitop_and(:tna_data_name [idx$ACTIVE_PROP],\
 gbl$ACTIVE_PROP_SIOREQ)))) || \\
   JPLTEXT=                ((amendment) && (!(tna_data [idx$ACTIVE_ROLE] &\
 cfg$AMENDMENT_ROLE))))
   JPLTEXT=        {
   JPLTEXT=                return
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        if (!amendment)
   JPLTEXT=        {
   JPLTEXT=                c_pp = pp
   JPLTEXT=                c_ppyear = ppyear
   JPLTEXT=                c_process_pp = ""
   JPLTEXT=                c_process_ppyear = ""
   JPLTEXT=                c_process_seq = ""
   JPLTEXT=                c_satstat2 = ""
   JPLTEXT=        }
   JPLTEXT=        else
   JPLTEXT=        {
   JPLTEXT=                c_pp = prior_tna_data [idx$PAY_CORR_PP]
   JPLTEXT=                c_ppyear = prior_tna_data [idx$PAY_CORR_YEAR]
   JPLTEXT=                c_process_pp = pp
   JPLTEXT=                c_process_ppyear = ppyear
   JPLTEXT=                c_process_seq = prior_tna_data[idx$PAY_CURR_SEQ]
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        if (cfg$websphere)
   JPLTEXT=        {
   JPLTEXT=                obj_retval = sm_obj_call (payroll_hndl,\
 "update_comments", tna_data, active_empid, c_ppyear, c_pp, amendment, \\
   JPLTEXT=                                          c_process_ppyear,\
 c_process_pp, c_process_seq, emp_comment, orig_emp_comment, tk_comment,\
 orig_tk_comment)
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        if !(retval)
   JPLTEXT=                comment_save_flag = gbl$TRUE
   JPLTEXT=
   JPLTEXT=        return(retval)
   JPLTEXT=}
   JPLTEXT=
