   JPLTEXT=//      Routine: do_leave
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine saves the payroll data to the database
   JPLTEXT=//    and displays the leave balance screen.
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2000-09-10      D. Bodlien / E. Ren     CR20000398
   JPLTEXT=//                      Add processing for amendments.
   JPLTEXT=//      2003-12-12      J. Hollett      CR20030273
   JPLTEXT=//                      Switch sm_jwindow call to sm_jform to
   JPLTEXT=//                      ensure account number transfer.
   JPLTEXT=//      2005-05-31      E. Ramos        CR20050039
   JPLTEXT=//                      Add support for cost structure components.
   JPLTEXT=//      2006-04-20      T. Peace        PR20060258
   JPLTEXT=//                      Add status change and edit checking fields\
 to the update_time EJB call in order to
   JPLTEXT=//                      allow timesheet and status changes to be\
 completed within a single database transaction.
   JPLTEXT=//      2006-08-01      T. Peace        PR20060487 - rework
   JPLTEXT=//                      Clear save_flag and edit_reqd flags before\
 initiating save processing when exiting timesheet
   JPLTEXT=//                      via one of the payroll screen tabs.
   JPLTEXT=//      2007-02-21      T. Peace        PR20060893
   JPLTEXT=//                      Add payroll_flag indicator on calls to\
 load_default_leave procedure.
   JPLTEXT=//      2007-11-08      T. Peace        CR20070158
   JPLTEXT=//                      Set save process control flag as\
 appropriate to eliminate unnecessary hrsprim/amend_hrsprim
   JPLTEXT=//                      deletion/re-insertion cycle for timesheet\
 when no data change has been made.
   JPLTEXT=//      2007-12-17      T. Peace        PR20070812
   JPLTEXT=//                      Ensure deletion of timesheet row triggers\
 timesheet save on exit via screen tab.
   JPLTEXT=//      2012-08-06      A. McCormick    CR20120016
   JPLTEXT=//                      Added functionality to allow Importing\
 timesheet lines from input_hrsprim.
   JPLTEXT=//      2016-07-19      E. Ramos        PR20160268
   JPLTEXT=//                      Removed references to the flag\
 other_wk_chg_flag which is now obsolete.
   JPLTEXT=//      2025-01-23      A. McCormick    DFCT0014136
   JPLTEXT=//                      Added call to update_comments() to ensure\
 the comments are saved with the timesheet.
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=proc do_leave ()
   JPLTEXT={
   JPLTEXT=        UsrMadeChg = 0
   JPLTEXT=        if (force_save_flag == gbl$TRUE)
   JPLTEXT=             UsrMadeChg = 1
   JPLTEXT=
   JPLTEXT=        // must clear so do_valid_screen functions properly
   JPLTEXT=        save_flag = 0
   JPLTEXT=        edit_reqd = 0
   JPLTEXT=
   JPLTEXT=        // Set default leave codes (disabled fields are not\
 transmitted from the browser to the server)
   JPLTEXT=        call load_default_leave(gbl$TRUE)
   JPLTEXT=
   JPLTEXT=        if ( do_valid_screen() )
   JPLTEXT=        {
   JPLTEXT=                return (-1)
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        if (save_flag == 1 )
   JPLTEXT=             UsrMadeChg = 1
   JPLTEXT=
   JPLTEXT=        //Save comments
   JPLTEXT=        if ( update_comments() )
   JPLTEXT=        {
   JPLTEXT=                return (-1)
   JPLTEXT=        }
   JPLTEXT=        //Save payroll data.
   JPLTEXT=
   JPLTEXT=        if ( update_time() )
   JPLTEXT=        {
   JPLTEXT=                return (-1)
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=//PR20060258
   JPLTEXT=// Remove call to update payroll status.
   JPLTEXT=// Must be performed within update_time EJB call above
   JPLTEXT=// to ensure status is updated within the same
   JPLTEXT=// DB transaction.
   JPLTEXT=//
   JPLTEXT=
   JPLTEXT=        call sm_jform("leave")
   JPLTEXT=        return (0)
   JPLTEXT=} //end proc do_leave()
   JPLTEXT=
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine: change_status
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine updates the status.
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2001-01-05      D. Maynard
   JPLTEXT=//                      Adjust logic for security flag handling.
   JPLTEXT=//      2000-09-10      D. Bodlien / E. Ren     CR20000398
   JPLTEXT=//                      Add processing for amendments.
   JPLTEXT=//      2002-04-16      D. Maynard      PR20020291
   JPLTEXT=//                      Clear out the first line of the err_msg_dl\
 after
   JPLTEXT=//                      a call to ut_run_edits to hide the EWD\
 focus information.
   JPLTEXT=//      2004-02-18      M. Ryan         CR20030587
   JPLTEXT=//                      Removed excess display code
   JPLTEXT=//      2005-04-07      A. McCormick    CR20040387
   JPLTEXT=//                      Added call to set_leave_defaults()
   JPLTEXT=//      2005-05-31      E. Ramos        CR20050039
   JPLTEXT=//                      Add support for cost structure components.
   JPLTEXT=//      2005-11-28      A. McCormick    PR20051022
   JPLTEXT=//                      Added code to make status changes via link\
 for screen reader friendliness.
   JPLTEXT=//      2006-04-21      T. Peace        PR20060258
   JPLTEXT=//                      Reorganize procedure flow to allow time\
 update and status update
   JPLTEXT=//                      to be performed by a single ejb call to\
 update_time in order to
   JPLTEXT=//                      make all changes within a single DBMS\
 transaction.
   JPLTEXT=//                      Relocate code to determine direction of\
 status change prior to
   JPLTEXT=//                      call to do_valid_screen which was\
 overwriting variables used
   JPLTEXT=//                      by find_status_direction procedure.
   JPLTEXT=//      2006-06-14      T. Peace        PR20060357
   JPLTEXT=//                      Add checks to consider if a change occurred
   JPLTEXT=//                      in the timesheet data of the non-displayed
   JPLTEXT=//                      week when determining the status change\
 direction.
   JPLTEXT=//                      Moved orig_status to an on-screen hidden\
 widget.
   JPLTEXT=//      2006-07-11      T. Peace        PR20060465
   JPLTEXT=//                      Revise change_status procedure to ensure
   JPLTEXT=//                      edit_checks are performed when timesheet
   JPLTEXT=//                      changes are saved via status update if
   JPLTEXT=//                      new status is greater than UNV, regardless
   JPLTEXT=//                      of status change direction.
   JPLTEXT=//      2007-02-21      T. Peace        PR20060893
   JPLTEXT=//                      Add payroll_flag indicator on calls to
   JPLTEXT=//                      load_default_leave procedure.
   JPLTEXT=//      2007-10-02      T. Peace        PR20070477
   JPLTEXT=//                      Combine amendment time updates and any\
 status changes
   JPLTEXT=//                      in a single service_call & database\
 transaction.
   JPLTEXT=//      2007-11-08      T. Peace        CR20070158
   JPLTEXT=//                      Ensure edit checks are run on
   JPLTEXT=//                      status downgrages even if any
   JPLTEXT=//                      timesheet changes are detected.
   JPLTEXT=//      2007-11-16      T. Peace        CR20070206
   JPLTEXT=//                      Ensure that data changes made in other week
   JPLTEXT=//                      are saved when user changes week and\
 attempts status change.
   JPLTEXT=//      2007-12-17      T. Peace        PR20070812
   JPLTEXT=//                      Ensure deletion of timesheet row triggers
   JPLTEXT=//                      timesheet save on status change request.
   JPLTEXT=//      2009-02-26      T. Peace        PR20080259
   JPLTEXT=//                      Ensure status array and orig_status fields\
 are set to UNV
   JPLTEXT=//                      when edit error is encountered.
   JPLTEXT=//      2020-09-11      E. Ramos        DFCT0011860
   JPLTEXT=//                      Prevent bogus values from being saved as\
 status of the timesheet.
   JPLTEXT=//      2022-02-25      I. Lane         ENHC0036378
   JPLTEXT=//                      Removed code based on obtaining screen\
 reader friendly status.
   JPLTEXT=//      2025-01-23      A. McCormick    DFCT0014136
   JPLTEXT=//                      Added call to update_comments() to ensure\
 the comments are saved with the timesheet.
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=proc change_status ()
   JPLTEXT={
   JPLTEXT=        vars retval(10) = 0, new_status(10), new_idx(10)
   JPLTEXT=        vars direction(10)
   JPLTEXT=        vars period_idx(10), b_idx(10), e_idx(10)
   JPLTEXT=        vars cur_idx(10)
   JPLTEXT=        vars orig_period_idx(10),orig_new_idx(10)
   JPLTEXT=        vars orig_cur_idx(10),orig_direction(10)
   JPLTEXT=        vars save_orig_status(10), b_idx(10), e_idx(10)
   JPLTEXT=        vars temp_save_flag(10)
   JPLTEXT=
   JPLTEXT=        edit_reqd = gbl$FALSE
   JPLTEXT=
   JPLTEXT=        if (force_save_flag == gbl$TRUE)
   JPLTEXT=             UsrMadeChg = 1
   JPLTEXT=
   JPLTEXT=        //Determine direction of status change requested by user
   JPLTEXT=        // This may be changed by screen_validation called below
   JPLTEXT=        // if a data problem or change is found.
   JPLTEXT=
   JPLTEXT=        save_orig_status = orig_status
   JPLTEXT=        new_status = ut_strtoupper (wstatus_type_om)
   JPLTEXT=        orig_new_idx = ut_find_status_index(new_status)
   JPLTEXT=// PR20060357
   JPLTEXT=        orig_period_idx = ut_find_status_index(orig_status)
   JPLTEXT=        orig_cur_idx =\
 ut_find_status_index(status_array[orig_period_idx])
   JPLTEXT=        orig_direction = find_status_direction(orig_new_idx,\
 orig_period_idx)
   JPLTEXT=
   JPLTEXT=        if ((orig_new_idx == -1) || (orig_direction ==\
 pr$bad_status))
   JPLTEXT=        {
   JPLTEXT=                // Log error in the log files
   JPLTEXT=                call ut_log_error("Invalid status value\
 (:new_status) detected payroll.jam active_empname (:active_empname)\
 active_empid (:active_empid) active_role (:active_role) %N orig_new_idx\
 (:orig_new_idx) orig_direction (:orig_direction)")
   JPLTEXT=
   JPLTEXT=                call ut5_msg_warning("Invalid status value detected\
 (:new_status). %N Please contact Help Desk if problem persists.")
   JPLTEXT=                return -1
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        // orig_status now set immediately after service_call\
 return
   JPLTEXT=        // and stores the status value found active in database.
   JPLTEXT=
   JPLTEXT=
   JPLTEXT=        // Set default leave codes (disabled fields are not\
 transmitted from the browser to the server)
   JPLTEXT=        call load_default_leave(gbl$TRUE)
   JPLTEXT=
   JPLTEXT=        call set_leave_defaults()
   JPLTEXT=
   JPLTEXT=        if ( do_valid_screen() )
   JPLTEXT=        {
   JPLTEXT=                return (-1)
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=/*PR20070477  Remove amendment specific logic path/call to\
 update_time
   JPLTEXT=*/
   JPLTEXT=
   JPLTEXT=        //Determine direction of change.
   JPLTEXT=
   JPLTEXT=        if ( orig_direction == pr$downgrade ) && (new_status ==\
 'UNV' ) && (direction == 0)
   JPLTEXT=        {
   JPLTEXT=             //Force downgrade to UNV if user explicitly requested\
 it
   JPLTEXT=             // and avoid unnecessary warning message by avoiding
   JPLTEXT=             // call to find_status_direction.
   JPLTEXT=
   JPLTEXT=             direction = pr$downgrade
   JPLTEXT=             status_flag = gbl$TRUE
   JPLTEXT=        }
   JPLTEXT=        else
   JPLTEXT=        {
   JPLTEXT=             // Confirm that no data change or problem has
   JPLTEXT=             // changed target status.
   JPLTEXT=
   JPLTEXT=             new_idx = ut_find_status_index(new_status)
   JPLTEXT=             cur_idx = ut_find_status_index(orig_status)
   JPLTEXT=
   JPLTEXT=             new_status = ut_strtoupper (wstatus_type_om)
   JPLTEXT=
   JPLTEXT=             direction = find_status_direction(new_idx, cur_idx)
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=
   JPLTEXT=        if ( direction == pr$downgrade )
   JPLTEXT=        {
   JPLTEXT=                //Either user explicitedly requested a downgrade or
   JPLTEXT=                //a problem encountered during screen validation
   JPLTEXT=                //required automatic downgrade to UNV.
   JPLTEXT=
   JPLTEXT=                //Update the status array and database status.
   JPLTEXT=
   JPLTEXT=                if ( !cfg$MIS_DAILY_STATUS )
   JPLTEXT=                {
   JPLTEXT=                        b_idx = 1
   JPLTEXT=                        e_idx = 14
   JPLTEXT=                }
   JPLTEXT=                else
   JPLTEXT=                {
   JPLTEXT=                        b_idx = period_idx
   JPLTEXT=                        e_idx = 14
   JPLTEXT=                }
   JPLTEXT=
   JPLTEXT=                call change_status_array(new_status, b_idx, e_idx)
   JPLTEXT=                status_flag = gbl$TRUE
   JPLTEXT=
   JPLTEXT=                if (new_status != 'UNV' )
   JPLTEXT=                {
   JPLTEXT=                        edit_reqd = gbl$TRUE
   JPLTEXT=                }
   JPLTEXT=
   JPLTEXT=        }
   JPLTEXT=        else if ( direction == pr$upgrade )
   JPLTEXT=        {
   JPLTEXT=                status_flag = gbl$TRUE
   JPLTEXT=
   JPLTEXT=                //Run edits.
   JPLTEXT=
   JPLTEXT=                if (!amendment)
   JPLTEXT=                {
   JPLTEXT=
   JPLTEXT=// PR20060258 - Need to perform the edit_check during
   JPLTEXT=//              the update_time transaction.
   JPLTEXT=//              Can't call edits service call directly
   JPLTEXT=//              since it's a separate transaction and
   JPLTEXT=//              the actual DB update hasn't happened
   JPLTEXT=//              yet.
   JPLTEXT=//
   JPLTEXT=                        edit_reqd = gbl$TRUE
   JPLTEXT=                        ed_pp = ut_get_pp()
   JPLTEXT=                        ed_ppyear = ut_get_ppyear()
   JPLTEXT=
   JPLTEXT=                        call ut_get_timestamp()
   JPLTEXT=                }
   JPLTEXT=                else
   JPLTEXT=                {
   JPLTEXT=                        edit_reqd = 1
   JPLTEXT=                }
   JPLTEXT=
   JPLTEXT=        }
   JPLTEXT=        else
   JPLTEXT=        {
   JPLTEXT=                // NO NEED TO CHANGE STATUS BASED SINCE NO\
 DIRECTION DETECTED
   JPLTEXT=                status_flag = gbl$FALSE
   JPLTEXT=                if ((save_flag))  && (new_status != 'UNV')
   JPLTEXT=                {
   JPLTEXT=
   JPLTEXT=                      edit_reqd = gbl$TRUE
   JPLTEXT=                      status_flag = gbl$TRUE
   JPLTEXT=                }
   JPLTEXT=                else
   JPLTEXT=                {
   JPLTEXT=                      status_flag = gbl$FALSE
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=        //Save time data (consistency).
   JPLTEXT=        // Status will be updated during the same transaction as\
 time update
   JPLTEXT=        // unless status_flag turned off in preceding test.
   JPLTEXT=
   JPLTEXT=
   JPLTEXT=        if ( !retval )
   JPLTEXT=        {
   JPLTEXT=                //Update the status array and database status.
   JPLTEXT=
   JPLTEXT=                if ( !cfg$MIS_DAILY_STATUS )
   JPLTEXT=                {
   JPLTEXT=                        b_idx = 1
   JPLTEXT=                        e_idx = 14
   JPLTEXT=                }
   JPLTEXT=                else
   JPLTEXT=                {
   JPLTEXT=                        b_idx = 1
   JPLTEXT=                        e_idx = period_idx
   JPLTEXT=                }
   JPLTEXT=
   JPLTEXT=                call change_status_array(new_status, b_idx, e_idx)
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        if ( update_comments() )
   JPLTEXT=        {
   JPLTEXT=                return (-1)
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        if ( update_time() )
   JPLTEXT=        {
   JPLTEXT=                // problem so must restore original status
   JPLTEXT=                if ( !cfg$MIS_DAILY_STATUS )
   JPLTEXT=                {
   JPLTEXT=                        b_idx = 1
   JPLTEXT=                        e_idx = 14
   JPLTEXT=                }
   JPLTEXT=                else
   JPLTEXT=                {
   JPLTEXT=                        b_idx = 1
   JPLTEXT=                        e_idx = period_idx
   JPLTEXT=                }
   JPLTEXT=
   JPLTEXT=                call change_status_array(orig_status, b_idx, e_idx)
   JPLTEXT=                orig_status = save_orig_status
   JPLTEXT=
   JPLTEXT=                return (-1)
   JPLTEXT=        }
   JPLTEXT=        if (retval )
   JPLTEXT=        {
   JPLTEXT=               // processing error occurred so re-retrieve\
 timesheet and display
   JPLTEXT=               // message since EJB would have issued a rollback
   JPLTEXT=               call do_select()
   JPLTEXT=
   JPLTEXT=               call sm_n_clear_array("err_msg_dl")
   JPLTEXT=               call sm_n_clear_array("err_img")
   JPLTEXT=               ed_error_count = 0
   JPLTEXT=               err_msg_idx    = 1
   JPLTEXT=               validation_error = gbl$FALSE
   JPLTEXT=
   JPLTEXT=               call ut5_msg_warning("Hourly Timesheet update\
 operation failed.%N " ## \\
   JPLTEXT=                    "The latest data has been retrieved from the "\
 ## \\
   JPLTEXT=                    " database for your convenience.(5)")
   JPLTEXT=        }
   JPLTEXT=        else if (ed_error_count > 0 )
   JPLTEXT=        {
   JPLTEXT=              // Validation error,
   JPLTEXT=                if ( !cfg$MIS_DAILY_STATUS )
   JPLTEXT=                {
   JPLTEXT=                        b_idx = 1
   JPLTEXT=                        e_idx = 14
   JPLTEXT=                }
   JPLTEXT=                else
   JPLTEXT=                {
   JPLTEXT=                        b_idx = status_idx
   JPLTEXT=                        e_idx = 14
   JPLTEXT=                }
   JPLTEXT=
   JPLTEXT=                call change_status_array("UNV", b_idx, e_idx)
   JPLTEXT=
   JPLTEXT=        }
   JPLTEXT=        else
   JPLTEXT=        {
   JPLTEXT=                orig_status = save_orig_status
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        if ( (direction) && !(retval) ) && !(save_orig_status ==\
 status_array[14])
   JPLTEXT=        {
   JPLTEXT=                call ut5_msg_information("Hourly Timesheet status\
 updated.")
   JPLTEXT=                err_msg_idx = err_msg_idx + 1
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        return (0)
   JPLTEXT=} //end proc change_status()
   JPLTEXT=
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine: find_status_period
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine determines the index into the status
   JPLTEXT=//    array which is a two-week period.
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2006-06-15      T. Peace        PR20060357
   JPLTEXT=//                      This procedure is now obsolete.
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=proc find_status_period ()
   JPLTEXT={
   JPLTEXT=        vars retval(10), idx(10), status_period(20)
   JPLTEXT=
   JPLTEXT=        if ( !cfg$MIS_DAILY_STATUS )
   JPLTEXT=        {
   JPLTEXT=                retval = ut_find_status_index(status_array[14])
   JPLTEXT=        }
   JPLTEXT=        else
   JPLTEXT=        {
   JPLTEXT=                status_period = ut_strtolower(wstatus_period_om)
   JPLTEXT=                if ( status_period == "sat" )
   JPLTEXT=                {
   JPLTEXT=                        idx = 7
   JPLTEXT=                }
   JPLTEXT=                else if ( status_period == "sun" )
   JPLTEXT=                {
   JPLTEXT=                        idx = 1
   JPLTEXT=                }
   JPLTEXT=                else if ( status_period == "mon" )
   JPLTEXT=                {
   JPLTEXT=                        idx = 2
   JPLTEXT=                }
   JPLTEXT=                else if ( status_period == "tue" )
   JPLTEXT=                {
   JPLTEXT=                        idx = 3
   JPLTEXT=                }
   JPLTEXT=                else if ( status_period == "wed" )
   JPLTEXT=                {
   JPLTEXT=                        idx = 4
   JPLTEXT=                }
   JPLTEXT=                else if ( status_period == "thu" )
   JPLTEXT=                {
   JPLTEXT=                        idx = 5
   JPLTEXT=                }
   JPLTEXT=                else if ( status_period == "fri" )
   JPLTEXT=                {
   JPLTEXT=                        idx = 6
   JPLTEXT=                }
   JPLTEXT=                retval = ((week_num - 1) * 7) + idx
   JPLTEXT=        }
   JPLTEXT=        return (retval)
   JPLTEXT=} //end proc find_status_period()
   JPLTEXT=
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine: find_status_direction
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine determines the status change\
 direction.
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2006-06-15      T. Peace        PR20060357
   JPLTEXT=//                      Use orig_status value (which represents\
 actual status on timesheet as saved in database)
   JPLTEXT=//                      as the initial status when determining\
 status change direction.
   JPLTEXT=//      2020-09-11      E. Ramos        DFCT0011860
   JPLTEXT=//                      Prevent bogus values from being saved as\
 status of the timesheet.
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=proc find_status_direction (new_idx, period_idx)
   JPLTEXT={
   JPLTEXT=        vars retval (10), cur_idx(10)
   JPLTEXT=
   JPLTEXT=
   JPLTEXT=        cur_idx = ut_find_status_index(orig_status)
   JPLTEXT=
   JPLTEXT=        if (new_idx == -1)
   JPLTEXT=        {
   JPLTEXT=                retval = pr$bad_status
   JPLTEXT=        }
   JPLTEXT=        else if ( new_idx < cur_idx )
   JPLTEXT=        {
   JPLTEXT=                retval = pr$downgrade
   JPLTEXT=        }
   JPLTEXT=        else if ( new_idx > cur_idx )
   JPLTEXT=        {
   JPLTEXT=                retval = pr$upgrade
   JPLTEXT=        }
   JPLTEXT=        else
   JPLTEXT=        {
   JPLTEXT=                call ut5_msg_information("No need to change status\
 from" ## \\
   JPLTEXT=                        " :gbl$status_options[:cur_idx] to" ## \\
   JPLTEXT=                        " :gbl$status_options[:new_idx]" ## ".")
   JPLTEXT=                retval = pr$no_change
   JPLTEXT=        }
   JPLTEXT=        return (retval)
   JPLTEXT=} //end proc find_status_direction()
   JPLTEXT=
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine: change_status_array
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine updates the status array.
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2016-06-20      E. Ramos        CR20160062
   JPLTEXT=//                      Show week 1 and week 2 on the same screen.
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=proc change_status_array (new_status, b_idx, e_idx)
   JPLTEXT={
   JPLTEXT=        vars i(10), week_idx(10), idx(10)
   JPLTEXT=        vars week_num
   JPLTEXT=
   JPLTEXT=        for i = b_idx while i <= e_idx
   JPLTEXT=        {
   JPLTEXT=                status_array[i] = new_status
   JPLTEXT=        }
   JPLTEXT=        for week_num = 1 while week_num<=2
   JPLTEXT=        {
   JPLTEXT=                week_idx = ((week_num - 1) * 7)
   JPLTEXT=                for i = 1 while i <= 7
   JPLTEXT=                {
   JPLTEXT=                        idx = week_idx + i
   JPLTEXT=                        //@widget("total_:i")[2] =\
 status_array[idx]
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=        status_flag = gbl$TRUE
   JPLTEXT=        return (0)
   JPLTEXT=} //end proc change_status_array()
   JPLTEXT=
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine: valid_shiftcode
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine validates the shift code.
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2003-12-29      M. Ryan         PR20030981
   JPLTEXT=//                      Changed msg_warnings to include bad value
   JPLTEXT=//                      when validating account, shiftcode.
   JPLTEXT=//      2011-08-23      A. McCormick    PR20110436
   JPLTEXT=//                      Modified validiation function to take the\
 quotes out before SQL.
   JPLTEXT=//      2011-09-16      A. McCormick    PR20110436 - rework
   JPLTEXT=//                      Replace value on screen if the\
 substitutions clear the value.
   JPLTEXT=//      2020-07-02      I. Lane         ENHC0035988
   JPLTEXT=//                      Retrieve shiftcodes from ref_shiftcode\
 table.
   JPLTEXT=//
   JPLTEXT=//////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=proc valid_shiftcode (fld_no, fld_val, fld_occ, fld_flag)
   JPLTEXT={
   JPLTEXT=
   JPLTEXT=        vars retval = 0, t_val(10)
   JPLTEXT=
   JPLTEXT=        if ( !flag_changes(fld_no, fld_val, fld_occ, fld_flag) )
   JPLTEXT=        {
   JPLTEXT=                return (0)
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        //Pad the shiftcode.
   JPLTEXT=
   JPLTEXT=        t_val = ut_squeeze(fld_val, ut$SQUEEZE_LEAD_TRAIL, "")
   JPLTEXT=        t_val = ut_substitute(t_val, "'", "")
   JPLTEXT=        t_val = ut_substitute(t_val, '"', '')
   JPLTEXT=        if ( @length(t_val) == 0 )
   JPLTEXT=        {
   JPLTEXT=                @field_num(fld_no)[fld_occ] = ''
   JPLTEXT=                return (0)
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        if ( !ut_dbms_exists("select count(*) from ref_shiftcode\
 where shiftcode = ':t_val'") )
   JPLTEXT=        {
   JPLTEXT=
   JPLTEXT=                //Record not found.
   JPLTEXT=
   JPLTEXT=                call ut5_msg_warning("Invalid shiftcode ':t_val'.")
   JPLTEXT=                retval = 1
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        if ( (fld_val != t_val) || (@length(fld_val) !=\
 @length(t_val)) )
   JPLTEXT=                @field_num(fld_no)[fld_occ] = t_val
   JPLTEXT=
   JPLTEXT=return (retval)
   JPLTEXT=
   JPLTEXT=} //end proc valid_shiftcode()
   JPLTEXT=
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine: valid_base
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine validates the base rate.
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2011-08-23      A. McCormick    PR20110436
   JPLTEXT=//                      Modified validiation function to take the\
 quotes out before SQL.
   JPLTEXT=//      2011-09-16      A. McCormick    PR20110436 - rework
   JPLTEXT=//                      Replace value on screen if the\
 substitutions clear the value.
   JPLTEXT=//      2016-07-14      E. Ramos        PR20160238
   JPLTEXT=//                      Correct references to w*occ and w*poscode\
 fields to support both weeks on the screen.
   JPLTEXT=//      2018-08-17      E. Ramos        PR20180250
   JPLTEXT=//                      Define local variable week_num and set its\
 value before referencing it.
   JPLTEXT=//      2022-02-11      A. McCormick    ENHC0035398
   JPLTEXT=//                      Modified validation to include new valid\
 values.
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=proc valid_base (fld_no, fld_val, fld_occ, fld_flag)
   JPLTEXT={
   JPLTEXT=        vars retval(10) = 0, t_val(10)
   JPLTEXT=
   JPLTEXT=        vars week_num
   JPLTEXT=
   JPLTEXT=        if ( !flag_changes(fld_no, fld_val, fld_occ, fld_flag) )
   JPLTEXT=        {
   JPLTEXT=                return (0)
   JPLTEXT=        }
   JPLTEXT=        t_val = ut_squeeze(fld_val, ut$SQUEEZE_LEAD_TRAIL, "")
   JPLTEXT=        t_val = ut_substitute(t_val, "'", "")
   JPLTEXT=        t_val = ut_substitute(t_val, '"', '')
   JPLTEXT=        if ( @length(t_val) == 0 )
   JPLTEXT=        {
   JPLTEXT=                week_num = GetWeekNumberFromFieldNumber(fld_no)
   JPLTEXT=                @field_num(fld_no)[fld_occ] = ''
   JPLTEXT=                @widget("w:(week_num)occ") = ""
   JPLTEXT=                @widget("w:(week_num)poscode") = ""
   JPLTEXT=                return (0)
   JPLTEXT=        }
   JPLTEXT=        t_val = ut_strtoupper(t_val)
   JPLTEXT=        if ( (t_val != "Y") && (t_val != "N") && (t_val != "A") &&\
 (t_val != "B") )
   JPLTEXT=        {
   JPLTEXT=                retval = 1
   JPLTEXT=                call ut5_msg_warning("Base Rate must be 'Y', 'N',\
 'A' or 'B'.")
   JPLTEXT=        }
   JPLTEXT=        return (retval)
   JPLTEXT=} //end proc valid_base()
   JPLTEXT=
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine: valid_account
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine validates the account.
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2000-09-10      D. Bodlien / E. Ren     CR20000398
   JPLTEXT=//                      Add processing for amendments.
   JPLTEXT=//      2003-12-29      M. Ryan         PR20030981
   JPLTEXT=//                      Changed msg_warnings to include bad value
   JPLTEXT=//                      when validating account, shiftcode.
   JPLTEXT=//       2005-05-31     E. Ramos        CR20050039
   JPLTEXT=//                      Add support for cost structure components.
   JPLTEXT=//      2005-06-03      E. Ramos        CR20050039 rework
   JPLTEXT=//                      Added function to duplicate row.
   JPLTEXT=//      2005-06-21      A. McCormick    PR20050499
   JPLTEXT=//                      Deactivated Key and Cost validation for\
 amendments.
   JPLTEXT=//      2007-01-18      E. Ramos        PR20060777
   JPLTEXT=//                      Retrieve description of account if\
 available also when editing an amendment.
   JPLTEXT=//      2007-06-21      E. Ramos        PR20070363
   JPLTEXT=//                      Validate account values even if their\
 numeric value is 0.
   JPLTEXT=//      2009-11-05      E. Ramos        PR20090657
   JPLTEXT=//                      Consider Inactive accounts valid when\
 validating amendments.
   JPLTEXT=//      2009-11-16      T. Peace        PR20090483
   JPLTEXT=//                      Add missing comma.
   JPLTEXT=//      2016-06-20      E. Ramos        CR20160062
   JPLTEXT=//                      Show week 1 and week 2 on the same screen.
   JPLTEXT=//      2023-10-10      E. Ramos        ENHC0036728
   JPLTEXT=//                      Avoid validating account if populated with\
 a default leave value.
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=proc valid_account (fld_no, fld_val, fld_occ, fld_flag)
   JPLTEXT={
   JPLTEXT=        vars retval(10) = 1, num_fields(10), t_act_code(10),\
 t_net_code(10)
   JPLTEXT=        vars t_ws_type(10), t_ws_id(30), sql_stmt(255),\
 where_stmt(255)
   JPLTEXT=        vars fld_len(10), t_buf(80)
   JPLTEXT=        vars v_ffsaccounts_name, v_fname, v_description
   JPLTEXT=        vars week_num
   JPLTEXT=        vars v_hrcode v_isLeave
   JPLTEXT=
   JPLTEXT=        week_num = GetWeekNumberFromFieldNumber(fld_no)
   JPLTEXT=
   JPLTEXT=        if (cfg$COST_COMPONENTS_FOR_ENTRY=1)
   JPLTEXT=          return 0
   JPLTEXT=
   JPLTEXT=        // If the account is an auto-populated leave default value\
 avoid performing the validation
   JPLTEXT=        if (cfg$USE_DFLT_LV_ACCT)
   JPLTEXT=        {
   JPLTEXT=                v_hrcode = w:(week_num)hrcode
   JPLTEXT=                v_isLeave=isDefaultLeaveHourCode(v_hrcode)
   JPLTEXT=                if (v_isLeave>0)
   JPLTEXT=                {
   JPLTEXT=                        return 0
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=        // Replace double qoutes with spaces
   JPLTEXT=        fld_val = ut_replace_char(fld_val,'"',' ')
   JPLTEXT=        @field_num(fld_no) = fld_val
   JPLTEXT=
   JPLTEXT=        call flag_changes(fld_no, fld_val, fld_occ, fld_flag)
   JPLTEXT=
   JPLTEXT=        //Validate the account.
   JPLTEXT=
   JPLTEXT=        count_star = 0
   JPLTEXT=        t_buf = ut_clean_account(fld_val)
   JPLTEXT=
   JPLTEXT=
   JPLTEXT=        if t_buf == ""
   JPLTEXT=                return 0
   JPLTEXT=
   JPLTEXT=
   JPLTEXT=        if amendment
   JPLTEXT=        {
   JPLTEXT=                v_ffsaccounts_name  = "ffsaccounts"
   JPLTEXT=                v_fname = "coststruct"
   JPLTEXT=                v_description = "ffsdescription"
   JPLTEXT=        }
   JPLTEXT=        else
   JPLTEXT=        {
   JPLTEXT=                v_ffsaccounts_name = "v_active_accounts"
   JPLTEXT=                v_fname = "account"
   JPLTEXT=                v_description = "description"
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=
   JPLTEXT=        if ( cfg$MIS_DIV_ACCOUNT )
   JPLTEXT=        {
   JPLTEXT=                fld_len = @length(t_buf)
   JPLTEXT=                if ( fld_len >= 4 )
   JPLTEXT=                {
   JPLTEXT=                        t_act_code = ut_squeeze(t_buf(1,4),\
 ut$SQUEEZE_LEAD_TRAIL, "")
   JPLTEXT=                }
   JPLTEXT=                if ( fld_len >= 8 )
   JPLTEXT=                {
   JPLTEXT=                        t_net_code = ut_squeeze(t_buf(5,4),\
 ut$SQUEEZE_LEAD_TRAIL, "")
   JPLTEXT=                }
   JPLTEXT=                if ( fld_len >= 9 )
   JPLTEXT=                {
   JPLTEXT=                        t_ws_type = ut_squeeze(t_buf(9,1),\
 ut$SQUEEZE_LEAD_TRAIL, "")
   JPLTEXT=                }
   JPLTEXT=                if ( fld_len >= 19 )
   JPLTEXT=                {
   JPLTEXT=                        t_ws_id = ut_squeeze(t_buf(10,9),\
 ut$SQUEEZE_LEAD_TRAIL, "")
   JPLTEXT=                }
   JPLTEXT=                where_stmt = ac_bld_where(t_act_code, t_net_code,\
 t_ws_type, t_ws_id)
   JPLTEXT=                if ( where_stmt != "where" )
   JPLTEXT=                {
   JPLTEXT=                        sql_stmt = "select count(*) from\
 v0_labor_activities :where_stmt"
   JPLTEXT=                        call ut_dbms_sql("count_star", sql_stmt)
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=        else
   JPLTEXT=        {
   JPLTEXT=                sql_stmt = "select cstdesc from v_active_cststrct\
 where csttkid = ':emptkid'"
   JPLTEXT=                sql_stmt = sql_stmt ## " and tkcode = :+emptkcode \
 and trim(cstid) ="
   JPLTEXT=                sql_stmt = sql_stmt ## " trim(:+t_buf)"
   JPLTEXT=                count_star = ut_dbms_sql("descrip", sql_stmt)
   JPLTEXT=        }
   JPLTEXT=        if ( count_star )
   JPLTEXT=        {
   JPLTEXT=                retval = 0
   JPLTEXT=                w:(week_num)costdesc[fld_occ] = descrip
   JPLTEXT=        }
   JPLTEXT=        else
   JPLTEXT=        {
   JPLTEXT=                if (!amendment)
   JPLTEXT=                {
   JPLTEXT=                        call ut5_msg_warning("Account :+t_buf is\
 not a valid account.")
   JPLTEXT=                        retval = 1
   JPLTEXT=                }
   JPLTEXT=                else
   JPLTEXT=                {
   JPLTEXT=                        count_star = \\
   JPLTEXT=                                ut_dbms_sql ("descrip", "select\
 :v_description from :v_ffsaccounts_name where trim(:v_fname) =\
 trim(:+t_buf)")
   JPLTEXT=                        if (count_star)
   JPLTEXT=                        {
   JPLTEXT=                                retval = 0
   JPLTEXT=                                w:(week_num)costdesc [fld_occ] =\
 descrip
   JPLTEXT=                        }
   JPLTEXT=                        else
   JPLTEXT=                        {
   JPLTEXT=                            call ut5_msg_warning("Account :+t_buf\
 is not a valid account.")
   JPLTEXT=                                retval = 1
   JPLTEXT=                        }
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=        if (amendment)
   JPLTEXT=        {
   JPLTEXT=              return(0)
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        return (retval)
   JPLTEXT=} //end proc valid_account()
   JPLTEXT=
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine: valid_key
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine validates the account key.
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2003-12-29      M. Ryan         PR20030981
   JPLTEXT=//                      Changed msg_warnings to include bad value
   JPLTEXT=//                      when validating account, shiftcode.
   JPLTEXT=//      2005-06-03      E. Ramos        CR20050039 rework
   JPLTEXT=//                      Added function to duplicate row.
   JPLTEXT=//      2005-06-21      A. McCormick    PR20050499
   JPLTEXT=//                      Deactivated Key and Cost validation for\
 amendments.
   JPLTEXT=//      2005-09-13      A. McCormick    PR20050783
   JPLTEXT=//                      Updated proc valid_key(); function should\
 return the valid cost structure if the key
   JPLTEXT=//                      used is valid (for amendments) and return\
 if the key used is invalid.
   JPLTEXT=//      2006-10-08      E. Ramos        CR20060209
   JPLTEXT=//                      Added logic to validate the costkey field\
 when components are enabled
   JPLTEXT=//      2006-12-18      E. Ramos        PR20060887
   JPLTEXT=//                      Remove commented msg emsg command.
   JPLTEXT=//      2011-08-23      A. McCormick    PR20110436
   JPLTEXT=//                      Modified validation function to take the\
 quotes out before SQL.
   JPLTEXT=//      2011-09-28      E. Ramos        PR20110658
   JPLTEXT=//                      Added logic to validate favorite key.
   JPLTEXT=//      2012-04-19      E. Ramos        PR20120276
   JPLTEXT=//                      Display error message if quotes are\
 detected in the key field.
   JPLTEXT=//      2012-07-06      E. Ramos        CR20120138
   JPLTEXT=//                      Support cfg$COST_KEY_OVERRIDE_ROLE setting.
   JPLTEXT=//      2012-08-03      E. Ramos        CR20120138 rework
   JPLTEXT=//                      Avoid presenting message stating current\
 role  is not allowed to override when not appropriate.
   JPLTEXT=//      2012-08-22      A. McCormick    PR20120841
   JPLTEXT=//                      Correct valid_key() function so to allow\
 populating cost structures.
   JPLTEXT=//      2016-05-25      E. Ramos        CR20160062
   JPLTEXT=//                      Show week 1 and week 2 on the payroll and\
 other_hours screens.
   JPLTEXT=//      2019-05-31      E. Ramos        DFCT0010934
   JPLTEXT=//                      Require current role to be specified in\
 cfg$COST_KEY_OVERRIDE_ROLE
   JPLTEXT=//                      to allow the user to use the cost key\
 override value.
   JPLTEXT=//      2021-01-08      A. McCormick    DFCT0011908
   JPLTEXT=//                      Removed quote character validation from\
 valid_key(), verification is now in
   JPLTEXT=//                      ut_check_valid_characters() in\
 ut_client.jpl
   JPLTEXT=//      2021-04-21      E. Ramos        ENHC0036192
   JPLTEXT=//                      Retrieve cost keys from both\
 COST_ELEMENT_FAVORITES and V_BORCOSTKEY when\
 cfg$COST_COMPONENTS_COST_KEY_BOR==1.
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=proc valid_key (fld_no, fld_val, fld_occ, fld_flag)
   JPLTEXT={
   JPLTEXT=        vars retval(10) = 0, sql_stmt(255), t_buf(80)
   JPLTEXT=
   JPLTEXT=        vars v_key, i
   JPLTEXT=        vars fld_len
   JPLTEXT=        vars week_num
   JPLTEXT=        vars v_emplocation
   JPLTEXT=
   JPLTEXT=        week_num = GetWeekNumberFromFieldNumber(fld_no)
   JPLTEXT=
   JPLTEXT=        v_key = fld_val
   JPLTEXT=
   JPLTEXT=        fld_len = @length(fld_val)
   JPLTEXT=
   JPLTEXT=        call flag_changes(fld_no, fld_val, fld_occ, fld_flag)
   JPLTEXT=
   JPLTEXT=        t_buf = ut_squeeze(fld_val, ut$SQUEEZE_LEAD_TRAIL, "")
   JPLTEXT=        t_buf = ut_substitute(t_buf, "'", "")
   JPLTEXT=        t_buf = ut_substitute(t_buf, '"', '')
   JPLTEXT=        if ( @length(t_buf) == 0 )
   JPLTEXT=        {
   JPLTEXT=                w:(week_num)key[fld_occ] = ""
   JPLTEXT=                return (0)
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        // Validate that the current role is allowed to use the\
 cost key override value
   JPLTEXT=        if (cfg$COST_COMPONENTS_FOR_ENTRY) &&\
 (cfg$COST_COMPONENTS_COST_KEY)
   JPLTEXT=        {
   JPLTEXT=                if ( (ut_bitop_and(cfg$COST_COMPONENTS_CHANGE_ROLE,\
 tna_data[idx$ACTIVE_ROLE])) || \\
   JPLTEXT=                     (ut_bitop_and(cfg$COST_KEY_OVERRIDE_ROLE     ,\
 tna_data[idx$ACTIVE_ROLE])) )
   JPLTEXT=                {
   JPLTEXT=
   JPLTEXT=                        if ( (t_buf== cfg$COST_KEY_OVERRIDE) &&\
 (@widget("save_dl")->hidden == PV_NO) && !ut_bitop_and(\
 cfg$COST_KEY_OVERRIDE_ROLE , tna_data[idx$ACTIVE_ROLE] ) )
   JPLTEXT=                        {
   JPLTEXT=                                call ut5_msg_warning("Current role\
 not allowed to override cost key validations entering :cfg$COST_KEY_OVERRIDE\
 .")
   JPLTEXT=                                return 1
   JPLTEXT=                        }
   JPLTEXT=                        else
   JPLTEXT=                                return 0
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        if (cfg$COST_COMPONENTS_FOR_ENTRY) &&\
 (cfg$COST_COMPONENTS_COST_KEY)
   JPLTEXT=        {
   JPLTEXT=                // Validate only if the user is not allowed to\
 change the list of favorites and the value is not the cost key override value
   JPLTEXT=                if (fld_val == cfg$COST_KEY_OVERRIDE)
   JPLTEXT=                        return 0
   JPLTEXT=
   JPLTEXT=                if (tna_data[idx$ACTIVE_ROLE]  ==\
 gbl$ROLE_EMPLOYEE)
   JPLTEXT=                {
   JPLTEXT=                        if ( ! ( ut_bitop_and(\
 cfg$CHANGE_FAVORITES_ROLE, tna_data[idx$ACTIVE_ROLE] ) ) )
   JPLTEXT=                        {
   JPLTEXT=
   JPLTEXT=                                sql_stmt = "select costkey from\
 cost_element_favorites where empid = :+active_empid  and costkey = :+fld_val"
   JPLTEXT=                                if\
 (cfg$COST_COMPONENTS_COST_KEY_BOR==1)
   JPLTEXT=                                {
   JPLTEXT=                                        dbms ALIAS v_emplocation
   JPLTEXT=                                        sql SELECT emplocation from\
 employee where empid = :+active_empid
   JPLTEXT=                                        dbms ALIAS
   JPLTEXT=                                        sql_stmt = sql_stmt ## "\
 UNION ALL select costkey from V_BORCOSTKEY WHERE location = :+v_emplocation\
 AND costkey = :+fld_val"
   JPLTEXT=                                }
   JPLTEXT=                                count_star = ut_dbms_sql("descrip",\
 sql_stmt)
   JPLTEXT=                                if ( count_star )
   JPLTEXT=                                        return 0
   JPLTEXT=                                else
   JPLTEXT=                                {
   JPLTEXT=                                        call\
 ut5_msg_warning("Favorite Key :fld_val is not valid.")
   JPLTEXT=                                        return 1
   JPLTEXT=                                }
   JPLTEXT=                        }
   JPLTEXT=                        else
   JPLTEXT=                                return 0
   JPLTEXT=                }
   JPLTEXT=                else
   JPLTEXT=                        return 0
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        sql_stmt = "select cstid, cstdesc from v_active_cststrct\
 where csttkid = :+emptkid"
   JPLTEXT=        sql_stmt = sql_stmt ## " and tkcode = :+emptkcode and\
 cstcode = :+t_buf"
   JPLTEXT=        if ( !ut_dbms_sql("t_cost, descrip", sql_stmt) )
   JPLTEXT=        {
   JPLTEXT=        // PR20050783 - Moved code added for PR20050499 into this\
 if; want the cost struct returned
   JPLTEXT=        // if it exists.
   JPLTEXT=
   JPLTEXT=                if (amendment)
   JPLTEXT=                {
   JPLTEXT=                      return(0)
   JPLTEXT=                }
   JPLTEXT=                else
   JPLTEXT=                {
   JPLTEXT=                        call ut5_msg_warning("Account key :t_buf is\
 not valid.")
   JPLTEXT=                        retval = 1
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=        else
   JPLTEXT=        {
   JPLTEXT=                w:(week_num)costdesc[fld_occ] = descrip
   JPLTEXT=                w:(week_num)cost[fld_occ] =\
 ut_format_account(t_cost)
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        return (retval)
   JPLTEXT=} //end proc valid_key()
   JPLTEXT=
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine: flag_changes
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine flags changes made to a row that has\
 associated time.
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2001-01-09      D. Maynard
   JPLTEXT=//                      Remove most all other occurrences of\
 reference
   JPLTEXT=//                      to the security flag from the validation\
 routines
   JPLTEXT=//                      and place it here after the check between
   JPLTEXT=//                      the before and after widget data image.
   JPLTEXT=//                      This is critical to detect if an\
 unauthorized
   JPLTEXT=//                      change is being attempted.
   JPLTEXT=//      2007-06-22      E. Ramos        PR20070363
   JPLTEXT=//                      Fixed problem that made the function return\
 0 even though changes were made.
   JPLTEXT=//                      Validate account values even if their\
 numeric value is 0.
   JPLTEXT=//      2012-01-09      A. McCormick    PR20120032
   JPLTEXT=//                      Verify the single quotation mark has been\
 removed prior to comparing
   JPLTEXT=//                      Modified comparison to compare as character\
 strings.
   JPLTEXT=//      2012-04-19      E. Ramos        PR20120276
   JPLTEXT=//                      Prevent JPL error if single or double quote\
 characters were entered in a field.
   JPLTEXT=//      2016-06-20      E. Ramos        CR20160062
   JPLTEXT=//                      Show week 1 and week 2 on the same screen.
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=proc flag_changes (fld_no, fld_val, fld_occ, fld_flag)
   JPLTEXT={
   JPLTEXT=        vars status_idx(10), b_idx(10), e_idx(10)
   JPLTEXT=        vars fld_nm(32), b_image(50), a_image(50)
   JPLTEXT=        vars fieldName
   JPLTEXT=        vars week_num
   JPLTEXT=
   JPLTEXT=        //Compare before and after images, return 0 if no change.
   JPLTEXT=
   JPLTEXT=        fld_nm = @field_num(:fld_no)->name
   JPLTEXT=        week_num = GetWeekNumberFromFieldNumber(fld_no)
   JPLTEXT=
   JPLTEXT=        fieldName = fld_nm ## "[:fld_occ]"
   JPLTEXT=        a_image = :fieldName
   JPLTEXT=        a_image = ut_substitute(a_image, "'", " ")
   JPLTEXT=
   JPLTEXT=        fieldName = "b:fld_nm" ## "[:fld_occ]"
   JPLTEXT=        b_image = :fieldName
   JPLTEXT=        b_image = ut_substitute(b_image, "'", " ")
   JPLTEXT=
   JPLTEXT=        if (fieldName=="bw1dyncost15" &&\
 cfg$COST_COMPONENTS_COST_KEY_BOR)
   JPLTEXT=                return 0
   JPLTEXT=
   JPLTEXT=        if ( ':a_image' == ':b_image' )
   JPLTEXT=        {
   JPLTEXT=                return (0)
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        // Check to see if an unauthorized change is being\
 attempted.
   JPLTEXT=        // If so, turn on the message flag and leave without
   JPLTEXT=        // flagging the change for later update.
   JPLTEXT=
   JPLTEXT=        if ( !security_flag ) && ( !save_flag )
   JPLTEXT=        {
   JPLTEXT=                unauthorized_action = gbl$TRUE
   JPLTEXT=                return(0)
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        //Indicate a change.
   JPLTEXT=
   JPLTEXT=        if ( fld_nm(1,5) == "w1day"  || fld_nm(1,5) == "w2day"  )
   JPLTEXT=        {
   JPLTEXT=                status_idx = ((week_num - 1) * 7) + fld_nm(7,1)
   JPLTEXT=        }
   JPLTEXT=        else
   JPLTEXT=        {
   JPLTEXT=                if ((cfg$WORKTYPE_EMPORGCODES != "NONE") &&\
 (fld_nm=="regular_hours" || fld_nm=="premium_hours"))
   JPLTEXT=                {
   JPLTEXT=                  // Set status_idx=1 if called because of changes\
 to fields in the worktype distribution grid
   JPLTEXT=                  status_idx=1
   JPLTEXT=                }
   JPLTEXT=                else
   JPLTEXT=                  status_idx = find_time(week_num,fld_occ)
   JPLTEXT=        }
   JPLTEXT=        save_flag = gbl$TRUE
   JPLTEXT=
   JPLTEXT=        if ( status_idx )
   JPLTEXT=        {
   JPLTEXT=                if ( status_array[status_idx] != "UNV" )
   JPLTEXT=                {
   JPLTEXT=                        if ( !cfg$MIS_DAILY_STATUS )
   JPLTEXT=                        {
   JPLTEXT=                                b_idx = 1
   JPLTEXT=                                e_idx = 14
   JPLTEXT=                        }
   JPLTEXT=                        else
   JPLTEXT=                        {
   JPLTEXT=                                b_idx = status_idx
   JPLTEXT=                                e_idx = 14
   JPLTEXT=                        }
   JPLTEXT=                        call change_status_array("UNV", b_idx,\
 e_idx)
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=        return (1)
   JPLTEXT=} //end proc flag_changes()
