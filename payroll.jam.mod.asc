 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine: update_default
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine updates the default payroll data.
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2002-04-02      D. Bodlien      PR20020281
   JPLTEXT=//                      When saving a Default Schedule of a\
 Pre-Print Schedule, invoke the timesheet edits after the save.
   JPLTEXT=//      2002-04-16      D. Maynard      PR20020291
   JPLTEXT=//                      Clear out the first line of the err_msg_dl\
 after
   JPLTEXT=//                      a call to ut_run_edits to hide the EWD\
 focus information.
   JPLTEXT=//      2005-05-31      E. Ramos        CR20050039
   JPLTEXT=//                      Add support for cost structure components.
   JPLTEXT=//      2006-04-20      T. Peace        PR20060258
   JPLTEXT=//                      Ensure status_flag is off before call save\
 default timesheet.
   JPLTEXT=//      2006-07-13      T. Peace        PR20060465
   JPLTEXT=//                      Call dyncost_prepare_save to ensure
   JPLTEXT=//                      cost componenet fields are properly\
 formatted prior to save.
   JPLTEXT=//      2007-02-21      T. Peace        PR20060893
   JPLTEXT=//                      Add payroll_flag indicator on calls to\
 load_default_leave procedure.
   JPLTEXT=//      2007-12-18      T. Peace        PR20070812
   JPLTEXT=//                      Set UsrMadeChg flag to force default\
 timesheet to be saved.
   JPLTEXT=//      2011-05-11      A. McCormick    PR20110185
   JPLTEXT=//                      Set DefaultSaved variable; to be used in\
 update_time for non-dflt and non-preprints
   JPLTEXT=//      2016-07-19      E. Ramos        PR20160268
   JPLTEXT=//                      Clear add_del_flag and del_row_flag when\
 save is successful.
   JPLTEXT=//      2023-06-01      A. McCormick    DFCT0013320
   JPLTEXT=//                      Call update_default_preprint() instead of\
 update_time() to better seperate the default
   JPLTEXT=//                      and pre-print timesheets from the current\
 timesheet.
   JPLTEXT=//
  \
 JPLTEXT=/////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=proc update_default ()
   JPLTEXT={
   JPLTEXT=        vars retval(10), t_pp(10), t_ppyear(10)
   JPLTEXT=        vars t_save_flag (10)
   JPLTEXT=        vars new_status = "none"
   JPLTEXT=
   JPLTEXT=        call sm_n_clear_array("err_msg_dl")
   JPLTEXT=        call sm_n_clear_array("err_img")
   JPLTEXT=        ed_error_count = 0
   JPLTEXT=        err_msg_idx = 1
   JPLTEXT=
   JPLTEXT=        retval = default_preprint_hrscodes()
   JPLTEXT=        if (retval != 0)
   JPLTEXT=        {
   JPLTEXT=                // Ensure the weekly and daily totals are displayed\
 correctly
   JPLTEXT=                call get_daily_totals()
   JPLTEXT=
   JPLTEXT=                call get_week_totals()
   JPLTEXT=
   JPLTEXT=                call ut5_msg_warning ("Default Timesheet not\
 saved.")
   JPLTEXT=                return (retval)
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        retval = 0
   JPLTEXT=        if ( security_flag )
   JPLTEXT=        {
   JPLTEXT=                // Set default leave codes (disabled fields are not\
 transmitted from the browser to the server)
   JPLTEXT=                call load_default_leave(gbl$TRUE)
   JPLTEXT=
   JPLTEXT=                // Prepare component values to be saved (i.e.\
 components with formatting = 10, which means they are substrings of other\
 components)
   JPLTEXT=                call dyncost_prepare_save()
   JPLTEXT=
   JPLTEXT=                if ( do_valid_screen() )
   JPLTEXT=                {
   JPLTEXT=                        return (-1)
   JPLTEXT=                }
   JPLTEXT=
   JPLTEXT=                status_flag = 0
   JPLTEXT=                //Save time data.
   JPLTEXT=
   JPLTEXT=                t_pp = pp
   JPLTEXT=                t_ppyear = ppyear
   JPLTEXT=                t_save_flag = save_flag
   JPLTEXT=                pp = gbl$SCHD_DFLT
   JPLTEXT=                ppyear = 0
   JPLTEXT=                save_flag = gbl$TRUE
   JPLTEXT=                UsrMadeChg = gbl$TRUE
   JPLTEXT=
   JPLTEXT=                retval = update_default_preprint()
   JPLTEXT=                pp = t_pp
   JPLTEXT=                ppyear = t_ppyear
   JPLTEXT=                save_flag = t_save_flag
   JPLTEXT=
   JPLTEXT=                if (retval != 0)
   JPLTEXT=                {
   JPLTEXT=                        call ut5_msg_warning ("Update of Default\
 Schedule failed")
   JPLTEXT=                        return (retval)
   JPLTEXT=                }
   JPLTEXT=
   JPLTEXT=                DefaultSaved = gbl$TRUE
   JPLTEXT=
   JPLTEXT=                //Run edits.
   JPLTEXT=
   JPLTEXT=                ed_pp = gbl$SCHD_DFLT
   JPLTEXT=                ed_ppyear = 0
   JPLTEXT=                retval = ut_run_edits( new_status )
   JPLTEXT=
   JPLTEXT=                // Clear the EWD focus information from the first\
 message line.
   JPLTEXT=
   JPLTEXT=                err_msg_dl[1] = ""
   JPLTEXT=                err_img[1]    = ""
   JPLTEXT=
   JPLTEXT=                if (retval == 0)
   JPLTEXT=                {
   JPLTEXT=                        UsrMadeChg = 0
   JPLTEXT=                        err_msg_idx = err_msg_idx + 1
   JPLTEXT=                        call ut5_msg_information("Default Timesheet\
 saved.")
   JPLTEXT=                }
   JPLTEXT=                if (!retval)
   JPLTEXT=                {
   JPLTEXT=                        // Clear flags when save is successful
   JPLTEXT=                        add_del_flag = 0
   JPLTEXT=                        del_row_flag = 0
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=        else
   JPLTEXT=        {
   JPLTEXT=                unauthorized_action = gbl$TRUE
   JPLTEXT=        }
   JPLTEXT=        return (retval)
   JPLTEXT=} //end proc update_default()
   JPLTEXT=
  \
