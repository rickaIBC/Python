 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine:   get_timesheet_status
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine retrieves the current status of the\
 timesheet
   JPLTEXT=//    being displayed from the database.
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2009-12-10      T. Peace        PR20090612
   JPLTEXT=//                      Created.
   JPLTEXT=//      2010-02-24      T. Peace        PR20090612 - Rework
   JPLTEXT=//                      Handle amendment initial open condition\
 when
   JPLTEXT=//                      status has yet to be set.
   JPLTEXT=//      2010-07-08      T. Peace        PR20100284
   JPLTEXT=//                      Force the status to reset to UNV
   JPLTEXT=//                      on any save operation after a delete
   JPLTEXT=//                      row or duplicate row operation.
   JPLTEXT=//      2011-05-13      A. McCormick    PR20110185
   JPLTEXT=//                      Bypass function if Default or PrePrint was\
 just saved.
   JPLTEXT=//      2016-04-14      E. Ramos        CR20140256
   JPLTEXT=//                      Introduce PROCESS_SEQ
   JPLTEXT=//      2016-06-20      E. Ramos        CR20160062
   JPLTEXT=//                      Show week 1 and week 2 on the same screen.
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=proc get_timesheet_status ()
   JPLTEXT={
   JPLTEXT=        vars sql_stmt(255)
   JPLTEXT=        vars b_idx(10), e_idx(10)
   JPLTEXT=
   JPLTEXT=        if ( DefaultSaved || PrePrintSaved )
   JPLTEXT=                return 0
   JPLTEXT=
   JPLTEXT=        if (amendment)
   JPLTEXT=        {
   JPLTEXT=                sql_stmt = "select satstat2 from amend_ppstat where\
 empid = ':active_empid' and ppyear = :prior_tna_data[idx$PAY_CORR_YEAR] and\
 pp = :prior_tna_data[idx$PAY_CORR_PP] \\
   JPLTEXT=and process_ppyear = :ppyear and process_pp = :pp and process_seq=\
 :prior_tna_data[idx$PAY_CURR_SEQ]"
   JPLTEXT=        }
   JPLTEXT=        else
   JPLTEXT=        {
   JPLTEXT=                sql_stmt = "select satstat2 from ppstat where empid\
 = ':active_empid' and ppyear = :ppyear and pp = :pp"
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        call ut_dbms_sql("ts_status", sql_stmt)
   JPLTEXT=
   JPLTEXT=        if (status_array [14] != ts_status) && (ts_status != "") \
 && !(del_row_flag + add_del_flag)
   JPLTEXT=        {
   JPLTEXT=                call ut5_msg_warning("A status change has been\
 detected on this timesheet.")
   JPLTEXT=                b_idx = 1
   JPLTEXT=                e_idx = 14
   JPLTEXT=                call change_status_array(ts_status, b_idx,e_idx)
   JPLTEXT=        }
   JPLTEXT=}  // end get_timesheet_status()
   JPLTEXT=
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine:   do_copy_week
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine copies week 1 data to week 2.
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2011-09-20      A. McCormick    CR20060126
   JPLTEXT=//                      Created function.
   JPLTEXT=//      2015-03-25      E. Ramos        PR20150140
   JPLTEXT=//                      Added mentoring field.
   JPLTEXT=//      2016-02-03      E. Ramos        CR20150129
   JPLTEXT=//                      Added widgets to introduce work type data\
 as part of the regular timesheets.
   JPLTEXT=//      2016-06-20      E. Ramos        CR20160062
   JPLTEXT=//                      Show week 1 and week 2 on the same screen.
   JPLTEXT=//      2021-03-08      A. McCormick    ENHC0036105
   JPLTEXT=//                      Updated function to allow it to work with\
 Defaults and Pre-Prints.
   JPLTEXT=//      2021-12-08      A. McCormick    DFCT0012450
   JPLTEXT=//                      Correct the Copy Week 1 to Week 2\
 functionality to prevent amendments from deleting regular timesheet data.
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=proc do_copy_week ()
   JPLTEXT={
   JPLTEXT=        vars i
   JPLTEXT=        // If we are in this function then they confirmed they\
 wanted to copy week 1 to week 2.
   JPLTEXT=
   JPLTEXT=        if(default_preprint == 2)
   JPLTEXT=        {
   JPLTEXT=                ppyear = 0
   JPLTEXT=                pp = 97
   JPLTEXT=                call update_default()
   JPLTEXT=        }
   JPLTEXT=        else if(default_preprint == 3)
   JPLTEXT=        {
   JPLTEXT=                ppyear = 0
   JPLTEXT=                pp = 98
   JPLTEXT=                call update_pre()
   JPLTEXT=        }
   JPLTEXT=        else
   JPLTEXT=        {
   JPLTEXT=                call update_payroll()
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        if(amendment)
   JPLTEXT=        {
   JPLTEXT=                db_array[1] = "delete from amend_hrsprim where\
 empid = :+active_empid and ppyear = :prior_tna_data[idx$PAY_CORR_YEAR] and pp\
 = :prior_tna_data[idx$PAY_CORR_PP] " ## \\
   JPLTEXT=                                "and process_ppyear = :ppyear and\
 process_pp = :pp and process_seq= :prior_tna_data[idx$PAY_CURR_SEQ] and\
 ppweek = 2"
   JPLTEXT=                db_array[2] = ""
   JPLTEXT=                db_array[3] = ""
   JPLTEXT=                db_array[4] = ""
   JPLTEXT=        }
   JPLTEXT=        else
   JPLTEXT=        {
   JPLTEXT=                db_array[1] = "delete from hrsprim where empid =\
 :+active_empid and ppyear = :ppyear and pp = :pp and ppweek = 2"
   JPLTEXT=                db_array[2] = ""
   JPLTEXT=                db_array[3] = ""
   JPLTEXT=                db_array[4] = ""
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        call ut_dbms_write()
   JPLTEXT=
   JPLTEXT=        call sm_n_copyarray("w2hrcode",    "w1hrcode")
   JPLTEXT=        call sm_n_copyarray("w2hrdesc",    "w1hrdesc")
   JPLTEXT=        call sm_n_copyarray("w2hrtype",    "w1hrtype")
   JPLTEXT=        call sm_n_copyarray("w2day_1",     "w1day_1")
   JPLTEXT=        call sm_n_copyarray("w2day_2",     "w1day_2")
   JPLTEXT=        call sm_n_copyarray("w2day_3",     "w1day_3")
   JPLTEXT=        call sm_n_copyarray("w2day_4",     "w1day_4")
   JPLTEXT=        call sm_n_copyarray("w2day_5",     "w1day_5")
   JPLTEXT=        call sm_n_copyarray("w2day_6",     "w1day_6")
   JPLTEXT=        call sm_n_copyarray("w2day_7",     "w1day_7")
   JPLTEXT=        call sm_n_copyarray("w2key",       "w1key")
   JPLTEXT=        call sm_n_copyarray("w2cost",      "w1cost")
   JPLTEXT=        call sm_n_copyarray("w2costdesc",  "w1costdesc")
   JPLTEXT=        call sm_n_copyarray("w2shift",     "w1shift")
   JPLTEXT=        call sm_n_copyarray("w2envcode",   "w1envcode")
   JPLTEXT=        call sm_n_copyarray("w2telework",  "w1telework")
   JPLTEXT=        call sm_n_copyarray("w2base",      "w1base")
   JPLTEXT=        call sm_n_copyarray("w2occ",       "w1occ")
   JPLTEXT=        call sm_n_copyarray("w2poscode",   "w1poscode")
   JPLTEXT=        call sm_n_copyarray("w2taskcode",  "w1taskcode")
   JPLTEXT=        call sm_n_copyarray("w2dyncost1",  "w1dyncost1")
   JPLTEXT=        call sm_n_copyarray("w2dyncost2",  "w1dyncost2")
   JPLTEXT=        call sm_n_copyarray("w2dyncost3",  "w1dyncost3")
   JPLTEXT=        call sm_n_copyarray("w2dyncost4",  "w1dyncost4")
   JPLTEXT=        call sm_n_copyarray("w2dyncost5",  "w1dyncost5")
   JPLTEXT=        call sm_n_copyarray("w2dyncost6",  "w1dyncost6")
   JPLTEXT=        call sm_n_copyarray("w2dyncost7",  "w1dyncost7")
   JPLTEXT=        call sm_n_copyarray("w2dyncost8",  "w1dyncost8")
   JPLTEXT=        call sm_n_copyarray("w2dyncost9",  "w1dyncost9")
   JPLTEXT=        call sm_n_copyarray("w2dyncost10", "w1dyncost10")
   JPLTEXT=        call sm_n_copyarray("w2dyncost11", "w1dyncost11")
   JPLTEXT=        call sm_n_copyarray("w2dyncost12", "w1dyncost12")
   JPLTEXT=        call sm_n_copyarray("w2dyncost13", "w1dyncost13")
   JPLTEXT=        call sm_n_copyarray("w2dyncost14", "w1dyncost14")
   JPLTEXT=        call sm_n_copyarray("w2dyncost15", "w1dyncost15")
   JPLTEXT=        call sm_n_copyarray("w2dyncost16", "w1dyncost16")
   JPLTEXT=        call sm_n_copyarray("w2dyncost17", "w1dyncost17")
   JPLTEXT=        call sm_n_copyarray("w2dyncost18", "w1dyncost18")
   JPLTEXT=        call sm_n_copyarray("w2dyncost19", "w1dyncost19")
   JPLTEXT=        call sm_n_copyarray("w2dyncost20", "w1dyncost20")
   JPLTEXT=        call sm_n_copyarray("w2dyncost21", "w1dyncost21")
   JPLTEXT=        call sm_n_copyarray("w2freeze",    "w1freeze")
   JPLTEXT=        call sm_n_copyarray("w2mentoring", "w1mentoring")
   JPLTEXT=
   JPLTEXT=        for i = 8 while i <= 14
   JPLTEXT=        {
   JPLTEXT=                tot_array[i] = tot_array[i - 7]
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        // call week_2_bundle(2)
   JPLTEXT=
   JPLTEXT=        save_flag = gbl$TRUE
   JPLTEXT=
   JPLTEXT=        if(default_preprint == 2)
   JPLTEXT=        {
   JPLTEXT=                call update_default()
   JPLTEXT=                send bundle "default_preprint_ts" data 97,0
   JPLTEXT=        }
   JPLTEXT=        else if(default_preprint == 3)
   JPLTEXT=        {
   JPLTEXT=                call update_pre()
   JPLTEXT=                send bundle "default_preprint_ts" data 98,0
   JPLTEXT=        }
   JPLTEXT=        else
   JPLTEXT=        {
   JPLTEXT=                call update_payroll()
   JPLTEXT=                call get_week_totals()
   JPLTEXT=        }
   JPLTEXT=}
