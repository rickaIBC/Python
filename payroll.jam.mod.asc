 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine:   set_security_flag
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine sets the security flag
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2001-01-05      D. Maynard
   JPLTEXT=//                      New Proc.
   JPLTEXT=//      2000-09-10      D. Bodlien / E. Ren     CR20000398
   JPLTEXT=//                      Add processing for amendments.
   JPLTEXT=//      2006-07-19      T. Peace        PR20060487
   JPLTEXT=//                      Relax restrictions on permitted actions by
   JPLTEXT=//                      certifiers when cfg$MIS_NO_TKOLE is set.
   JPLTEXT=//      2006-08-01      T. Peace        PR20060487 - rework
   JPLTEXT=//                      Extend original PR-487 changes to all\
 certifier and Admin
   JPLTEXT=//                      users, regardless of the cfg$MIS_NO_TKOLE\
 setting.
   JPLTEXT=//                      Supress re-retrieval of timesheet after\
 either
   JPLTEXT=//                      certifier or administrator makes a status\
 change.
   JPLTEXT=//      2007-10-02      T. Peace        PR20070477
   JPLTEXT=//                      Supress re-retrieval of timesheet after\
 certifier changes status to UNV.
   JPLTEXT=//      2008-01-28      T. Peace        PR20080001
   JPLTEXT=//                      Post user message to screen when attempt to\
 update a timesheet is discovered after
   JPLTEXT=//                      it has been transmitted to FPPS.
   JPLTEXT=//      2008-07-28      E. Ramos        PR20080463
   JPLTEXT=//                      Allow administrators to create amendments.
   JPLTEXT=//      2008-08-05      T. Peace        PR20080463- rework
   JPLTEXT=//                      Allow save requests to be processed\
 properly and present correct status change options to
   JPLTEXT=//                      administrators when they are allowed to\
 create amendments.
   JPLTEXT=//      2009-07-02      E. Ramos        CR20010062
   JPLTEXT=//                      Support timesheets entered for pay periods\
 in the future.
   JPLTEXT=//      2009-08-11      E. Ramos        PR20090210 rework
   JPLTEXT=//                      Removed unnecessary log messages.
   JPLTEXT=//      2011-01-04      E. Ramos        PR20100609
   JPLTEXT=//                      Avoid presenting messages warning that the\
 timesheet has already
   JPLTEXT=//                      been released when logged in as an\
 Administrator and the
   JPLTEXT=//                      status of the timesheet is CRT or LCK.
   JPLTEXT=//      2023-06-02      A. McCormick    DFCT0013320
   JPLTEXT=//                      Reset the security flag to True if in a\
 Default / Pre-Print from the main menu.
   JPLTEXT=//      2024-08-15      A. McCormick    ENHC0037149
   JPLTEXT=//                      Removed non-WebSphere code.
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=proc set_security_flag ()
   JPLTEXT={
   JPLTEXT=        vars retval(10) = 0
   JPLTEXT=        vars t_save_flag (10)
   JPLTEXT=
   JPLTEXT=        t_save_flag = save_flag
   JPLTEXT=
   JPLTEXT=        if (!amendment)
   JPLTEXT=        {
   JPLTEXT=                if (cfg$websphere)
   JPLTEXT=                {
   JPLTEXT=                        obj_retval = \\
   JPLTEXT=                                sm_obj_call (payroll_hndl,\
 "get_save_flag", active_empid, pp, ppyear, :tna_data_name, save_flag)
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=        else
   JPLTEXT=        {
   JPLTEXT=                if (cfg$websphere)
   JPLTEXT=                {
   JPLTEXT=                        obj_retval = sm_obj_call\
 (amend_payroll_hndl, "g_a_save_flag", pp, ppyear, :tna_data_name, save_flag)
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        retval = ut_service_error ()
   JPLTEXT=        security_flag = save_flag
   JPLTEXT=        save_flag = t_save_flag
   JPLTEXT=
   JPLTEXT=        //PR20060487 - Remove amendment-specific branch in\
 following logic case structure,
   JPLTEXT=        //           - no known reason for different behavior when\
 processing amendment.
   JPLTEXT=        if ( status_array[14] = "UNV" )
   JPLTEXT=        {
   JPLTEXT=                if ( tna_data[idx$ACTIVE_ROLE] == gbl$ROLE_EMPLOYEE\
 )
   JPLTEXT=                {
   JPLTEXT=                        // continue
   JPLTEXT=                }
   JPLTEXT=                else if ( tna_data[idx$ACTIVE_ROLE] ==\
 gbl$ROLE_TIMEKEEPER )
   JPLTEXT=                {
   JPLTEXT=                        // continue
   JPLTEXT=                }
   JPLTEXT=                else if ( tna_data[idx$ACTIVE_ROLE] ==\
 gbl$ROLE_CERTIFIER )
   JPLTEXT=                {
   JPLTEXT=                        //PR20070477 - Previously called\
 do_select() if
   JPLTEXT=                        //             cfg$MIS_NO_TKROLE was false
   JPLTEXT=                        // continue
   JPLTEXT=                }
   JPLTEXT=                else if ( tna_data[idx$ACTIVE_ROLE] ==\
 gbl$ROLE_ADMINISTRATOR )
   JPLTEXT=                {
   JPLTEXT=                        // Re-retrieve data unless this is an\
 administrator creating an amendment and this is allowed
   JPLTEXT=                        if !( amendment && (cfg$AMENDMENT_ROLE &\
 gbl$ROLE_ADMINISTRATOR))
   JPLTEXT=                                call do_select()
   JPLTEXT=                }
   JPLTEXT=                else
   JPLTEXT=                {
   JPLTEXT=                        security_flag = gbl$TRUE
   JPLTEXT=                        call do_select()
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=        else if ( status_array[14] = "VER" )
   JPLTEXT=        {
   JPLTEXT=                if ( tna_data[idx$ACTIVE_ROLE] == gbl$ROLE_EMPLOYEE\
 )
   JPLTEXT=                {
   JPLTEXT=
   JPLTEXT=                        // continue
   JPLTEXT=
   JPLTEXT=                }
   JPLTEXT=                else if ( tna_data[idx$ACTIVE_ROLE] ==\
 gbl$ROLE_TIMEKEEPER )
   JPLTEXT=                {
   JPLTEXT=                        security_flag = gbl$TRUE
   JPLTEXT=
   JPLTEXT=                        // continue
   JPLTEXT=
   JPLTEXT=                }
   JPLTEXT=                else if ( tna_data[idx$ACTIVE_ROLE] ==\
 gbl$ROLE_CERTIFIER )
   JPLTEXT=                {
   JPLTEXT=                       if cfg$MIS_NO_TKROLE == gbl$FALSE
   JPLTEXT=                              call do_select()
   JPLTEXT=                }
   JPLTEXT=                else if ( tna_data[idx$ACTIVE_ROLE] ==\
 gbl$ROLE_ADMINISTRATOR )
   JPLTEXT=                {
   JPLTEXT=                        // Re-retrieve data unless this is an\
 administrator creating an amendment and this is allowed
   JPLTEXT=                        if !( amendment && (cfg$AMENDMENT_ROLE &\
 gbl$ROLE_ADMINISTRATOR))
   JPLTEXT=                             call do_select()
   JPLTEXT=                }
   JPLTEXT=                else
   JPLTEXT=                {
   JPLTEXT=                        call do_select()
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=        else if ( status_array[14] = "VAL" )
   JPLTEXT=        {
   JPLTEXT=                if ( tna_data[idx$ACTIVE_ROLE] == gbl$ROLE_EMPLOYEE\
 )
   JPLTEXT=                {
   JPLTEXT=                        call do_select()
   JPLTEXT=                }
   JPLTEXT=                else if ( tna_data[idx$ACTIVE_ROLE] ==\
 gbl$ROLE_TIMEKEEPER )
   JPLTEXT=                {
   JPLTEXT=                        security_flag = gbl$TRUE
   JPLTEXT=
   JPLTEXT=                        // continue
   JPLTEXT=
   JPLTEXT=                }
   JPLTEXT=                else if ( tna_data[idx$ACTIVE_ROLE] ==\
 gbl$ROLE_CERTIFIER )
   JPLTEXT=                {
   JPLTEXT=                        security_flag = gbl$TRUE
   JPLTEXT=                        // continue
   JPLTEXT=                }
   JPLTEXT=                else if ( tna_data[idx$ACTIVE_ROLE] ==\
 gbl$ROLE_ADMINISTRATOR )
   JPLTEXT=                {
   JPLTEXT=                        // Re-retrieve data unless this is an\
 administrator creating an amendment and this is allowed
   JPLTEXT=                        if !( amendment && (cfg$AMENDMENT_ROLE &\
 gbl$ROLE_ADMINISTRATOR))
   JPLTEXT=                             call do_select()
   JPLTEXT=                }
   JPLTEXT=                else
   JPLTEXT=                {
   JPLTEXT=                        call do_select()
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=        else if ( status_array[14] = "CRT" )
   JPLTEXT=        {
   JPLTEXT=                if ( previously_transmitted_flag == gbl$TRUE)
   JPLTEXT=                {
   JPLTEXT=                        call ut5_msg_warning("A problem was\
 encountered saving timesheet.%N ")
   JPLTEXT=                        call ut5_msg_warning("Changes to this\
 timesheet must be made in the amendment process." ## \\
   JPLTEXT=                             "%NThis timesheet has already been\
 released.%N" ## \\
   JPLTEXT=                             "The latest data has been retrieved\
 from the" ## \\
   JPLTEXT=                             " database for your convenience.(1)")
   JPLTEXT=                }
   JPLTEXT=                else if ( tna_data[idx$ACTIVE_ROLE] ==\
 gbl$ROLE_EMPLOYEE )
   JPLTEXT=                {
   JPLTEXT=                        call do_select()
   JPLTEXT=                }
   JPLTEXT=                else if ( tna_data[idx$ACTIVE_ROLE] ==\
 gbl$ROLE_TIMEKEEPER )
   JPLTEXT=                {
   JPLTEXT=                        call do_select()
   JPLTEXT=                }
   JPLTEXT=                else if ( tna_data[idx$ACTIVE_ROLE] ==\
 gbl$ROLE_CERTIFIER )
   JPLTEXT=                {
   JPLTEXT=                        security_flag = gbl$TRUE
   JPLTEXT=                        // continue
   JPLTEXT=                }
   JPLTEXT=                else if ( tna_data[idx$ACTIVE_ROLE] ==\
 gbl$ROLE_ADMINISTRATOR )
   JPLTEXT=                {
   JPLTEXT=                        security_flag = gbl$TRUE
   JPLTEXT=                        // continue
   JPLTEXT=                }
   JPLTEXT=                else
   JPLTEXT=                {
   JPLTEXT=                        call do_select()
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=        else if ( ( status_array[14] = "LCK" ) || \\
   JPLTEXT=                ( status_array[14] = "REL" ) )
   JPLTEXT=        {
   JPLTEXT=                if ( tna_data[idx$ACTIVE_ROLE] ==\
 gbl$ROLE_ADMINISTRATOR )
   JPLTEXT=                {
   JPLTEXT=                        security_flag = gbl$FALSE
   JPLTEXT=                        return 0
   JPLTEXT=                }
   JPLTEXT=                else  if ( tna_data[idx$ACTIVE_ROLE] ==\
 gbl$ROLE_EMPLOYEE )
   JPLTEXT=                {
   JPLTEXT=                        call do_select()
   JPLTEXT=                }
   JPLTEXT=                else if ( tna_data[idx$ACTIVE_ROLE] ==\
 gbl$ROLE_TIMEKEEPER )
   JPLTEXT=                {
   JPLTEXT=                        call do_select()
   JPLTEXT=                }
   JPLTEXT=                else if ( tna_data[idx$ACTIVE_ROLE] ==\
 gbl$ROLE_CERTIFIER )
   JPLTEXT=                {
   JPLTEXT=                        call do_select()
   JPLTEXT=                }
   JPLTEXT=                else
   JPLTEXT=                {
   JPLTEXT=                        call do_select()
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        if(default_preprint >= 2)
   JPLTEXT=        {
   JPLTEXT=                security_flag = gbl$TRUE
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        return (0)
   JPLTEXT=} //end proc set_security_flag()
