 cfg$ACCOUNT_CUSTOM_FORMULA != "")
   JPLTEXT=        {
   JPLTEXT=                call ut_dbms_sql ("default_acct","SELECT\
 :cfg$ACCOUNT_CUSTOM_FORMULA FROM employee WHERE empid=:+active_empid")
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        if cfg$HRSCODE_ONEXIT_JS == "jsBLMProtectSBY(this)"
   JPLTEXT=        {
   JPLTEXT=                call ut_dbms_sql ("BLMcode,BLMcode2,BLMflag", \\
   JPLTEXT=                                "SELECT code,code2,description FROM\
 cost_element_ref_list WHERE reference_id = 'SUBACTIVITY-SBY' ORDER BY code")
   JPLTEXT=
   JPLTEXT=                BLMcode_numrows = BLMcode->num_occurrences
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        COST_KEY_ONEXIT_JS = cfg$COST_KEY_ONEXIT_JS
   JPLTEXT=        COST_KEY_BOR = cfg$COST_COMPONENTS_COST_KEY_BOR
   JPLTEXT=
