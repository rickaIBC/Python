   JPLTEXT=                // Clear  widgets on the right hand side of the\
 timesheet depending on configuration flags
   JPLTEXT=                call clear_hidden_fields()
   JPLTEXT=                // Hide/show widgets on the right hand side of the\
 timesheet depending on configuration flags
   JPLTEXT=
   JPLTEXT=                if (cfg$TIMESHEET_HAZARD == 0 )
   JPLTEXT=                {
   JPLTEXT=                        w:(week_num)envcode->hidden = PV_YES
   JPLTEXT=                        w:(week_num)envcode->validation_func = ""
   JPLTEXT=                }
   JPLTEXT=                else
   JPLTEXT=                {
   JPLTEXT=                        w:(week_num)envcode->hidden = PV_NO
   JPLTEXT=                }
   JPLTEXT=
   JPLTEXT=                if (cfg$TIMESHEET_TELEWORK == 0 )
   JPLTEXT=                {
   JPLTEXT=                        w:(week_num)telework->hidden = PV_YES
   JPLTEXT=                        w:(week_num)telework->validation_func = ""
   JPLTEXT=                }
   JPLTEXT=                else
   JPLTEXT=                        w:(week_num)telework->hidden = PV_NO
   JPLTEXT=
   JPLTEXT=                if (cfg$TIMESHEET_DAYSTOTAL == 0 )
   JPLTEXT=                {
   JPLTEXT=                        w:(week_num)daystotal->hidden = PV_YES
   JPLTEXT=                        w:(week_num)daystotal->validation_func = ""
   JPLTEXT=                }
   JPLTEXT=                else
   JPLTEXT=                {
   JPLTEXT=                        w:(week_num)daystotal->hidden = PV_NO
   JPLTEXT=                        call\
 ut_protect_field("w:(week_num)daystotal")
   JPLTEXT=
   JPLTEXT=                        w:(week_num)daystotal-> prefix_markup = "<div class='daystotal' tabindex='0' ><span><span class='visually-hidden'>row total</span>"
   JPLTEXT=                        w:(week_num)daystotal-> suffix_markup = "</span></div>"
   JPLTEXT=                }
   JPLTEXT=
   JPLTEXT=
   JPLTEXT=                if (cfg$TIMESHEET_OCC_SERIES == 0)
   JPLTEXT=                {
   JPLTEXT=                        w:(week_num)occ->hidden = PV_YES
   JPLTEXT=                        w:(week_num)occ->validation_func = ""
   JPLTEXT=                }
   JPLTEXT=                else
   JPLTEXT=                        w:(week_num)occ->hidden = PV_NO
   JPLTEXT=
   JPLTEXT=                if (cfg$TIMESHEET_SEC_RATE == 0)
   JPLTEXT=                {
   JPLTEXT=                        w:(week_num)base->hidden = PV_YES
   JPLTEXT=                        w:(week_num)base->validation_func = ""
   JPLTEXT=                }
   JPLTEXT=                else
   JPLTEXT=                        w:(week_num)base->hidden = PV_NO
   JPLTEXT=
   JPLTEXT=                if (cfg$TIMESHEET_POS_CODE == 0)
   JPLTEXT=                {
   JPLTEXT=                        w:(week_num)poscode->hidden = PV_YES
   JPLTEXT=                        w:(week_num)poscode->validation_func = ""
   JPLTEXT=                }
   JPLTEXT=                else
   JPLTEXT=                        w:(week_num)poscode->hidden = PV_NO
   JPLTEXT=
   JPLTEXT=                if (cfg$TIMESHEET_SHIFT_CODE == 0)
   JPLTEXT=                {
   JPLTEXT=                        w:(week_num)shift->hidden = PV_YES
   JPLTEXT=                        w:(week_num)shift->validation_func = ""
   JPLTEXT=                }
   JPLTEXT=                else
   JPLTEXT=                        w:(week_num)shift->hidden = PV_NO
   JPLTEXT=
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        // Hide Occ Series and Position Title Code if necessary\
 (PR20060528, PR20060559)
   JPLTEXT=        call ut_hide_occseries_and_poscode(active_empid,amendment,\
 ':prior_tna_data[idx$PAY_CORR_YEAR]', ':prior_tna_data[idx$PAY_CORR_PP]')
   JPLTEXT=
   JPLTEXT=        // Hide or show the mentoring column (CR20140260) and the\
 work type-related widgets (CR20150129)
   JPLTEXT=        v_sioreq = ut_bitop_and (:tna_data_name [idx$ACTIVE_PROP],\
 gbl$ACTIVE_PROP_SIOREQ)
   JPLTEXT=
   JPLTEXT=        if (future || amendment)
   JPLTEXT=        {
   JPLTEXT=                call ut_hide_mentoring (active_empid,amendment,\
 :tna_data_name [idx$PAY_CORR_YEAR], :tna_data_name [idx$PAY_CORR_PP])
   JPLTEXT=                call ut_hide_worktype  (active_empid,amendment,\
 future, :tna_data_name [idx$PAY_CORR_YEAR], :tna_data_name [idx$PAY_CORR_PP],\
 v_sioreq)
   JPLTEXT=        }
   JPLTEXT=        else
   JPLTEXT=        {
   JPLTEXT=                call ut_hide_mentoring (active_empid,amendment,\
 ut_get_ppyear(), ut_get_pp() )
   JPLTEXT=                call ut_hide_worktype  (active_empid,amendment,\
 future, ut_get_ppyear(), ut_get_pp() , v_sioreq )
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        for week_num = 1 while week_num<=2
   JPLTEXT=        {
   JPLTEXT=                //Set selection list for account (wcost) and key\
 (wkey).
   JPLTEXT=                if ( cfg$MIS_DIV_ACCOUNT )
   JPLTEXT=                {
   JPLTEXT=                        w:(week_num)cost->on_focus =\
 "SetAccount(this,'400','330');jsUnSetColumnFocus(this);"
   JPLTEXT=                }
   JPLTEXT=                else
   JPLTEXT=                {
   JPLTEXT=                        w:(week_num)cost->on_focus =\
 "SetKeyAccount(this,'700','300');jsUnSetColumnFocus(this);"
   JPLTEXT=                }
   JPLTEXT=
   JPLTEXT=                // Determine if the wkey (key code) field should be\
 displayed/hidden
   JPLTEXT=                if (( ut_bitop_and(:tna_data_name\
 [idx$ACTIVE_PROP], gbl$ACTIVE_PROP_TKASSOC) && \\
   JPLTEXT=                (cfg$ACCOUNT_TYPE == gbl$ACCOUNT_FPPS) )) ||\
 (cfg$COST_COMPONENTS_FOR_ENTRY && amendment)
   JPLTEXT=                {
   JPLTEXT=                        w:(week_num)key->hidden = PV_YES
   JPLTEXT=                        w:(week_num)key->validation_func = ""
   JPLTEXT=                }
   JPLTEXT=                else
   JPLTEXT=                {
   JPLTEXT=                        w:(week_num)key->hidden = PV_NO
   JPLTEXT=                        if (cfg$COST_COMPONENTS_FOR_ENTRY &&\
 cfg$COST_COMPONENTS_COST_KEY)
   JPLTEXT=                        {
   JPLTEXT=                                w:(week_num)key->on_focus =\
 "SetKeyAccountComponent(this,'700','300');jsUnSetColumnFocus(this);"
   JPLTEXT=
   JPLTEXT=                                w:(week_num)key->validation_func =\
 "valid_key"
   JPLTEXT=                                w:(week_num)key->column_title =\
 "Fav<br>Key"
   JPLTEXT=                        }
   JPLTEXT=                        else
   JPLTEXT=                        {
   JPLTEXT=                                w:(week_num)key->on_focus =\
 "SetKeyAccount(this,'700','300');jsUnSetColumnFocus(this);"
   JPLTEXT=                        }
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        //Protect time change from unauthorized roles.
   JPLTEXT=
   JPLTEXT=        if (((!amendment) && \\
   JPLTEXT=                ((tna_data [idx$ACTIVE_ROLE] == gbl$ROLE_CERTIFIER)\
 || \\
   JPLTEXT=                (tna_data [idx$ACTIVE_ROLE] ==\
 gbl$ROLE_ADMINISTRATOR) || \\
   JPLTEXT=                (ut_bitop_and(:tna_data_name [idx$ACTIVE_PROP],\
 gbl$ACTIVE_PROP_SIOREQ)))) || \\
   JPLTEXT=                ((amendment) && (!(tna_data [idx$ACTIVE_ROLE] &\
 cfg$AMENDMENT_ROLE))))
   JPLTEXT=        {
   JPLTEXT=                protect_time = 1
   JPLTEXT=        }
   JPLTEXT=        if ((tna_data [idx$ACTIVE_ROLE] != gbl$ROLE_TIMEKEEPER) ||\
 (tna_data [idx$PAY_ACTIVE] == gbl$PAY_CORRECTION) || \\
   JPLTEXT=                (cfg$DISABLE_PRE_PRINT) || (amendment))
   JPLTEXT=        {
   JPLTEXT=                pre_dl -> hidden = PV_YES
   JPLTEXT=                lpre_dl -> hidden = PV_YES
   JPLTEXT=        }
   JPLTEXT=        if (!((tna_data [idx$ACTIVE_ROLE] == gbl$ROLE_EMPLOYEE) ||\
 (tna_data [idx$ACTIVE_ROLE] == gbl$ROLE_TIMEKEEPER)) || \\
   JPLTEXT=                (tna_data [idx$PAY_ACTIVE] == gbl$PAY_CORRECTION)\
 || \\
   JPLTEXT=                (ut_bitop_and (:tna_data_name [idx$ACTIVE_PROP],\
 gbl$ACTIVE_PROP_SIOREQ)) || (amendment))
   JPLTEXT=        {
   JPLTEXT=                dflt_dl -> hidden = PV_YES
   JPLTEXT=                ldflt_dl -> hidden = PV_YES
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        for week_num=1 while week_num<=2
   JPLTEXT=        {
   JPLTEXT=                if (amendment)
   JPLTEXT=                {
   JPLTEXT=                        if (cfg$HIDE_ENV_CODES)
   JPLTEXT=                        {
   JPLTEXT=                                call ut_protect_field\
 ("w:(week_num)envcode")
   JPLTEXT=                        }
   JPLTEXT=
   JPLTEXT=                        call ut_protect_field\
 ("w:(week_num)taskcode")
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        //Set status change options and protections flag.
   JPLTEXT=
   JPLTEXT=        status_option_count = set_status_options()
   JPLTEXT=        if ( status_option_count > 0 )
   JPLTEXT=        {
   JPLTEXT=                protect_status = gbl$FALSE
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        // Always prepare the non-dynamic grid members
   JPLTEXT=        call wgrid_prepare()
   JPLTEXT=
   JPLTEXT=        // Always verify if dynamic cost structure
   JPLTEXT=        // components should be prepared
   JPLTEXT=        call dyncost_prepare()
   JPLTEXT=
   JPLTEXT=        // Remove quotes from list of hour codes to display in\
 confirmation dialog box
   JPLTEXT=        v_txt_list_of_hrscodes = ut_replace_char\
 (cfg$INPUT_HRSPRIM_HRSCODES_CLEAR,"'",' ')
   JPLTEXT=
   JPLTEXT=        if(ut_is_read_only_employee())
   JPLTEXT=        {
   JPLTEXT=                protect_status          = gbl$TRUE
   JPLTEXT=                protect_time            = gbl$TRUE
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=
   JPLTEXT=        if ( (protect_status) || (protect_time) )
   JPLTEXT=        {
   JPLTEXT=                call ut_protect_all()
   JPLTEXT=                save_dl->hidden          = PV_YES
   JPLTEXT=                import_dl->hidden        = PV_YES
   JPLTEXT=                refresh_dl->hidden       = PV_YES
   JPLTEXT=                new_dl->hidden           = PV_YES
   JPLTEXT=                cut_dl->hidden           = PV_YES
   JPLTEXT=
   JPLTEXT=                w1copy_dl->hidden        = PV_YES
   JPLTEXT=                w1paste_dl->hidden       = PV_YES
   JPLTEXT=                w1delete_dl->hidden      = PV_YES
   JPLTEXT=                w2copy_dl->hidden        = PV_YES
   JPLTEXT=                w2paste_dl->hidden       = PV_YES
   JPLTEXT=                w2delete_dl->hidden      = PV_YES
   JPLTEXT=
   JPLTEXT=                copy_all_dl->hidden      = PV_YES
   JPLTEXT=                copy_week_dl->hidden     = PV_YES
   JPLTEXT=                copy_favs_dl->hidden     = PV_YES
   JPLTEXT=                duplicateline_dl->hidden = PV_YES
   JPLTEXT=                lookup_dl->hidden        = PV_YES
   JPLTEXT=
   JPLTEXT=                for week_num = 1 while week_num <= 2
   JPLTEXT=                {
   JPLTEXT=                  //Allow description display.
   JPLTEXT=
   JPLTEXT=                        w:(week_num)hrcode->on_focus =\
 w:(week_num)hrcode->on_focus ## "jsDisHrcodeAccount(this);"
   JPLTEXT=                        w:(week_num)day_1->on_focus =\
 w:(week_num)day_1->on_focus ## "jsDisHrcodeAccount(this);"
   JPLTEXT=                        w:(week_num)day_2->on_focus =\
 w:(week_num)day_2->on_focus ## "jsDisHrcodeAccount(this);"
   JPLTEXT=                        w:(week_num)day_3->on_focus =\
 w:(week_num)day_3->on_focus ## "jsDisHrcodeAccount(this);"
   JPLTEXT=                        w:(week_num)day_4->on_focus =\
 w:(week_num)day_4->on_focus ## "jsDisHrcodeAccount(this);"
   JPLTEXT=                        w:(week_num)day_5->on_focus =\
 w:(week_num)day_5->on_focus ## "jsDisHrcodeAccount(this);"
   JPLTEXT=                        w:(week_num)day_6->on_focus =\
 w:(week_num)day_6->on_focus ## "jsDisHrcodeAccount(this);"
   JPLTEXT=                        w:(week_num)day_7->on_focus =\
 w:(week_num)day_7->on_focus ## "jsDisHrcodeAccount(this);"
   JPLTEXT=                        w:(week_num)key->on_focus =\
 w:(week_num)key->on_focus ## "jsDisHrcodeAccount(this);"
   JPLTEXT=                        w:(week_num)cost->on_focus =\
 w:(week_num)cost->on_focus ## "jsDisHrcodeAccount(this);"
   JPLTEXT=
   JPLTEXT=                }
   JPLTEXT=
   JPLTEXT=                /* Comment related buttons and fields*/
   JPLTEXT=                emp_delete_dl->hidden    = PV_YES
   JPLTEXT=                emp_deleteall_dl->hidden = PV_YES
   JPLTEXT=                tk_delete_dl->hidden     = PV_YES
   JPLTEXT=                tk_deleteall_dl->hidden  = PV_YES
   JPLTEXT=                call ut_protect_field("emp_comment")
   JPLTEXT=                call ut_protect_field("tk_comment")
   JPLTEXT=        }
   JPLTEXT=        else
   JPLTEXT=        {
   JPLTEXT=                call ut_web_link_repost("refresh_dl",\
 "reload_screen")
   JPLTEXT=                new_dl->default_link = "javascript::addRow_onClick(this);"
   JPLTEXT=                cut_dl->default_link = "javascript::delRow_onClick(this);"
   JPLTEXT=                call ut_web_link_repost("duplicateline_dl",\
 "do_duplicate_row")
   JPLTEXT=
   JPLTEXT=                if (!future)
   JPLTEXT=                        call ut_web_link_repost("copy_all_dl",\
 "do_copy_timesheet")
   JPLTEXT=
   JPLTEXT=                copy_week_dl->default_link =\
 "javascript::jsConfirmWeekCopy('do_copy_week');"
   JPLTEXT=                w1copy_dl->active = PV_NO
   JPLTEXT=                w1delete_dl->active = PV_NO
   JPLTEXT=                w1paste_dl->active = PV_NO
   JPLTEXT=                w2copy_dl->active = PV_NO
   JPLTEXT=                w2delete_dl->active = PV_NO
   JPLTEXT=                w2paste_dl->active = PV_NO
   JPLTEXT=                import_dl->default_link =\
 "javascript::jsImportHrsprim('do_import_hrsprim',':v_txt_list_of_hrscodes');"
   JPLTEXT=                call ut_web_link_repost("copy_favs_dl",\
 "do_copy_favorites")
   JPLTEXT=
   JPLTEXT=
   JPLTEXT=                /* Comment related buttons and fields*/
   JPLTEXT=                emp_delete_dl->hidden    = PV_NO
   JPLTEXT=                emp_deleteall_dl->hidden = PV_NO
   JPLTEXT=                tk_delete_dl->hidden     = PV_NO
   JPLTEXT=                tk_deleteall_dl->hidden  = PV_NO
   JPLTEXT=                call ut_unprotect_field("emp_comment")
   JPLTEXT=                call ut_unprotect_field("tk_comment")
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        if ( !protect_status )
   JPLTEXT=        {
   JPLTEXT=                call ut_unprotect_field("wstatus_type_om")
   JPLTEXT=                call ut_unprotect_field("wstatus_period_om")
   JPLTEXT=                wstatus_type_om->hidden = PV_NO
   JPLTEXT=                tstatus_type_om->hidden = PV_YES
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        if(cfg$USE_DFLT_LV_ACCT && !called)
   JPLTEXT=        {
   JPLTEXT=                for week_num = 1 while week_num<=2
   JPLTEXT=                {
   JPLTEXT=                        w:(week_num)hrcode->on_change =\
 w:(week_num)hrcode->on_change ## "jsDefaultLeave('',this,\
 'payroll',0,false);"
   JPLTEXT=                }
   JPLTEXT=                called = 1
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        if(cfg$ACCOUNT_CUSTOM_FORMULA!="" &&\
 cfg$COST_COMPONENTS_FOR_ENTRY== 0 && !called2)
   JPLTEXT=        {
   JPLTEXT=                for week_num = 1 while week_num<=2
   JPLTEXT=                {
   JPLTEXT=                        w:(week_num)hrcode->on_change =\
 w:(week_num)hrcode->on_change ## "jsDefaultAccount('',this,\
 'payroll',0,false);"
   JPLTEXT=                }
   JPLTEXT=                called2 = 1
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        call ut_web_link_repost("dflt_dl", "update_default")
   JPLTEXT=        call ut_web_link_repost("pre_dl", "update_pre")
   JPLTEXT=//      call ut_web_link_repost("lpre_dl", "load_pre")
   JPLTEXT=//      call ut_web_link_repost("ldflt_dl", "load_default")
   JPLTEXT=
   JPLTEXT=        back_dl->label = 'Back'
   JPLTEXT=        S_status->label = 'Status::'
   JPLTEXT=
   JPLTEXT=        if(cfg$COST_COMPONENTS_FOR_ENTRY)
   JPLTEXT=        {
   JPLTEXT=                for i = 1 while i <= 21
   JPLTEXT=                {
   JPLTEXT=                        w1dyncost:(i)->on_focus =\
 w1dyncost:(i)->on_focus ## "CompDescription(:(i), this, '');"
   JPLTEXT=                        w2dyncost:(i)->on_focus =\
 w2dyncost:(i)->on_focus ## "CompDescription(:(i), this, '');"
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        call get_favorites_payroll_data()
   JPLTEXT=
   JPLTEXT=        w1telework->column_title = cfg$TELEWORK_TITLE
   JPLTEXT=        w2telework->column_title = cfg$TELEWORK_TITLE
   JPLTEXT=
   JPLTEXT=        // Always protect the status column for BOR, this is\
 assuming COMP13 is always the Status column
   JPLTEXT=        // The operations performed below to protect the field\
 should be consistent with
   JPLTEXT=        // the operations performed in\
 dyncost_prepare@ut_client.jpl to protect fields.
   JPLTEXT=        if (cfg$COST_COMPONENTS_COST_KEY_BOR)
   JPLTEXT=        {
   JPLTEXT=                for week_num = 1 while week_num<=2
   JPLTEXT=                {
   JPLTEXT=                   call ut_protect_field("w:(week_num)dyncost13")
   JPLTEXT=                   w:(week_num)dyncost13->attributes =\\
   JPLTEXT=                   w:(week_num)dyncost13->attributes ## ' ' ##\\
   JPLTEXT=                  \
 'style="background-color::var(--qt-nav-link-hover);"'
   JPLTEXT=
   JPLTEXT=                        /*
   JPLTEXT=                        // TESTING FOR ADVERSE SIDE EFFECTS OF\
 HIDING STATUS AND DESCRIPTION IN THE PAYROLL SCREEN
   JPLTEXT=                        // Hide status from the payroll screen
   JPLTEXT=                        w:(week_num)dyncost13 -> hidden = PV_YES
   JPLTEXT=                        // Hide description from the payroll screen
   JPLTEXT=                        w:(week_num)dyncost15 -> hidden = PV_YES
   JPLTEXT=                        */
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        call select_leavebal()
   JPLTEXT=        call select_comments()
   JPLTEXT=
   JPLTEXT=        return (0)
   JPLTEXT=
   JPLTEXT=} //end proc refresh_screen()
