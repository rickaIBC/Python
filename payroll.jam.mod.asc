 w:(week_num)dyncost5[grid_occ]
   JPLTEXT=        w:(week_num)dyncost6[new_grid_occ]  =\
 w:(week_num)dyncost6[grid_occ]
   JPLTEXT=        w:(week_num)dyncost7[new_grid_occ]  =\
 w:(week_num)dyncost7[grid_occ]
   JPLTEXT=        w:(week_num)dyncost8[new_grid_occ]  =\
 w:(week_num)dyncost8[grid_occ]
   JPLTEXT=        w:(week_num)dyncost9[new_grid_occ]  =\
 w:(week_num)dyncost9[grid_occ]
   JPLTEXT=        w:(week_num)dyncost10[new_grid_occ] =\
 w:(week_num)dyncost10[grid_occ]
   JPLTEXT=        w:(week_num)dyncost11[new_grid_occ] =\
 w:(week_num)dyncost11[grid_occ]
   JPLTEXT=        w:(week_num)dyncost12[new_grid_occ] =\
 w:(week_num)dyncost12[grid_occ]
