   JPLTEXT=//      Routine:   do_delete_all_emp_cmt
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              Delete all records for this individual during this\
 pay period
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2024-08-15      A. McCormick    ENHC0037149
   JPLTEXT=//                      Created from time_comments.jam
   JPLTEXT=//      2025-01-23      A. McCormick    DFCT0014136
   JPLTEXT=//                      Set the UsrMadeChg, status and edit flags\
 when needed.
   JPLTEXT=//      2025-04-15      T. Peace        DFCT0014160
   JPLTEXT=//                      For amendments, set status to UNV after
   JPLTEXT=//                      comments are removed.
   JPLTEXT=//                      Don't turn on edit_reqd.  It will be set
   JPLTEXT=//                      elsewhere if needed.
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=proc do_delete_all_emp_cmt()
   JPLTEXT={
   JPLTEXT=        vars retval(10), i
   JPLTEXT=        vars amend_comments_cnt(10)
   JPLTEXT=        vars edit045_cnt(10)
   JPLTEXT=
   JPLTEXT=        call sm_n_clear_array("err_msg_dl")
   JPLTEXT=        call sm_n_clear_array("err_img")
   JPLTEXT=        err_msg_idx = 1
   JPLTEXT=
   JPLTEXT=        UsrMadeChg = gbl$TRUE
   JPLTEXT=        if(amendment)
   JPLTEXT=        {
   JPLTEXT=                status_flag = gbl$TRUE
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        for i = 1 while i <= 50
   JPLTEXT=        {
   JPLTEXT=                db_array[i]=''
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        if (!amendment)
   JPLTEXT=        {
   JPLTEXT=                db_array [1] = "DELETE FROM timesheet_comments\
 WHERE ppyear = :ppyear AND pp = :pp AND empid = :+active_empid and role =\
 'EMPLOYEE'"
   JPLTEXT=        }
   JPLTEXT=        else
   JPLTEXT=        {
   JPLTEXT=                db_array [1] = "DELETE FROM\
 amend_timesheet_comments WHERE empid = :+active_empid and role = 'EMPLOYEE'"
   JPLTEXT=                db_array [1] = db_array [1] ## " and ppyear =\
 :prior_tna_data[idx$PAY_CORR_YEAR] and pp = :prior_tna_data[idx$PAY_CORR_PP]\
 and process_ppyear = :ppyear and process_pp = :pp and process_seq =\
 :prior_tna_data[idx$PAY_CURR_SEQ]"
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        retval = ut_dbms_write_qtctx("tna_data")
   JPLTEXT=        // clear the on screen array and reset occurrences count
   JPLTEXT=        call sm_n_clear_array("emp_comment")
   JPLTEXT=        emp_comment->num_occurrences = 0
   JPLTEXT=        if (amendment)
   JPLTEXT=        {
   JPLTEXT=               amend_comments_cnt =\
 ut_amend_comments_count(":active_empid",ppyear,pp,prior_tna_data[idx$PAY_CORR\
_YEAR],prior_tna_data[idx$PAY_CORR_PP],prior_tna_data[idx$PAY_CURR_SEQ])
   JPLTEXT=                edit045_cnt = ut_is_edit_active("E-PAY-045")
   JPLTEXT=
   JPLTEXT=                // If no comments remain after the employee\
 comments are removed:
   JPLTEXT=                // then we need to downgrade status to UNV if\
 E-PAY-045 is active.
   JPLTEXT=                if (amend_comments_cnt == 0    &&  \\
   JPLTEXT=                    orig_status != "UNV"       &&  \\
   JPLTEXT=                    edit045_cnt  > 0 )
   JPLTEXT=                {
   JPLTEXT=                        call change_status_array("UNV", 1, 14)
   JPLTEXT=                        wstatus_type_om = 'UNV'
   JPLTEXT=                        call  change_status()
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        call refresh_screen()
   JPLTEXT=
   JPLTEXT=        if !(retval)
   JPLTEXT=        {
   JPLTEXT=                call ut5_msg_information("All Employee Comment rows\
 deleted. Comment data saved.")
   JPLTEXT=                save_flag = 1
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        return(retval)
   JPLTEXT=}
   JPLTEXT=
  \
