 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine: valid_time
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine validates the time. If performing a
   JPLTEXT=//              screen validation (since the VALIDED bit is not
   JPLTEXT=//              retained in the WEB), flag that total calculations
   JPLTEXT=//              be performed once after screen validation.
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2001-01-16      D. Bodlien      CR20000396
   JPLTEXT=//                      Added message to user when hours data is\
 changed automatically.
   JPLTEXT=//                      Added widget wday_begin to hold a copy of\
 the time before any changes.
   JPLTEXT=//                      Changed procedures scr_entry, valid_time,\
 daily_total, do_valid_screen,
   JPLTEXT=//                      and change_status.
   JPLTEXT=//      2016-06-20      E. Ramos        CR20160062
   JPLTEXT=//                      Show week 1 and week 2 on the same screen.
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=proc valid_time (fld_no, fld_val, fld_occ, fld_flag)
   JPLTEXT={
   JPLTEXT=        vars retval = 0, t_time(10)
   JPLTEXT=        vars day_idx(10)
   JPLTEXT=        vars hrcode_nm (32)
   JPLTEXT=
   JPLTEXT=        call flag_changes (fld_no, fld_val, fld_occ, fld_flag)
   JPLTEXT=
   JPLTEXT=        // This procedure cannot return based on the results of\
 flag_changes.
   JPLTEXT=        // The processing for rounding or not rounding regular\
 hours must be done even if the time for regular hours did not
   JPLTEXT=        //       change; this is because the time for AWOL may have\
 been deleted.
   JPLTEXT=
   JPLTEXT=        //First convert the time to minutes, then hours for\
 consistent display.
   JPLTEXT=
   JPLTEXT=        t_time = ut_squeeze(fld_val, ut$SQUEEZE_LEAD_TRAIL, "")
   JPLTEXT=        t_time = ut_hhmm2mm(t_time)
   JPLTEXT=        if ( t_time == -1 )
   JPLTEXT=        {
   JPLTEXT=                call ut5_msg_warning("Invalid time ':fld_val'.\
 Correct format is HH:" ## "MM.")
   JPLTEXT=                retval = 1
   JPLTEXT=        }
   JPLTEXT=        else if ( t_time == -2 )
   JPLTEXT=        {
   JPLTEXT=                call ut5_msg_warning("Invalid time ':fld_val'.\
 Minute field greater than 59.%N" ## \\
   JPLTEXT=                        "Correct format is HH:" ## "MM.")
   JPLTEXT=                retval = 1
   JPLTEXT=        }
   JPLTEXT=        else if ( t_time > 1440 )
   JPLTEXT=        {
   JPLTEXT=                call ut5_msg_warning("Invalid time ':fld_val'.\
 Maximum time is 24:" ## "00.")
   JPLTEXT=                retval = 1
   JPLTEXT=        }
   JPLTEXT=        if ( retval )
   JPLTEXT=        {
   JPLTEXT=                return (retval)
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        //Do daily totals only once during screen validation.
   JPLTEXT=
   JPLTEXT=        if ( !scr_validation )
   JPLTEXT=        {
   JPLTEXT=                hrcode_nm = @field_num(:fld_no)->name
   JPLTEXT=                day_idx = hrcode_nm(6,1)
   JPLTEXT=
   JPLTEXT=                call daily_total\
 (GetWeekNumberFromFieldNumber(fld_no),day_idx)
   JPLTEXT=        }
   JPLTEXT=        return (retval)
   JPLTEXT=} //end proc valid_time()
   JPLTEXT=
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine: valid_worktype_time
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine validates the time. If performing a
   JPLTEXT=//              screen validation (since the VALIDED bit is not
   JPLTEXT=//              retained in the WEB), flag that total calculations
   JPLTEXT=//              be performed once after screen validation.
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2016-02-22      E. Ramos
   JPLTEXT=//                      Created based on valid_time.
   JPLTEXT=//      2016-06-20      E. Ramos        CR20160062
   JPLTEXT=//                      Show week 1 and week 2 on the same screen.
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=proc valid_worktype_time (fld_no, fld_val, fld_occ, fld_flag)
   JPLTEXT={
   JPLTEXT=        vars retval = 0, t_time(10)
   JPLTEXT=        vars day_idx(10)
   JPLTEXT=        vars hrcode_nm (32)
   JPLTEXT=
   JPLTEXT=        call flag_changes (fld_no, fld_val, fld_occ, fld_flag)
   JPLTEXT=
   JPLTEXT=        // This procedure cannot return based on the results of\
 flag_changes.
   JPLTEXT=        // The processing for rounding or not rounding regular\
 hours must be done even if the time for regular hours did not
   JPLTEXT=        //       change; this is because the time for AWOL may have\
 been deleted.
   JPLTEXT=
   JPLTEXT=        //First convert the time to minutes, then hours for\
 consistent display.
   JPLTEXT=
   JPLTEXT=        t_time = ut_squeeze(fld_val, ut$SQUEEZE_LEAD_TRAIL, "")
   JPLTEXT=        t_time = ut_hhmm2mm(t_time)
   JPLTEXT=        if ( t_time == -1 )
   JPLTEXT=        {
   JPLTEXT=                call ut5_msg_warning("Invalid time ':fld_val'.\
 Correct format is HH:" ## "MM.")
   JPLTEXT=                retval = 1
   JPLTEXT=        }
   JPLTEXT=        else if ( t_time == -2 )
   JPLTEXT=        {
   JPLTEXT=                call ut5_msg_warning("Invalid time ':fld_val'.\
 Minute field greater than 59.%N" ## \\
   JPLTEXT=                        "Correct format is HH:" ## "MM.")
   JPLTEXT=                retval = 1
   JPLTEXT=        }
   JPLTEXT=        else if ( t_time > (99*60) )
   JPLTEXT=        {
   JPLTEXT=                call ut5_msg_warning("Invalid time ':fld_val'. Work\
 type time can not exceed 99 hours.")
   JPLTEXT=                retval = 1
   JPLTEXT=        }
   JPLTEXT=        else if ( t_time < (0) )
   JPLTEXT=        {
   JPLTEXT=                call ut5_msg_warning("Invalid time ':fld_val'. Work\
 type time can not be negative.")
   JPLTEXT=                retval = 1
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        if ( retval )
   JPLTEXT=        {
   JPLTEXT=                return (retval)
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        //Do daily totals only once during screen validation.
   JPLTEXT=
   JPLTEXT=        return (retval)
   JPLTEXT=} //end proc valid_worktype_time()
   JPLTEXT=
