 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine: daily_total
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine calculates the daily total for the\
 specified day.
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2001-01-16      D. Bodlien      CR20000396
   JPLTEXT=//                      Added message to user when hours data is\
 changed automatically.
   JPLTEXT=//                      Added widget wday_begin to hold a copy of\
 the time before any changes.
   JPLTEXT=//                      Changed procedures scr_entry, valid_time,\
 daily_total, do_valid_screen, and change_status
   JPLTEXT=//      2001-04-29      D. Bodlien
   JPLTEXT=//                      Remove prior change for CR2000320.
   JPLTEXT=//                      This applies to Sign In employees only.
   JPLTEXT=//                      Do not round time for Sign In employees.
   JPLTEXT=//                      Sign In employees can enter overtime in\
 exact minutes and Payroll employees cannot.
   JPLTEXT=//      2000-09-10      D. Bodlien / E. Ren     CR20000398
   JPLTEXT=//                      Add processing for amendments.
   JPLTEXT=//      2009-03-10      A. McCormick    CR20090021
   JPLTEXT=//                      Add an exception for three day work week\
 employees (3DY)
   JPLTEXT=//      2012-01-09      A. McCormick    CR20110177
   JPLTEXT=//                      Change ut_15mm2hhmm to ut_incr_mm2hhmm -\
 Allows NARA to use
   JPLTEXT=//                      6 minute increments, and other clients want\
 15 minute increments.
   JPLTEXT=//      2012-02-20      A. McCormick    CR20100219
   JPLTEXT=//                      Allow hour code 'PPC' (Park Police Comp) to\
 be entered in one minute increments,
   JPLTEXT=//                      similar to AWOL.
   JPLTEXT=//      2016-06-20      E. Ramos        CR20160062
   JPLTEXT=//                      Show week 1 and week 2 on the same screen.
   JPLTEXT=//      2023-03-08      E. Ramos        DFCT0013147
   JPLTEXT=//                      Set force_save_flag=1 flag if hours are\
 rounded
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=proc daily_total (week_num,day_idx)
   JPLTEXT={
   JPLTEXT=        vars num_occ(10), cur_idx(10), i(10),cur_tot(10) = 0
   JPLTEXT=        vars cur_tod_tot(10) = 0, cur_others_tot(10) = 0
   JPLTEXT=        vars week_1(10) = 0, week_2(10) = 0, fld_val(10),\
 fld_nm(32)
   JPLTEXT=        vars t_hrcode (5)
   JPLTEXT=        vars j (10)
   JPLTEXT=        vars round_min (10)
   JPLTEXT=        vars hhmm (10)
   JPLTEXT=        vars t_time (10)
   JPLTEXT=
   JPLTEXT=        num_occ = w:(week_num)day_:day_idx -> num_occurrences
   JPLTEXT=        for i = 1 while i <= num_occ
   JPLTEXT=        {
   JPLTEXT=                t_time = ut_hhmm2mm (w:(week_num)day_:day_idx [i])
   JPLTEXT=                w:(week_num)day_begin [i] = t_time
   JPLTEXT=                t_hrcode = w:(week_num)hrcode [i]
   JPLTEXT=                t_hrcode = ut_squeeze (t_hrcode,\
 ut$SQUEEZE_LEAD_TRAIL, "")
   JPLTEXT=                round_min = gbl$TRUE
   JPLTEXT=                if ((ut_bitop_and (:tna_data_name\
 [idx$ACTIVE_PROP], gbl$ACTIVE_PROP_SIOREQ)) && (!amendment))
   JPLTEXT=                {
   JPLTEXT=                        round_min = gbl$FALSE
   JPLTEXT=                }
   JPLTEXT=                else if (t_hrcode == tna_awolasminutes || t_hrcode\
 == tna$PARKPOLICECOMP)
   JPLTEXT=                {
   JPLTEXT=                        round_min = gbl$FALSE
   JPLTEXT=                }
   JPLTEXT=                else if (t_hrcode == tna_regularasminutes)
   JPLTEXT=                {
   JPLTEXT=
   JPLTEXT=                        //Check for other time in the day allocated\
 to tna$AWOLASMINUTES.
   JPLTEXT=
   JPLTEXT=                        for j = 1 while j <= num_occ
   JPLTEXT=                        {
   JPLTEXT=                                if ((w:(week_num)day_:day_idx [j]\
 != "") && (w:(week_num)hrcode [j] == tna_awolasminutes))
   JPLTEXT=                                {
   JPLTEXT=                                        round_min = gbl$FALSE
   JPLTEXT=                                        break
   JPLTEXT=                                }
   JPLTEXT=                        }
   JPLTEXT=                }
   JPLTEXT=
   JPLTEXT=                if (tna_data[idx$ACTIVE_ALTSCHD] ==\
 gbl$EMP_SCHD_3DY)
   JPLTEXT=                {
   JPLTEXT=                        round_min = gbl$FALSE
   JPLTEXT=                }
   JPLTEXT=
   JPLTEXT=                if (t_time == 0)
   JPLTEXT=                {
   JPLTEXT=                        hhmm = ""
   JPLTEXT=                }
   JPLTEXT=                else if (round_min)
   JPLTEXT=                {
   JPLTEXT=                        hhmm = ut_incr_mm2hhmm (t_time, t_hrcode)
   JPLTEXT=                }
   JPLTEXT=                else
   JPLTEXT=                {
   JPLTEXT=                        hhmm = ut_mm2hhmm (t_time, hhmm)
   JPLTEXT=                }
   JPLTEXT=                w:(week_num)day_:day_idx [i] = hhmm
   JPLTEXT=                cur_tot = cur_tot + ut_hhmm2mm (hhmm)
   JPLTEXT=                if ( ut_isRegularHrsCode (t_hrcode))
   JPLTEXT=                  cur_tod_tot = cur_tod_tot + ut_hhmm2mm (hhmm)
   JPLTEXT=                else
   JPLTEXT=                  cur_others_tot = cur_others_tot + ut_hhmm2mm\
 (hhmm)
   JPLTEXT=        }
   JPLTEXT=        for i = 1 while i <= num_occ
   JPLTEXT=        {
   JPLTEXT=                if (w:(week_num)day_begin [i] != ut_hhmm2mm\
 (w:(week_num)day_:day_idx [i]))
   JPLTEXT=                {
   JPLTEXT=                        data_changed_message = gbl$TRUE
   JPLTEXT=                        call ut5_msg_information ("Time for hour\
 code " ## w:(week_num)hrcode[i] ## " was changed on day :day_idx of week\
 :(week_num).")
   JPLTEXT=
   JPLTEXT=                        // If hours have been rounded require\
 saving
   JPLTEXT=                        force_save_flag=1
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=        cur_idx = ((week_num - 1) * 7) + day_idx
   JPLTEXT=
   JPLTEXT=        tot_array [cur_idx] = cur_tot
   JPLTEXT=        tod_others_tot_array [cur_idx] = cur_others_tot
   JPLTEXT=        tod_tot_array [cur_idx] = cur_tod_tot
   JPLTEXT=
   JPLTEXT=        w:(week_num)total_:day_idx [1] = blankifzero( ut_mm2hhmm\
 (cur_tod_tot   , fld_val) )
   JPLTEXT=        w:(week_num)total_:day_idx [2] = blankifzero( ut_mm2hhmm\
 (cur_others_tot, fld_val) )
   JPLTEXT=        w:(week_num)total_:day_idx [3] = blankifzero( ut_mm2hhmm\
 (cur_tot       , fld_val)  )
   JPLTEXT=
   JPLTEXT=
   JPLTEXT=        //Do week totals only once during screen validation.
   JPLTEXT=
   JPLTEXT=        if ( !scr_validation )
   JPLTEXT=        {
   JPLTEXT=                call get_week_totals()
   JPLTEXT=        }
   JPLTEXT=        return (0)
   JPLTEXT=} //end proc daily_total()
   JPLTEXT=
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine: blankifzero
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine returns a blank value if the time is\
 all zeros
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=string proc blankifzero (fld_val)
   JPLTEXT={
   JPLTEXT=        if (fld_val == "00::00")
   JPLTEXT=                return ""
   JPLTEXT=        return fld_val
   JPLTEXT=}
   JPLTEXT=
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine: get_daily_totals
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine calculates the totals for each day in\
 the pay period.
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2016-06-20      E. Ramos        CR20160062
   JPLTEXT=//                      Created.
   JPLTEXT=//      2016-07-13      E. Ramos        PR20160238
   JPLTEXT=//                      Save totals in a bundle to be able to show\
 them
   JPLTEXT=//                      when there are screen validation errors.
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=proc get_daily_totals()
   JPLTEXT={
   JPLTEXT=  vars i w
   JPLTEXT=  for w=1 while w<=2
   JPLTEXT=  {
   JPLTEXT=    for i = 1 while i <= 7
   JPLTEXT=    {
   JPLTEXT=      call daily_total(w,i)
   JPLTEXT=    }
   JPLTEXT=  }
   JPLTEXT=
   JPLTEXT=  send bundle "daily_totals" data w1col1, w1total_1, w1total_2,\
 w1total_3, w1total_4,w1total_5,w1total_6,w1total_7,w1total,w2col1, w2total_1,\
 w2total_2, w2total_3, w2total_4,w2total_5,w2total_6,w2total_7,w2total
   JPLTEXT=
   JPLTEXT=return
   JPLTEXT=}
   JPLTEXT=
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine: get_week_days_occ(week_num)
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine calculates the maximum occcurrences in\
 the grid for a given week.
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2019-09-03      E. Ramos        ENHC0035628
   JPLTEXT=//                      Created.
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=proc get_week_days_occ(week_num)
   JPLTEXT={
   JPLTEXT=  vars i
   JPLTEXT=  vars numocc
   JPLTEXT=
   JPLTEXT=  for i=1 while i<=7
   JPLTEXT=  {
   JPLTEXT=    if w:(week_num)day_:(i)->num_occurrences > numocc
   JPLTEXT=      numocc = w:(week_num)day_:(i)->num_occurrences
   JPLTEXT=  }
   JPLTEXT=  if numocc < w:(week_num)grid->onscreen_rows
   JPLTEXT=    numocc = w:(week_num)grid->onscreen_rows
   JPLTEXT=
   JPLTEXT=  if numocc < w:(week_num)daystotal->num_occurrences
   JPLTEXT=    numocc = w:(week_num)daystotal->num_occurrences
   JPLTEXT=
   JPLTEXT=  return numocc
   JPLTEXT=}
   JPLTEXT=
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine: get_week_totals
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine calculates the week and period totals.
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2016-02-22      E. Ramos
   JPLTEXT=//                      Added logic to update worktype totals.
   JPLTEXT=//      2016-06-20      E. Ramos        CR20160062
   JPLTEXT=//                      Show week 1 and week 2 on the same screen.
   JPLTEXT=//      2016-07-13      E. Ramos        PR20160238
   JPLTEXT=//                      Save calculated totals in a bundle.
   JPLTEXT=//      2019-09-03      E. Ramos        ENHC0035628
   JPLTEXT=//                      Calculate week row totals and populate\
 w(:week_num)daystotal
   JPLTEXT=//      2019-11-19      E. Ramos        ENHC0035637 REWORK
   JPLTEXT=//                      Format days total using the\
 ut_formatdaystotal function.
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=proc get_week_totals ()
   JPLTEXT={
   JPLTEXT=        vars i(10), j(10), cur_tot(10), week_1(10) = 0, week_2(10)\
 = 0, fld_nm(32)
   JPLTEXT=        vars week_1_tod_others(10)=0
   JPLTEXT=        vars week_2_tod_others(10)=0
   JPLTEXT=        vars week_1_tod(10)=0
   JPLTEXT=        vars week_2_tod(10)=0
   JPLTEXT=        vars t_buf(30)
   JPLTEXT=        vars v_regular_hours_reported
   JPLTEXT=        vars v_premium_hours_reported
   JPLTEXT=        vars grid_occ
   JPLTEXT=        vars week_num
   JPLTEXT=
   JPLTEXT=        vars row_mi
   JPLTEXT=
   JPLTEXT=        vars week_days_occ
   JPLTEXT=        for week_num = 1 while week_num<=2
   JPLTEXT=        {
   JPLTEXT=                week_days_occ = get_week_days_occ(week_num)
   JPLTEXT=                for i = 1 while (i <= week_days_occ)
   JPLTEXT=                {
   JPLTEXT=                        row_mi = 0
   JPLTEXT=                        for j = 1 while j <= 7
   JPLTEXT=                        {
   JPLTEXT=                                row_mi = row_mi + ut_hhmm2mm (\
 w:(week_num)day_:(j)[i] )
   JPLTEXT=                        }
   JPLTEXT=                        w:(week_num)daystotal[i] =\
 ut_formatdaystotal(ut_mm2hhmm (row_mi))
   JPLTEXT=                        if (cfg$DEVELOPMENT)
   JPLTEXT=                                call ut_log_error("enhc0035628\
 get_week_totals week_num (:week_num) week_days_occ (:week_days_occ) i (:i)\
 hrcode (" ## (w:(week_num)hrcode->num_occurrences) ## " grid " ##\
 w:(week_num)grid->onscreen_rows )
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        for i = 1 while i <= 7
   JPLTEXT=        {
   JPLTEXT=                week_1 = week_1 + tot_array[i]
   JPLTEXT=                week_2 = week_2 + tot_array[i + 7]
   JPLTEXT=
   JPLTEXT=                week_1_tod = week_1_tod + tod_tot_array[i]
   JPLTEXT=                week_2_tod = week_2_tod + tod_tot_array[i + 7]
   JPLTEXT=
   JPLTEXT=                week_1_tod_others = week_1_tod_others +\
 tod_others_tot_array[i]
   JPLTEXT=                week_2_tod_others = week_2_tod_others +\
 tod_others_tot_array[i+7]
   JPLTEXT=        }
   JPLTEXT=        cur_tot = week_1 + week_2
   JPLTEXT=        period_total = ut_mm2hhmm(cur_tot, period_total)
   JPLTEXT=
   JPLTEXT=        // Copy calculated totals to the grid that is displayed on\
 the screen
   JPLTEXT=
   JPLTEXT=        w1total[1] = ut_mm2hhmm(week_1_tod, week_total)
   JPLTEXT=        w2total[1] = ut_mm2hhmm(week_2_tod, week_total)
   JPLTEXT=
   JPLTEXT=        w1total[2] = ut_mm2hhmm(week_1_tod_others, week_total)
   JPLTEXT=        w2total[2] = ut_mm2hhmm(week_2_tod_others, week_total)
   JPLTEXT=
   JPLTEXT=        w1total[3] = ut_mm2hhmm(week_1, week_total)
   JPLTEXT=        w2total[3] = ut_mm2hhmm(week_2, week_total)
   JPLTEXT=
   JPLTEXT=        fld_nm = "week_1"
   JPLTEXT=        cur_tot = :fld_nm
   JPLTEXT=        week_total = ut_mm2hhmm(cur_tot, week_total)
   JPLTEXT=
   JPLTEXT=        if (cfg$WORKTYPE_EMPORGCODES!="NONE")
   JPLTEXT=        {
   JPLTEXT=                // Calculate work type total regular and premium\
 hours
   JPLTEXT=                regular_hours_total = 0
   JPLTEXT=                premium_hours_total = 0
   JPLTEXT=
   JPLTEXT=                for i = 1 while i <= worktype_code->num_occurrences
   JPLTEXT=                {
   JPLTEXT=                        regular_hours_total = regular_hours_total +\
 ut_hhmm2mm(regular_hours[i])
   JPLTEXT=                        premium_hours_total = premium_hours_total +\
 ut_hhmm2mm(premium_hours[i])
   JPLTEXT=                }
   JPLTEXT=
   JPLTEXT=
   JPLTEXT=
   JPLTEXT=                // Calculate total regular and premium hours\
 reported on timesheet
   JPLTEXT=                v_regular_hours_reported = 0
   JPLTEXT=                v_premium_hours_reported = 0
   JPLTEXT=
   JPLTEXT=                for week_num = 1 while week_num<=2
   JPLTEXT=                {
   JPLTEXT=                        for i = 1 while i <=\
 w:(week_num)hrcode->num_occurrences
   JPLTEXT=                        {
   JPLTEXT=
   JPLTEXT=                                if (ut_isWorkTypeRegularHrsCode\
 (w:(week_num)hrcode[i]) )
   JPLTEXT=                                {
   JPLTEXT=                                        for j = 1 while j <= 7
   JPLTEXT=                                        {
   JPLTEXT=                                               \
 v_regular_hours_reported = v_regular_hours_reported +\
 ut_hhmm2mm(w:(week_num)day_:(j) [i])
   JPLTEXT=
   JPLTEXT=                                        }
   JPLTEXT=                                }
   JPLTEXT=                                if (ut_isWorkTypePremiumHrsCode\
 (w:(week_num)hrcode[i]) )
   JPLTEXT=                                {
   JPLTEXT=                                        for j = 1 while j <= 7
   JPLTEXT=                                        {
   JPLTEXT=                                               \
 v_premium_hours_reported = v_premium_hours_reported +\
 ut_hhmm2mm(w:(week_num)day_:(j) [i])
   JPLTEXT=                                        }
   JPLTEXT=                                }
   JPLTEXT=                        }
   JPLTEXT=                }
   JPLTEXT=
   JPLTEXT=                regular_hours_unaccounted = ut_mm2hhmm\
 (v_regular_hours_reported - regular_hours_total, t_buf)
   JPLTEXT=                premium_hours_unaccounted = ut_mm2hhmm\
 (v_premium_hours_reported - premium_hours_total, t_buf)
   JPLTEXT=
   JPLTEXT=                regular_hours_reported = ut_mm2hhmm\
 (v_regular_hours_reported, t_buf)
   JPLTEXT=                premium_hours_reported = ut_mm2hhmm\
 (v_premium_hours_reported, t_buf)
   JPLTEXT=
   JPLTEXT=                // Convert work type total regular and premium\
 hours to HH:MM format
   JPLTEXT=                regular_hours_total =\
 ut_mm2hhmm(regular_hours_total, t_buf)
   JPLTEXT=                premium_hours_total =\
 ut_mm2hhmm(premium_hours_total, t_buf)
   JPLTEXT=
   JPLTEXT=                // Show unaccounted hours in red if not 0
   JPLTEXT=
   JPLTEXT=                send bundle "worktype_totals" data\
 regular_hours_reported, regular_hours_total, regular_hours_unaccounted,\
 premium_hours_reported, premium_hours_total, premium_hours_unaccounted
   JPLTEXT=
   JPLTEXT=                regular_hours->column_title = "Regular</br>" ##\
 regular_hours_total
   JPLTEXT=                premium_hours->column_title = "Premium</br>" ##\
 premium_hours_total
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        return (0)
   JPLTEXT=} //end proc get_week_totals()
   JPLTEXT=
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine: do_valid_screen
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine validates the entire screen. In a WEB,
   JPLTEXT=//              all changed fields are validated against their\
 field
   JPLTEXT=//              validation functions. In a GUI, all but the last\
 field
   JPLTEXT=//              requires validation.
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2001-01-16      D. Bodlien      CR20000396
   JPLTEXT=//                      Added message to user when hours data is\
 changed automatically.
   JPLTEXT=//                      Added widget wday_begin to hold a copy of\
 the time before any changes.
   JPLTEXT=//                      Changed procedures scr_entry, valid_time,\
 daily_total,
   JPLTEXT=//                      do_valid_screen, and change_status
   JPLTEXT=//      2002-04-16      D. Maynard      PR20020291
   JPLTEXT=//                      Set a new field to tell web_exit what\
 message
   JPLTEXT=//                      to display if there are validation errors.
   JPLTEXT=//      2005-03-21      A. McCormick    CR20040387
   JPLTEXT=//                      Skip validations of Key and Cost of freeze\
 variable set for wgrid row
   JPLTEXT=//      2005-05-11      E. Ramos        CR20050039
   JPLTEXT=//                      Validate components
   JPLTEXT=//      2005-06-23      T. Peace         PR20050503
   JPLTEXT=//                      process Invalid cost components as\
 not-fatal,
   JPLTEXT=//                      issue warning message(s) and allow
   JPLTEXT=//                      timesheet to save.
   JPLTEXT=//      2005-10-21      T. Peace        PR20050927
   JPLTEXT=//                      Supress validation warning of component\
 field
   JPLTEXT=//                      if default leave entered.
   JPLTEXT=//      2006-10-08      E. Ramos        CR20060209
   JPLTEXT=//                      Added logic to correctly validate the\
 costkey field when components are enabled.
   JPLTEXT=//      2006-12-18      E. Ramos        PR20060887
   JPLTEXT=//                      Remove commented msg emsg command.
   JPLTEXT=//      2007-02-16      T. Peace        PR20060893
   JPLTEXT=//                      Pass indicator to valid_component proc when\
 processing a default leave type.
   JPLTEXT=//      2007-03-01      T. Peace        CR20060273
   JPLTEXT=//                      Add widgets and processing for new telework\
 field on timesheets.
   JPLTEXT=//      2007-08-29      E. Ramos        PR20070603
   JPLTEXT=//                      Force execution of the validation function\
 of the cost key field when
   JPLTEXT=//                      cfg$COST_COMPONENTS_COST_KEY is set to 1.
   JPLTEXT=//      2008-10-09      E. Ramos        PR20080591
   JPLTEXT=//                      Present warnings when component fields\
 don't pass the lookup validation.
   JPLTEXT=//      2008-10-22      E. Ramos        PR20080721
   JPLTEXT=//                      Allow switching weeks even if\
 do_valid_screen generated warnings.
   JPLTEXT=//      2010-07-06      E. Ramos        PR20100275
   JPLTEXT=//                      Perform validations created for USGS-FBMS\
 as on-screen validations
   JPLTEXT=//                      on top of performing them as edit checks\
 E-PAY-135 and E-PAY-136.
   JPLTEXT=//      2011-07-06      E. Ramos        CR20110042
   JPLTEXT=//                      Perform validations created for FWS-FBMS as\
 on-screen validations
   JPLTEXT=//                      on top of performing them as edit checks\
 E-PAY-136 and E-PAY-145.
   JPLTEXT=//                      Validate values entered for favorites if\
 current role is allowed to change favorites.
   JPLTEXT=//      2012-03-20      E. Ramos        PR20120363
   JPLTEXT=//                      Perform on-screen validations on favorites\
 only on new favorites,
   JPLTEXT=//                      that is, favorites that have not yet been\
 saved to the database.
   JPLTEXT=//      2012-04-19      E. Ramos        PR20120276
   JPLTEXT=//                      Remove single and double quote characters\
 from component fields.
   JPLTEXT=//      2015-02-25      E. Ramos        CR20140260
   JPLTEXT=//                      Add mentoring column.
   JPLTEXT=//      2015-07-01      E. Ramos        CR20150118
   JPLTEXT=//                      Per BIA's request - If E_PAY-174 is enabled
   JPLTEXT=//                      validate that when the Work Order is\
 populated no other component should be populated.
   JPLTEXT=//      2016-02-03      E. Ramos        CR20150129
   JPLTEXT=//                      Added widgets to introduce work type data\
 as part of the regular timesheets.
   JPLTEXT=//      2016-06-20      E. Ramos        CR20160062
   JPLTEXT=//                      Show week 1 and week 2 on the same screen.
   JPLTEXT=//      2021-01-08      A. McCormick    DFCT0011908
   JPLTEXT=//                      Added validations to allow certain special\
 characters and prevent the rest.
   JPLTEXT=//      2021-04-26      E. Ramos        ENHC0036192
   JPLTEXT=//                      Custom validation of favorites when\
 cfg$COST_COMPONENTS_COST_KEY_BOR=1.
   JPLTEXT=//      2023-03-08      E. Ramos        DFCT0013147
   JPLTEXT=//                      Do not reset the data_changed_message flag
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=proc do_valid_screen ()
   JPLTEXT={
   JPLTEXT=        vars retval(10), i(10), j(10), num_occ(10),\
 comp_error_cnt(10)
   JPLTEXT=        vars num_occ_worktype(10)
   JPLTEXT=        vars fld_len
   JPLTEXT=        vars t_buf(30)
   JPLTEXT=        vars tmp
   JPLTEXT=        vars msgstring
   JPLTEXT=        vars sql_stmt(250)
   JPLTEXT=        vars rtv
   JPLTEXT=        vars week_num
   JPLTEXT=        vars v_bor_costkey_exists
   JPLTEXT=
   JPLTEXT=        //Set screen validation and get the before image.
   JPLTEXT=
   JPLTEXT=        validation_error = gbl$FALSE
   JPLTEXT=        scr_validation = 1
   JPLTEXT=
   JPLTEXT=        receive bundle "image_bndl1" keep data bw1hrcode, bw1day_1,\
 bw1day_2, bw1day_3, \\
   JPLTEXT=                bw1day_4, bw1day_5, bw1day_6, bw1day_7, bw1key,\
 bw1cost, bw1shift, \\
   JPLTEXT=                bw1envcode, bw1telework, bw1base, bw1occ,\
 bw1poscode, bw1mentoring, bw1taskcode,\\
   JPLTEXT=               \
 bw1dyncost1,bw1dyncost2,bw1dyncost3,bw1dyncost4,bw1dyncost5,bw1dyncost6,\\
   JPLTEXT=               \
 bw1dyncost7,bw1dyncost8,bw1dyncost9,bw1dyncost10,bw1dyncost11, \\
   JPLTEXT=               \
 bw1dyncost12,bw1dyncost13,bw1dyncost14,bw1dyncost15,bw1dyncost16,bw1dyncost17\
,\\
   JPLTEXT=                bw1dyncost18,bw1dyncost19,bw1dyncost20,bw1dyncost21
   JPLTEXT=
   JPLTEXT=        receive bundle "image_bndl2" keep data bw2hrcode, bw2day_1,\
 bw2day_2, bw2day_3, \\
   JPLTEXT=                bw2day_4, bw2day_5, bw2day_6, bw2day_7, bw2key,\
 bw2cost, bw2shift, \\
   JPLTEXT=                bw2envcode, bw2telework, bw2base, bw2occ,\
 bw2poscode, bw2mentoring, bw2taskcode,\\
   JPLTEXT=               \
 bw2dyncost1,bw2dyncost2,bw2dyncost3,bw2dyncost4,bw2dyncost5,bw2dyncost6,\\
   JPLTEXT=               \
 bw2dyncost7,bw2dyncost8,bw2dyncost9,bw2dyncost10,bw2dyncost11, \\
   JPLTEXT=               \
 bw2dyncost12,bw2dyncost13,bw2dyncost14,bw2dyncost15,bw2dyncost16,bw2dyncost17\
,\\
   JPLTEXT=                bw2dyncost18,bw2dyncost19,bw2dyncost20,bw2dyncost21
   JPLTEXT=
   JPLTEXT=        receive bundle "image_bndl_worktype" data bworktype_code,\
 bworktype_description, bregular_hours, bpremium_hours
   JPLTEXT=
   JPLTEXT=        //Clear all previous errors.
   JPLTEXT=
   JPLTEXT=        call sm_n_clear_array("err_msg_dl")
   JPLTEXT=        call sm_n_clear_array("err_img")
   JPLTEXT=        err_msg_idx = 1
   JPLTEXT=        w1hrcode_lbl = ""
   JPLTEXT=        w1cost_lbl = ""
   JPLTEXT=        w2hrcode_lbl = ""
   JPLTEXT=        w2cost_lbl = ""
   JPLTEXT=
   JPLTEXT=        num_occ_worktype =\
 ut_max(@widget("syncgrid_worktype")->num_occurrences,\
 @widget("bgrid_worktype")->num_occurrences)
   JPLTEXT=
   JPLTEXT=        for week_num = 1 while week_num <=2
   JPLTEXT=        {
   JPLTEXT=
   JPLTEXT=          num_occ =\
 ut_max(@widget("sync:(week_num)")->num_occurrences,\
 @widget("b:(week_num)grid")->num_occurrences)
   JPLTEXT=
   JPLTEXT=          // Invalid cost components are not-fatal so
   JPLTEXT=          // perform validation on cost fields first so
   JPLTEXT=          // so that any subsequent failures will force
   JPLTEXT=          // error/warning messages to show as errors
   JPLTEXT=          // (meaning in red).
   JPLTEXT=
   JPLTEXT=          for i = 1 while i <= num_occ
   JPLTEXT=          {
   JPLTEXT=            // PR20050927/PR20060893
   JPLTEXT=            //   If hrscode[i] is leave type and\
 cfg$USE_DFLT_LV_ACCT is true THEN
   JPLTEXT=            //   skip validation call on cost components.
   JPLTEXT=            //   This is accomplished by setting an indicator
   JPLTEXT=            //   in the memo6 field which the valid_component
   JPLTEXT=            //   proc in ut_client.jpl uses to determine if
   JPLTEXT=            //   we are processing a default leave type.
   JPLTEXT=            //   Valid_component must be invoked so that the
   JPLTEXT=            //   data changed condition is recognized by the
   JPLTEXT=            //   call it performs to flag_changes.
   JPLTEXT=
   JPLTEXT=            if (cfg$COST_COMPONENTS_FOR_ENTRY ==0)
   JPLTEXT=            {
   JPLTEXT=              call sm_i_fval("w:(week_num)key", i)
   JPLTEXT=              call sm_i_fval("w:(week_num)cost", i)
   JPLTEXT=            }
   JPLTEXT=            else
   JPLTEXT=            {
   JPLTEXT=              for j = 1 while j <= 21
   JPLTEXT=              {
   JPLTEXT=                if (cfg$USE_DFLT_LV_ACCT &&\
 isDefaultLeaveHourCode(w:(week_num)hrcode[i]))
   JPLTEXT=                  @widget("w:(week_num)dyncost:j")[i] -> memo6 =\
 '9'
   JPLTEXT=                // Remove any single or double quotes that may be\
 present in any component field
   JPLTEXT=                w:(week_num)dyncost:j =\
 ut_replace_char(w:(week_num)dyncost:j,'"',' ')
   JPLTEXT=                w:(week_num)dyncost:j =\
 ut_replace_char(w:(week_num)dyncost:j,"'",' ')
   JPLTEXT=                w:(week_num)dyncost:j =\
 ut_replace_char(w:(week_num)dyncost:j,'"',' ')
   JPLTEXT=                w:(week_num)dyncost:j =\
 ut_replace_char(w:(week_num)dyncost:j,"'",' ')
   JPLTEXT=
   JPLTEXT=                // Avoid validating components that are\
 BOR-specific that can't be directly modified by the user
   JPLTEXT=                if (cfg$COST_COMPONENTS_COST_KEY_BOR && (j>=13) )
   JPLTEXT=                        next
   JPLTEXT=
   JPLTEXT=                call sm_i_fval("w:(week_num)dyncost:j",i)
   JPLTEXT=              }
   JPLTEXT=            }
   JPLTEXT=          }
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=
   JPLTEXT=        // Save error count to determine threshold to
   JPLTEXT=        // use to determine if validation_error flag
   JPLTEXT=        // should be set.  Component errors are not
   JPLTEXT=        // fatal so we need to skip over them in this
   JPLTEXT=        // determination.
   JPLTEXT=
   JPLTEXT=        if (cfg$DEVELOPMENT)
   JPLTEXT=        {
   JPLTEXT=          call ut_log_error("HOURLY TS SCREEN err_msg_idx\
 :err_msg_idx")
   JPLTEXT=          call ut_log_error("HOURLY TS SCREEN err_msg_dl\
 :err_msg_dl")
   JPLTEXT=          call ut_log_error("HOURLY TS SCREEN comp_error_cnt\
 :comp_error_cnt")
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        if (cfg$COST_COMPONENTS_FOR_ENTRY ==0)
   JPLTEXT=        {
   JPLTEXT=                // cost components not active so
   JPLTEXT=                // all messages are fatal errors
   JPLTEXT=                comp_error_cnt = 1
   JPLTEXT=        }
   JPLTEXT=        else
   JPLTEXT=        {
   JPLTEXT=                // cost components are active so
   JPLTEXT=                // skip over any message currently
   JPLTEXT=                // in the list.
   JPLTEXT=                comp_error_cnt = err_msg_idx
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        for i = 1 while i <= num_occ_worktype
   JPLTEXT=        {
   JPLTEXT=                // Remove minus sign from field value
   JPLTEXT=                regular_hours[i] = ut_replace_char\
 (regular_hours[i],"-"," ")
   JPLTEXT=                premium_hours[i] = ut_replace_char\
 (premium_hours[i],"-"," ")
   JPLTEXT=
   JPLTEXT=                // Remove leading and trailing spaces
   JPLTEXT=                regular_hours[i] = ut_squeeze (regular_hours[i],\
 ut$SQUEEZE_LEAD_TRAIL, "")
   JPLTEXT=                premium_hours[i] = ut_squeeze (premium_hours[i],\
 ut$SQUEEZE_LEAD_TRAIL, "")
   JPLTEXT=
   JPLTEXT=                call sm_i_fval("regular_hours",i)
   JPLTEXT=                call sm_i_fval("premium_hours",i)
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        for week_num = 1 while week_num<=2
   JPLTEXT=        {
   JPLTEXT=          num_occ =\
 ut_max(@widget("sync:(week_num)")->num_occurrences,\
 @widget("b:(week_num)grid")->num_occurrences)
   JPLTEXT=          for i = 1 while i <= num_occ
   JPLTEXT=          {
   JPLTEXT=                        t_buf = w:(week_num)key[i]
   JPLTEXT=                        // The following function is in\
 ut_client.jpl. The first parameter is used in the on screen message for the\
 field with the characters not allowed.
   JPLTEXT=                        // The second parameter is the value to\
 verify. It looks at each character in the string and if anything outside of\
 all number, letters and a subset
   JPLTEXT=                        // of special characters allowed:\
 !,@,#,$,%,^,&,*,(,),-,_,=,+,/, and the period.
   JPLTEXT=                        call ut_check_valid_characters('Key',\
 t_buf)
   JPLTEXT=                        fld_len = @length(msg_txt)
   JPLTEXT=
   JPLTEXT=                        if(fld_len > 1)
   JPLTEXT=                        {
   JPLTEXT=                                call ut5_msg_warning(msg_txt)
   JPLTEXT=
   JPLTEXT=                                // Go to the next row and don't\
 perform the operations further down below.
   JPLTEXT=                                // Especially because the query to\
 validate the Fav Key would fail if the
   JPLTEXT=                                // Fav Key contains either single\
 or double quotes.
   JPLTEXT=                                next
   JPLTEXT=                        }
   JPLTEXT=
   JPLTEXT=            if (cfg$COST_COMPONENTS_COST_KEY &&\
 cfg$COST_COMPONENTS_FOR_ENTRY && (cfg$COST_KEY_OVERRIDE != ""))
   JPLTEXT=            {
   JPLTEXT=                               \
 @widget("w:(week_num)key")->validation_func = "valid_key"
   JPLTEXT=                                rtv = sm_i_fval("w:(week_num)key",\
 i)
   JPLTEXT=
   JPLTEXT=              if rtv != 0
   JPLTEXT=                next
   JPLTEXT=            }
   JPLTEXT=
   JPLTEXT=            call sm_i_fval("w:(week_num)hrcode", i)
   JPLTEXT=            call sm_i_fval("w:(week_num)day_1", i)
   JPLTEXT=            call sm_i_fval("w:(week_num)day_2", i)
   JPLTEXT=            call sm_i_fval("w:(week_num)day_3", i)
   JPLTEXT=            call sm_i_fval("w:(week_num)day_4", i)
   JPLTEXT=            call sm_i_fval("w:(week_num)day_5", i)
   JPLTEXT=            call sm_i_fval("w:(week_num)day_6", i)
   JPLTEXT=            call sm_i_fval("w:(week_num)day_7", i)
   JPLTEXT=
   JPLTEXT=            if ( !cfg$MIS_TASK_CODE )
   JPLTEXT=            {
   JPLTEXT=                                call\
 sm_i_fval("w:(week_num)taskcode", i)
   JPLTEXT=            }
   JPLTEXT=
   JPLTEXT=            if ( cfg$ACCOUNT_TYPE == gbl$ACCOUNT_FPPS )
   JPLTEXT=            {
   JPLTEXT=                                call sm_i_fval("w:(week_num)shift",\
 i)
   JPLTEXT=                                call sm_i_fval("w:(week_num)base",\
 i)
   JPLTEXT=                                call\
 sm_i_fval("w:(week_num)envcode", i)
   JPLTEXT=                                call\
 sm_i_fval("w:(week_num)telework", i)
   JPLTEXT=                                call\
 sm_i_fval("w:(week_num)poscode", i)
   JPLTEXT=                                call\
 sm_i_fval("w:(week_num)mentoring", i)
   JPLTEXT=                                call sm_i_fval("w:(week_num)occ",\
 i)
   JPLTEXT=            }
   JPLTEXT=
   JPLTEXT=            // If the Favorite functionality is being overriden\
 skip the favorite validations and go to the next row
   JPLTEXT=
   JPLTEXT=            if ( (w:(week_num)key[i] != "") &&\
 (cfg$COST_KEY_OVERRIDE == w:(week_num)key[i]))
   JPLTEXT=            {
   JPLTEXT=              next
   JPLTEXT=            }
   JPLTEXT=
   JPLTEXT=            // Perform FBMS (USGS / FWS / NPS) validations on the\
 favorites if the active role is allowed to change the values of the cost\
 components.
   JPLTEXT=
   JPLTEXT=
   JPLTEXT=            if ((!amendment)&&(active_role |\
 cfg$COST_COMPONENTS_CHANGE_ROLE) && (cfg$COST_COMPONENTS_COST_KEY==1) &&\
 (w:(week_num)key[i] != ""))
   JPLTEXT=            {
   JPLTEXT=              if (cfg$DEVELOPMENT)
   JPLTEXT=                      call ut_log_error("ENHC0036192 starting block\
 of on-screen validations")
   JPLTEXT=
   JPLTEXT=              v_bor_costkey_exists = 0
   JPLTEXT=              if (cfg$COST_COMPONENTS_COST_KEY_BOR)
   JPLTEXT=              {
   JPLTEXT=                // Check if this is a BOR cost key associated with\
 the location of the employee
   JPLTEXT=                v_bor_costkey_exists = 1
   JPLTEXT=                sql_stmt = "select count(*) from borcostkey k,\
 employee e where k.costkey= :+(w:(week_num)key[i]) and\
 k.location=e.emplocation and e.empid=:+active_empid"
   JPLTEXT=                if (!ut_dbms_exists(sql_stmt))
   JPLTEXT=                  v_bor_costkey_exists = 0
   JPLTEXT=              }
   JPLTEXT=              if (cfg$DEVELOPMENT)
   JPLTEXT=                      call ut_log_error("ENHC0036192\
 v_bor_costkey_exists (:v_bor_costkey_exists) costkey = :+(w:(week_num)key[i])\
 ")
   JPLTEXT=
   JPLTEXT=              // Perform on-screen Fav Key validations only on\
 Favorites that have not already been saved to the database (that is, new\
 ones)
   JPLTEXT=              sql_stmt = "select count(*) from\
 cost_element_favorites where empid = :+active_empid and costkey =\
 :+(w:(week_num)key[i])"
   JPLTEXT=              if ( (v_bor_costkey_exists==0) &&\
 !ut_dbms_exists(sql_stmt))
   JPLTEXT=              {
   JPLTEXT=                // E-PAY-136 is exclusive to USGS
   JPLTEXT=                // E-PAY-145 is exclusive to FWS
   JPLTEXT=                // E-PAY-155 is exclusive to NPS
   JPLTEXT=                // E-PAY-174 is exclusive to BIA
   JPLTEXT=                // E-PAY-199 is exclusive to BOR
   JPLTEXT=                // In no instance should more than 1 of the edit\
 checks listed above be enabled at a given time.
   JPLTEXT=                msgstring = ut_check_core_req_usgs\
 ('E-PAY-136',week_num,i,future)
   JPLTEXT=                msgstring = msgstring ##\
 ut_check_core_req_fws('E-PAY-145',week_num,i,future)
   JPLTEXT=
   JPLTEXT=                if (msgstring != "")
   JPLTEXT=                        call ut5_msg_warning("Fav Key " ##\
 w:(week_num)key[i] ## " " ## msgstring)
   JPLTEXT=
   JPLTEXT=                msgstring = ut_check_fav_cost_comps\
 ('E-PAY-135',week_num,i,future)
   JPLTEXT=
   JPLTEXT=                if (msgstring != "")
   JPLTEXT=                        call ut5_msg_warning("Fav Key " ##\
 w:(week_num)key[i] ## " " ## msgstring)
   JPLTEXT=
   JPLTEXT=                msgstring = ut_check_core_req_nps\
 ('E-PAY-155',week_num,i,future)
   JPLTEXT=
   JPLTEXT=                if msgstring != ""
   JPLTEXT=                        call ut5_msg_warning("Fav Key " ##\
 w:(week_num)key[i] ## " " ## msgstring)
   JPLTEXT=
   JPLTEXT=                msgstring = ut_check_core_req_bia\
 ('E-PAY-174',week_num,i,future)
   JPLTEXT=
   JPLTEXT=                if (msgstring != "")
   JPLTEXT=                {
   JPLTEXT=                        call ut5_msg_warning("Fav Key " ##\
 w:(week_num)key[i] ## " " ## msgstring)
   JPLTEXT=                }
   JPLTEXT=
   JPLTEXT=                msgstring = ut_check_core_req_bor\
 ('E-PAY-199',week_num,i,future)
   JPLTEXT=                if (cfg$DEVELOPMENT)
   JPLTEXT=                      call ut_log_error("ENHC0036192\
 ut_check_core_req_bor (E-PAY-199,:week_num,:i,:future) returned (" ##\
 msgstring ## ")")
   JPLTEXT=
   JPLTEXT=                if (msgstring != "")
   JPLTEXT=                {
   JPLTEXT=                        call ut5_msg_warning("Fav Key " ##\
 w:(week_num)key[i] ## " " ## msgstring)
   JPLTEXT=                }
   JPLTEXT=
   JPLTEXT=                // E-PAY-106 is enabled on multiple DOI instances\
 including BIA, SOS, OIG, and OST
   JPLTEXT=
   JPLTEXT=                msgstring = ut_check_fav_fbms_valrange\
 ('E-PAY-106',week_num,i,future)
   JPLTEXT=
   JPLTEXT=                if (msgstring != "")
   JPLTEXT=                        call ut5_msg_warning("Fav Key " ##\
 w:(week_num)key[i] ## " " ## msgstring)
   JPLTEXT=
   JPLTEXT=              }
   JPLTEXT=            }
   JPLTEXT=          }
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        if ( err_msg_idx > comp_error_cnt )
   JPLTEXT=        {
   JPLTEXT=                retval = 1
   JPLTEXT=                validation_error = gbl$TRUE
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        if ( !retval )
   JPLTEXT=        {
   JPLTEXT=                for week_num = 1 while week_num<=2
   JPLTEXT=                {
   JPLTEXT=                        for i = 1 while i <= 7
   JPLTEXT=                        {
   JPLTEXT=                                call daily_total(week_num,i)
   JPLTEXT=                                if (data_changed_message)
   JPLTEXT=                                {
   JPLTEXT=                                        retval = 1
   JPLTEXT=                                }
   JPLTEXT=                        }
   JPLTEXT=                }
   JPLTEXT=                //Get weekly totals.
   JPLTEXT=
   JPLTEXT=                call get_week_totals()
   JPLTEXT=
   JPLTEXT=                //Save before image
   JPLTEXT=                call buffer_2_week()
   JPLTEXT=        }
   JPLTEXT=        scr_validation = 0
   JPLTEXT=        if (cfg$DEVELOPMENT)
   JPLTEXT=          call ut_log_error("HOURLY TS SCREEN do_valid_screen\
 returns :retval validation_error is :validation_error")
   JPLTEXT=        return (retval)
   JPLTEXT=} //end proc do_valid_screen()
   JPLTEXT=
