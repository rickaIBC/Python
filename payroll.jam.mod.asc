 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine: find_time
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine finds the first day with time in the\
 changed row.
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2016-06-20      E. Ramos        CR20160062
   JPLTEXT=//                      Show week 1 and week 2 on the same screen.
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=proc find_time (week_num,fld_occ)
   JPLTEXT={
   JPLTEXT=        vars retval(10) = 0, i(10)
   JPLTEXT=
   JPLTEXT=        // Find first row with time in the worktype distribution\
 grid
   JPLTEXT=        if ((cfg$WORKTYPE_EMPORGCODES != "NONE") && (week_num==""))
   JPLTEXT=        {
   JPLTEXT=
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        //Find the first column with time.
   JPLTEXT=
   JPLTEXT=        for i = 1 while i <= 7
   JPLTEXT=        {
   JPLTEXT=                if ( @widget("w:(week_num)day_:i")[fld_occ] != "" )
   JPLTEXT=                {
   JPLTEXT=                        retval = i
   JPLTEXT=                        break
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=        retval = ((week_num - 1) * 7) + retval
   JPLTEXT=        return (retval)
   JPLTEXT=} //end proc find_time()
   JPLTEXT=
