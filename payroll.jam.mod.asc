 Timesheet Comments to this screen.
   JPLTEXT=//      2024-09-09      A. McCormick    ENHC0037161
   JPLTEXT=//                      Clear the default_preprint widget when\
 needed. To prevent a false positive on the Default Timesheet tests.
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=proc do_select ()
   JPLTEXT={
   JPLTEXT=        vars retval(10) = 1, err_msg(80), i(10), j(10) fld_val(30)
   JPLTEXT=        vars week_num
   JPLTEXT=        vars v_ppyear v_pp v_process_ppyear v_process_pp\
 v_process_seq
   JPLTEXT=
   JPLTEXT=        // Retrieve default component values for the employee
   JPLTEXT=        if (cfg$websphere)
   JPLTEXT=        {
   JPLTEXT=                obj_retval = sm_obj_call\
 (default_time_hndl,"get_core_defaults_components",active_empid,\\
   JPLTEXT=               \
 ddyncost1,ddyncost2,ddyncost3,ddyncost4,ddyncost5,ddyncost6,ddyncost7,ddyncos\
t8,ddyncost9,\\
   JPLTEXT=               \
 ddyncost10,ddyncost11,ddyncost12,ddyncost13,ddyncost14,ddyncost15,ddyncost16,\
ddyncost17,\\
   JPLTEXT=                ddyncost18,ddyncost19,ddyncost20,ddyncost21)
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        // Copy values from widgets to array so that they can be\
 displayed using the core_components.htm template
   JPLTEXT=        for i=1 while i<=21
   JPLTEXT=        {
   JPLTEXT=                ddyncost[i] = ddyncost:i
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        if (!amendment)
   JPLTEXT=        {
   JPLTEXT=                if (cfg$websphere)
   JPLTEXT=                {
   JPLTEXT=                        if(default_preprint >= 2)
   JPLTEXT=                        {
   JPLTEXT=                                obj_retval = \\
   JPLTEXT=                                sm_obj_call (payroll_hndl,\
 "get_default_preprint", active_empid, pp, ppyear, empschoption,\
 :tna_data_name, \\
   JPLTEXT=                                status_array,  w1hrcode, w1day_1,\
 w1day_2, w1day_3, w1day_4, w1day_5, w1day_6, \\
   JPLTEXT=                                w1day_7, w1key, w1cost, w1shift,\
 w1envcode, w1telework, w1base, w1occ, w1poscode, w1mentoring, w1taskcode,\
 w1pp, \\
   JPLTEXT=                                w1hrdesc, w1costdesc, w2hrcode,\
 w2day_1, w2day_2, w2day_3, w2day_4, w2day_5, w2day_6, \\
   JPLTEXT=                                w2day_7, w2key, w2cost, w2shift,\
 w2envcode, w2telework, w2base, w2occ, w2poscode, w2mentoring, w2taskcode,\
 w2pp, \\
   JPLTEXT=                                w2hrdesc, w2costdesc, tot_array,\
 cur_status_idx, status_dtg, cur_status_dtg, \\
   JPLTEXT=                                al_check_advance, save_flag,\
 err_msg_idx, err_msg_dl, UsrMadeChg, \\
   JPLTEXT=                               \
 w1dyncost1,w1dyncost2,w1dyncost3,w1dyncost4,w1dyncost5,w1dyncost6,\\
   JPLTEXT=                               \
 w1dyncost7,w1dyncost8,w1dyncost9,w1dyncost10,w1dyncost11, \\
   JPLTEXT=                               \
 w1dyncost12,w1dyncost13,w1dyncost14,w1dyncost15,w1dyncost16,w1dyncost17,\\
   JPLTEXT=                               \
 w1dyncost18,w1dyncost19,w1dyncost20,w1dyncost21, \\
   JPLTEXT=                               \
 w2dyncost1,w2dyncost2,w2dyncost3,w2dyncost4,w2dyncost5,w2dyncost6,\\
   JPLTEXT=                               \
 w2dyncost7,w2dyncost8,w2dyncost9,w2dyncost10,w2dyncost11, \\
   JPLTEXT=                               \
 w2dyncost12,w2dyncost13,w2dyncost14,w2dyncost15,w2dyncost16,w2dyncost17,\\
   JPLTEXT=                               \
 w2dyncost18,w2dyncost19,w2dyncost20,w2dyncost21)
   JPLTEXT=
   JPLTEXT=                                UsrMadeChg = 0
   JPLTEXT=                        }
   JPLTEXT=                        else
   JPLTEXT=                        {
   JPLTEXT=                                default_preprint = gbl$FALSE
   JPLTEXT=                                obj_retval = \\
   JPLTEXT=                                sm_obj_call (payroll_hndl,\
 "get_payroll", active_empid, pp, ppyear, empschoption, :tna_data_name, \\
   JPLTEXT=                                status_array,  w1hrcode, w1day_1,\
 w1day_2, w1day_3, w1day_4, w1day_5, w1day_6, \\
   JPLTEXT=                                w1day_7, w1key, w1cost, w1shift,\
 w1envcode, w1telework, w1base, w1occ, w1poscode, w1mentoring, w1taskcode,\
 w1pp, \\
   JPLTEXT=                                w1hrdesc, w1costdesc, w2hrcode,\
 w2day_1, w2day_2, w2day_3, w2day_4, w2day_5, w2day_6, \\
   JPLTEXT=                                w2day_7, w2key, w2cost, w2shift,\
 w2envcode, w2telework, w2base, w2occ, w2poscode, w2mentoring, w2taskcode,\
 w2pp, \\
   JPLTEXT=                                w2hrdesc, w2costdesc, tot_array,\
 cur_status_idx, status_dtg, cur_status_dtg, \\
   JPLTEXT=                                al_check_advance, save_flag,\
 err_msg_idx, err_msg_dl, UsrMadeChg, \\
   JPLTEXT=                               \
 w1dyncost1,w1dyncost2,w1dyncost3,w1dyncost4,w1dyncost5,w1dyncost6,\\
   JPLTEXT=                               \
 w1dyncost7,w1dyncost8,w1dyncost9,w1dyncost10,w1dyncost11, \\
   JPLTEXT=                               \
 w1dyncost12,w1dyncost13,w1dyncost14,w1dyncost15,w1dyncost16,w1dyncost17,\\
   JPLTEXT=                               \
 w1dyncost18,w1dyncost19,w1dyncost20,w1dyncost21, \\
   JPLTEXT=                               \
 w2dyncost1,w2dyncost2,w2dyncost3,w2dyncost4,w2dyncost5,w2dyncost6,\\
   JPLTEXT=                               \
 w2dyncost7,w2dyncost8,w2dyncost9,w2dyncost10,w2dyncost11, \\
   JPLTEXT=                               \
 w2dyncost12,w2dyncost13,w2dyncost14,w2dyncost15,w2dyncost16,w2dyncost17,\\
   JPLTEXT=                               \
 w2dyncost18,w2dyncost19,w2dyncost20,w2dyncost21,\\
   JPLTEXT=                                worktype_code,\
 worktype_description, regular_hours, premium_hours, \\
   JPLTEXT=                                flag_alc_annual, flag_alc_sick,\
 alc_hrs_annual, alc_hrs_sick )
   JPLTEXT=                        }
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=        else
   JPLTEXT=        {
   JPLTEXT=                if (cfg$websphere)
   JPLTEXT=                {
   JPLTEXT=                        obj_retval = \\
   JPLTEXT=                                sm_obj_call (amend_payroll_hndl,\
 "s_a_payroll", pp, ppyear, prior_tna_data, status_array, \\
   JPLTEXT=                                w1hrcode, w1day_1, w1day_2,\
 w1day_3, w1day_4, w1day_5, w1day_6, w1day_7, \\
   JPLTEXT=                                w1key, w1cost, w1shift, w1envcode,\
 w1telework, w1base, w1occ, w1poscode, w1mentoring, w1taskcode, w1pp,\
 w1hrdesc, \\
   JPLTEXT=                                w1costdesc, w2hrcode, w2day_1,\
 w2day_2, w2day_3, w2day_4, w2day_5, w2day_6, w2day_7, w2key, \\
   JPLTEXT=                                w2cost, w2shift, w2envcode,\
 w2telework, w2base, w2occ, w2poscode, w2mentoring, w2taskcode, w2pp,\
 w2hrdesc, \\
   JPLTEXT=                                w2costdesc, tot_array,\
 cur_status_idx, status_dtg, al_check_advance, save_flag, \\
   JPLTEXT=                                err_msg_idx, err_msg_dl, \\
   JPLTEXT=                               \
 w1dyncost1,w1dyncost2,w1dyncost3,w1dyncost4,w1dyncost5,w1dyncost6,\\
   JPLTEXT=                               \
 w1dyncost7,w1dyncost8,w1dyncost9,w1dyncost10,w1dyncost11, \\
   JPLTEXT=                               \
 w1dyncost12,w1dyncost13,w1dyncost14,w1dyncost15,w1dyncost16,w1dyncost17,\\
   JPLTEXT=                               \
 w1dyncost18,w1dyncost19,w1dyncost20,w1dyncost21, \\
   JPLTEXT=                               \
 w2dyncost1,w2dyncost2,w2dyncost3,w2dyncost4,w2dyncost5,w2dyncost6,\\
   JPLTEXT=                               \
 w2dyncost7,w2dyncost8,w2dyncost9,w2dyncost10,w2dyncost11, \\
   JPLTEXT=                               \
 w2dyncost12,w2dyncost13,w2dyncost14,w2dyncost15,w2dyncost16,w2dyncost17,\\
   JPLTEXT=                               \
 w2dyncost18,w2dyncost19,w2dyncost20,w2dyncost21,w1amend_hrsprim_rownum, \\
   JPLTEXT=                               \
 w1hrsprim_rownum,w2amend_hrsprim_rownum,w2hrsprim_rownum,             \\
   JPLTEXT=                               \
 w1empbureaucode,w2empbureaucode,w1empsbureaucode,w2empsbureaucode)
   JPLTEXT=
   JPLTEXT=                                UsrMadeChg = 0
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=        retval = ut_service_check()
   JPLTEXT=
   JPLTEXT=        orig_status = status_array[14]
   JPLTEXT=
   JPLTEXT=        force_save_flag = save_flag
   JPLTEXT=        del_row_flag = gbl$FALSE
   JPLTEXT=
   JPLTEXT= // PR20070812 - Set force_save_flag to true if ppyear is zero
   JPLTEXT= //              which indicates we are completing either
   JPLTEXT= //              load_default or load_pre processing.
   JPLTEXT=
   JPLTEXT=        if ppyear == 0
   JPLTEXT=                force_save_flag = gbl$TRUE
   JPLTEXT=
   JPLTEXT=        // Clear the cost key fields if editing an amendment and\
 COST_COMPONENTS_COST_KEY_AMEND==0
   JPLTEXT=
   JPLTEXT=        if ( (cfg$COST_COMPONENTS_FOR_ENTRY &&\
 cfg$COST_COMPONENTS_COST_KEY) && (cfg$COST_COMPONENTS_COST_KEY_AMEND==0 &&\
 amendment))
   JPLTEXT=        {
   JPLTEXT=                if (cfg$DEVELOPMENT)
   JPLTEXT=                  call ut_log_error("enhc0036178/dfct0012329\
 do_select@payroll.jam clearing cost key fields")
   JPLTEXT=                for week_num=1 while week_num<=2
   JPLTEXT=                {
   JPLTEXT=                        call sm_n_1clear_array("w:(week_num)key")
   JPLTEXT=                        call sm_n_1clear_array("bw:(week_num)key")
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=        else
   JPLTEXT=        {
   JPLTEXT=                if (cfg$DEVELOPMENT)
   JPLTEXT=                  call ut_log_error("enhc0036178/dfct0012329\
 do_select@payroll.jam NOT clearing cost key fields")
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=
   JPLTEXT=        // Clear hidden fields on the right side of the timesheets
   JPLTEXT=        call clear_hidden_fields()
   JPLTEXT=
   JPLTEXT=        //Initialize buffers.
   JPLTEXT=
   JPLTEXT=        for i = 1 while i <= w1hrcode->num_occurrences
   JPLTEXT=        {
   JPLTEXT=                w1freeze[i] = ""
   JPLTEXT=                for j = 1 while (gbl$USE_DEFAULT_LEAVE[j] != "")
   JPLTEXT=                {
   JPLTEXT=                        if (w1hrcode[i] ==\
 gbl$USE_DEFAULT_LEAVE[j]) &&    \\
   JPLTEXT=                           ( (status_array[14] == "UNV") ||\
 (status_array[14] == "VER") || \\
   JPLTEXT=                             (status_array[14] == "VAL") )
   JPLTEXT=                        {
   JPLTEXT=                                if (cfg$COST_COMPONENTS_FOR_ENTRY\
 == 0)
   JPLTEXT=                                {
   JPLTEXT=                                        // Retrieve account\
 description that corresponds to this row
   JPLTEXT=                                        w1costdesc[i] =\
 getDefaultLeaveAccountDesc(w1hrcode[i])
   JPLTEXT=                                }
   JPLTEXT=
   JPLTEXT=                                w1freeze[i] = "Y"
   JPLTEXT=                        }
   JPLTEXT=                        else
   JPLTEXT=                        {
   JPLTEXT=                                if (cfg$COST_COMPONENTS_FOR_ENTRY\
 == 1)
   JPLTEXT=                                        w1costdesc[i] = ""
   JPLTEXT=                        }
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        for i = 1 while i <= w2hrcode->num_occurrences
   JPLTEXT=        {
   JPLTEXT=                w2freeze[i] = ""
   JPLTEXT=                for j = 1 while (gbl$USE_DEFAULT_LEAVE[j] != "")
   JPLTEXT=                {
   JPLTEXT=                        if (w2hrcode[i] ==\
 gbl$USE_DEFAULT_LEAVE[j]) &&    \\
   JPLTEXT=                           ( (status_array[14] == "UNV") ||\
 (status_array[14] == "VER") || \\
   JPLTEXT=                             (status_array[14] == "VAL") )
   JPLTEXT=                        {
   JPLTEXT=                                if (cfg$COST_COMPONENTS_FOR_ENTRY\
 == 0)
   JPLTEXT=
   JPLTEXT=                                        w2costdesc[i] =\
 getDefaultLeaveAccountDesc(w1hrcode[i])
   JPLTEXT=
   JPLTEXT=                                w2freeze[i] = "Y"
   JPLTEXT=                        }
   JPLTEXT=                        else
   JPLTEXT=                        {
   JPLTEXT=                                if (cfg$COST_COMPONENTS_FOR_ENTRY\
 == 1)
   JPLTEXT=                                        w2costdesc[i] = ""
   JPLTEXT=                        }
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        call set_NumOcc()
   JPLTEXT=
   JPLTEXT=        call get_daily_totals()
   JPLTEXT=        //call get_week_totals()
   JPLTEXT=
   JPLTEXT=        call ut_get_emphomecan()
   JPLTEXT=
   JPLTEXT=        //Initialize flags.
   JPLTEXT=
   JPLTEXT=        if ( sm_is_bundle("defschd") )
   JPLTEXT=        {
   JPLTEXT=                receive bundle "defschd" data fld_val
   JPLTEXT=                save_flag = gbl$TRUE
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        if !(default_preprint)
   JPLTEXT=                status_flag = gbl$FALSE
   JPLTEXT=
   JPLTEXT=        //Display warning.
   JPLTEXT=
   JPLTEXT=        if ( retval )
   JPLTEXT=        {
   JPLTEXT=                if ( pp == gbl$SCHD_DFLT )
   JPLTEXT=                {
   JPLTEXT=                        err_msg = "Default Timesheet data"
   JPLTEXT=                }
   JPLTEXT=                else if ( pp == gbl$SCHD_PRE )
   JPLTEXT=                {
   JPLTEXT=                        err_msg = "Pre-print Timesheet data"
   JPLTEXT=                }
   JPLTEXT=                else
   JPLTEXT=                {
   JPLTEXT=                        err_msg = "Hourly Timesheet (:pp - :ppyear"\
 ## ") data"
   JPLTEXT=                }
   JPLTEXT=                call ut5_msg_information(":err_msg does not exist"\
 ## \\
   JPLTEXT=                        " for employee: ':active_empname'" ## ".")
   JPLTEXT=                err_msg_idx = err_msg_idx + 1
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        if(pp != gbl$SCHD_DFLT && \\
   JPLTEXT=           pp != gbl$SCHD_PRE && \\
   JPLTEXT=           default_preprint == 0)
   JPLTEXT=        {
   JPLTEXT=                call select_leavebal()
   JPLTEXT=                call select_comments()
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        // Advance Leave Check process call relocated
   JPLTEXT=        // to ut_alc_message in ut_client.jpl
   JPLTEXT=        // Now called from web_exit()
   JPLTEXT=
   JPLTEXT=        // Prepare for calling get_dyncostdescs
   JPLTEXT=        v_ppyear = ""
   JPLTEXT=        v_pp = ""
   JPLTEXT=        v_process_ppyear = ""
   JPLTEXT=        v_process_pp = ""
   JPLTEXT=        v_process_seq = ""
   JPLTEXT=
   JPLTEXT=        if(amendment)
   JPLTEXT=        {
   JPLTEXT=                v_pp = ":prior_tna_data[idx$PAY_CORR_PP]"
   JPLTEXT=                v_ppyear = ":prior_tna_data[idx$PAY_CORR_YEAR]"
   JPLTEXT=                v_process_ppyear = ppyear
   JPLTEXT=                v_process_pp = pp
   JPLTEXT=                v_process_seq = ":prior_tna_data[idx$PAY_CURR_SEQ]"
   JPLTEXT=        }
   JPLTEXT=        else
   JPLTEXT=        {
   JPLTEXT=                v_pp = pp
   JPLTEXT=                v_ppyear = ppyear
   JPLTEXT=                v_process_ppyear = ""
   JPLTEXT=                v_process_pp = ""
   JPLTEXT=                v_process_seq = ""
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        if((w1hrcode->num_occurrences == 0) && \\
   JPLTEXT=            (w1day_1->num_occurrences == 0) && \\
   JPLTEXT=            (w1day_2->num_occurrences == 0) && \\
   JPLTEXT=            (w1day_3->num_occurrences == 0) && \\
   JPLTEXT=            (w1day_4->num_occurrences == 0) && \\
   JPLTEXT=            (w1day_5->num_occurrences == 0) && \\
   JPLTEXT=            (w1day_6->num_occurrences == 0) && \\
   JPLTEXT=            (w1day_7->num_occurrences == 0) && \\
   JPLTEXT=           (w2hrcode->num_occurrences == 0) && \\
   JPLTEXT=            (w2day_1->num_occurrences == 0) && \\
   JPLTEXT=            (w2day_2->num_occurrences == 0) && \\
   JPLTEXT=            (w2day_3->num_occurrences == 0) && \\
   JPLTEXT=            (w2day_4->num_occurrences == 0) && \\
   JPLTEXT=            (w2day_5->num_occurrences == 0) && \\
   JPLTEXT=            (w2day_6->num_occurrences == 0) && \\
   JPLTEXT=            (w2day_7->num_occurrences == 0) && \\
   JPLTEXT=           (UsrMadeChg == gbl$TRUE))
   JPLTEXT=                UsrMadeChg = gbl$FALSE
   JPLTEXT=
   JPLTEXT=        // Retrieve component descriptions
   JPLTEXT=        call\
 get_dyncostdescs(v_ppyear,v_pp,v_process_ppyear,v_process_pp,v_process_seq)
   JPLTEXT=
   JPLTEXT=        // Save before image
   JPLTEXT=        call buffer_2_week()
   JPLTEXT=
   JPLTEXT=        // Move the component descriptions into the gird.
   JPLTEXT=        call set_comp_desc()
   JPLTEXT=
   JPLTEXT=        return (0)
   JPLTEXT=} //end proc do_select()
   JPLTEXT=
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine:   update_time
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine updates payroll data (minus status).
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2000-09-10      D. Bodlien / E. Ren     CR20000398
   JPLTEXT=//                      Add processing for amendments.
   JPLTEXT=//      2004-04-15      T. Peace        PR20030860
   JPLTEXT=//                      Include tna_data bundle on update_time\
 method
   JPLTEXT=//                      call to allow status concurrency checking
   JPLTEXT=//                      process (ut_chk_concurrency) to determine
   JPLTEXT=//                      pay periods properly.
   JPLTEXT=//      2005-05-31      E. Ramos        CR20050039
   JPLTEXT=//                      Add code to save values of the cost\
 structure dynamic components.
   JPLTEXT=//      2005-11-17      T. Peace        PR20051023
   JPLTEXT=//                      Add filter to log message if
   JPLTEXT=//                      attempting to save an unusually large
   JPLTEXT=//                      number of rows on a timesheet.
   JPLTEXT=//      2006-04-20      T. Peace        PR20060258
   JPLTEXT=//                      Combine payroll time update and any
   JPLTEXT=//                      resulting status change into a
   JPLTEXT=//                      single service_call in order to perform
   JPLTEXT=//                      all updates within the a single
   JPLTEXT=//                      database transaction.
   JPLTEXT=//                      Added additional widgets to screen
   JPLTEXT=//                      and update_time service call as
   JPLTEXT=//                      required to allow edit_checking and
   JPLTEXT=//                      status updates to be performed
   JPLTEXT=//                      while time sheet is being updated.
   JPLTEXT=//      2006-06-20      E. Ramos        CR20060085
   JPLTEXT=//                      Add code to support hidden components that\
 are substrings of visible components.
   JPLTEXT=//      2006-06-14      T. Peace        PR20060357
   JPLTEXT=//                      Save the original status after the database
   JPLTEXT=//                      update is performed.  Also clear indicator\
 that
   JPLTEXT=//                      a data change was made in the non-displayed\
 week.
   JPLTEXT=//      2006-07-12      T. Peace        PR20060465
   JPLTEXT=//                      Relocated "Payroll Data Saved" message from
   JPLTEXT=//                      proc update_payroll to proc update_time.
   JPLTEXT=//                      Save original status value.
   JPLTEXT=//      2006-07-19      T. Peace        PR20060487
   JPLTEXT=//                      Ensure updat_time service call is executed
   JPLTEXT=//                      if either edit check required or status
   JPLTEXT=//                      update is requested even though there may
   JPLTEXT=//                      be no time change on timesheet.
   JPLTEXT=//      2006-08-01      T. Peace        PR20060487 - rework
   JPLTEXT=//                      Clear save_flag and edit_reqd flags before
   JPLTEXT=//                      initiating save processing when exiting\
 timesheet
   JPLTEXT=//                      via one of the payroll screen tabs.
   JPLTEXT=//      2006-10-16      E. Ramos        PR20060665
   JPLTEXT=//                      Added call to retrieve component\
 descriptions
   JPLTEXT=//      2006-10-08      E. Ramos        CR20060209
   JPLTEXT=//                      Added logic to enable lookup of the costkey\
 field with components being enabled.
   JPLTEXT=//      2007-03-01      T. Peace        CR20060273
   JPLTEXT=//                      Add widgets and processing for new telework
   JPLTEXT=//                      field on timesheets.
   JPLTEXT=//      2007-10-02      T. Peace        PR20070477
   JPLTEXT=//                      Combine amendment time updates and any\
 status changes
   JPLTEXT=//                      in a single service_call & database\
 transaction.
   JPLTEXT=//      2008-01-28      T. Peace        PR20080001
   JPLTEXT=//                      Prevent updates to timesheets already
   JPLTEXT=//                      transmitted to FPPS.
   JPLTEXT=//      2008-04-24      T. Peace        PR20080212
   JPLTEXT=//                      Reset del_row_flag after timesheet update.
   JPLTEXT=//      2008-07-10      A. McCormick    PR20080460
   JPLTEXT=//                      Check the display_save_msg before\
 displaying Payroll Saved message.
   JPLTEXT=//      2009-02-26      T. Peace        PR20080259
   JPLTEXT=//                      Ensure status array and orig_status fields\
 are set to UNV
   JPLTEXT=//                      when edit error is encountered.
   JPLTEXT=//      2009-07-02      E. Ramos        CR20010062
   JPLTEXT=//                      Support timesheets entered for pay periods\
 in the future.
   JPLTEXT=//      2009-11-06      T. Peace        PR20090483
   JPLTEXT=//                      Provide user notification when time change
   JPLTEXT=//                      and status change save operations fail in
   JPLTEXT=//                      during the EJB execution.
   JPLTEXT=//      2009-12-10      T. Peace        PR20090612
   JPLTEXT=//                      Force edit checking when status change\
 detected.
   JPLTEXT=//                      Reset status_flag after timesheet\
 retrieval.
   JPLTEXT=//      2010-07-08      T. Peace        PR20100284
   JPLTEXT=//                      Force the status to reset to UNV
   JPLTEXT=//                      on any save operation after a delete
   JPLTEXT=//                      row or duplicate row operation.
   JPLTEXT=//      2011-05-13      A. McCormick    PR20110185 - Rework
   JPLTEXT=//                      Force save, downgrade and new edits if\
 saving an elevated
   JPLTEXT=//                      timesheet after saving a default /\
 preprint.
   JPLTEXT=//      2015-02-25      E. Ramos        CR20140260
   JPLTEXT=//                      Added mentoring column.
   JPLTEXT=//      2016-02-03      E. Ramos        CR20150129
   JPLTEXT=//                      Added widgets to introduce work type data\
 as part of the regular timesheets.
   JPLTEXT=//      2016-06-20      E. Ramos        CR20160062
   JPLTEXT=//                      Show week 1 and week 2 on the same screen.
   JPLTEXT=//      2016-07-19      E. Ramos        PR20160268
   JPLTEXT=//                      Clear add_del_flag and del_row_flag when\
 save is successful.
   JPLTEXT=//      2024-08-15      A. McCormick    ENHC0037149
   JPLTEXT=//                      Removed non-WebSphere code.
   JPLTEXT=//      2025-04-15      T. Peace        DFCT0014160
   JPLTEXT=//                      Ensure timesheet comments and leavbal info\
 are
   JPLTEXT=//                      reloaded after a time update is performed.
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=proc update_time ()
   JPLTEXT={
   JPLTEXT=        vars retval(10),num_occ(10)
   JPLTEXT=        vars w1_row_cnt(10)
   JPLTEXT=        vars w2_row_cnt(10)
   JPLTEXT=        vars temp_save_flag(10)
   JPLTEXT=        temp_save_flag = save_flag
   JPLTEXT=        vars timestamp_format(20)
   JPLTEXT=
   JPLTEXT=        if ( !security_flag ) && ( !save_flag )  && (!status_flag)\
 && (!edit_reqd)
   JPLTEXT=        {
   JPLTEXT=                if ( status_flag )
   JPLTEXT=                {
   JPLTEXT=                        unauthorized_action = gbl$TRUE
   JPLTEXT=                }
   JPLTEXT=                return (0)
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        if(ppyear != 0 && (DefaultSaved == gbl$TRUE ||\
 PrePrintSaved == gbl$TRUE))
   JPLTEXT=        {
   JPLTEXT=                UsrMadeChg = gbl$TRUE
   JPLTEXT=                status_flag = gbl$TRUE
   JPLTEXT=                edit_reqd = gbl$TRUE
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        // Prepare component values to be saved (i.e. components\
 with formatting = 10, which means they are substrings of other components)
   JPLTEXT=        call dyncost_prepare_save()
   JPLTEXT=
   JPLTEXT=        if (!amendment)
   JPLTEXT=        {
   JPLTEXT=               w1_row_cnt = @widget("w1hrcode") -> num_occurrences
   JPLTEXT=               w2_row_cnt = @widget("w2hrcode") -> num_occurrences
   JPLTEXT=
   JPLTEXT=               if (w1_row_cnt > 100) || (w2_row_cnt > 100 )
   JPLTEXT=               {
   JPLTEXT=                   // empty rows found log empid,pp-yr,counts
   JPLTEXT=                        call ut_log_error("PAYROLL-ROWS>100 for\
 :+active_empid in :ppyear - :pp wk1==> :w1_row_cnt, wk2==> :w2_row_cnt ")
   JPLTEXT=                        call ut_log_error("PAYROLL- wk1==>\
 :w1hrcode  wk2==> :w2hrcode ")
   JPLTEXT=               }
   JPLTEXT=               if (del_row_flag + add_del_flag) > 0
   JPLTEXT=                       status_flag = gbl$TRUE
   JPLTEXT=
   JPLTEXT=               if status_flag && status_array[14] != "UNV"
   JPLTEXT=                       edit_reqd = gbl$TRUE
   JPLTEXT=
   JPLTEXT=                if (cfg$DEVELOPMENT)
   JPLTEXT=                        call ut_log_error("cr20140260 payroll.jam\
 update_time UsrMadeChg :UsrMadeChg status_flag :status_flag edit_reqd\
 :edit_reqd")
   JPLTEXT=
   JPLTEXT=                if (cfg$websphere)
   JPLTEXT=                {
   JPLTEXT=                        if future
   JPLTEXT=                        {
   JPLTEXT=                                obj_retval = \\
   JPLTEXT=                                sm_obj_call (payroll_hndl,\
 "update_time", active_empid, pp, ppyear, active_role, \\
   JPLTEXT=                                w1hrcode, w1day_1, w1day_2,\
 w1day_3, w1day_4, w1day_5, w1day_6, w1day_7, \\
   JPLTEXT=                                w1key, w1cost, w1shift, w1envcode,\
 w1telework, w1base, w1occ, w1poscode, w1mentoring,  w1taskcode, w1pp,\
 w1hrdesc, \\
   JPLTEXT=                                w1costdesc, w2hrcode, w2day_1,\
 w2day_2, w2day_3, w2day_4, w2day_5, w2day_6, w2day_7, w2key, \\
   JPLTEXT=                                w2cost, w2shift, w2envcode,\
 w2telework, w2base, w2occ, w2poscode, w2mentoring, w2taskcode, w2pp,\
 w2hrdesc, \\
   JPLTEXT=                                w2costdesc, mgr_id, login_id,\
 status_dtg, cur_status_dtg, status_array, future_tna_data, \\
   JPLTEXT=                               \
 w1dyncost1,w1dyncost2,w1dyncost3,w1dyncost4,w1dyncost5,w1dyncost6,\\
   JPLTEXT=                               \
 w1dyncost7,w1dyncost8,w1dyncost9,w1dyncost10,w1dyncost11, \\
   JPLTEXT=                               \
 w1dyncost12,w1dyncost13,w1dyncost14,w1dyncost15,w1dyncost16,w1dyncost17,\\
   JPLTEXT=                               \
 w1dyncost18,w1dyncost19,w1dyncost20,w1dyncost21, \\
   JPLTEXT=                               \
 w2dyncost1,w2dyncost2,w2dyncost3,w2dyncost4,w2dyncost5,w2dyncost6,\\
   JPLTEXT=                               \
 w2dyncost7,w2dyncost8,w2dyncost9,w2dyncost10,w2dyncost11, \\
   JPLTEXT=                               \
 w2dyncost12,w2dyncost13,w2dyncost14,w2dyncost15,w2dyncost16,w2dyncost17,\\
   JPLTEXT=                               \
 w2dyncost18,w2dyncost19,w2dyncost20,w2dyncost21,\\
   JPLTEXT=                                worktype_code,\
 worktype_description, regular_hours, premium_hours,\\
   JPLTEXT=                                UsrMadeChg,status_flag, \\
   JPLTEXT=                               \
 edit_reqd,ed_error_count,ed_warning_count,err_img,err_msg_dl )
   JPLTEXT=                        }
   JPLTEXT=                        else
   JPLTEXT=                        {
   JPLTEXT=                                obj_retval = \\
   JPLTEXT=                                sm_obj_call (payroll_hndl,\
 "update_time", active_empid, pp, ppyear, active_role, \\
   JPLTEXT=                                w1hrcode, w1day_1, w1day_2,\
 w1day_3, w1day_4, w1day_5, w1day_6, w1day_7, \\
   JPLTEXT=                                w1key, w1cost, w1shift, w1envcode,\
 w1telework, w1base, w1occ, w1poscode, w1mentoring,  w1taskcode, w1pp,\
 w1hrdesc, \\
   JPLTEXT=                                w1costdesc, w2hrcode, w2day_1,\
 w2day_2, w2day_3, w2day_4, w2day_5, w2day_6, w2day_7, w2key, \\
   JPLTEXT=                                w2cost, w2shift, w2envcode,\
 w2telework, w2base, w2occ, w2poscode, w2mentoring, w2taskcode, w2pp,\
 w2hrdesc, \\
   JPLTEXT=                                w2costdesc, mgr_id, login_id,\
 status_dtg, cur_status_dtg, status_array, tna_data, \\
   JPLTEXT=                               \
 w1dyncost1,w1dyncost2,w1dyncost3,w1dyncost4,w1dyncost5,w1dyncost6,\\
   JPLTEXT=                               \
 w1dyncost7,w1dyncost8,w1dyncost9,w1dyncost10,w1dyncost11, \\
   JPLTEXT=                               \
 w1dyncost12,w1dyncost13,w1dyncost14,w1dyncost15,w1dyncost16,w1dyncost17,\\
   JPLTEXT=                               \
 w1dyncost18,w1dyncost19,w1dyncost20,w1dyncost21, \\
   JPLTEXT=                               \
 w2dyncost1,w2dyncost2,w2dyncost3,w2dyncost4,w2dyncost5,w2dyncost6,\\
   JPLTEXT=                               \
 w2dyncost7,w2dyncost8,w2dyncost9,w2dyncost10,w2dyncost11, \\
   JPLTEXT=                               \
 w2dyncost12,w2dyncost13,w2dyncost14,w2dyncost15,w2dyncost16,w2dyncost17,\\
   JPLTEXT=                               \
 w2dyncost18,w2dyncost19,w2dyncost20,w2dyncost21,\\
   JPLTEXT=                                worktype_code,\
 worktype_description, regular_hours, premium_hours,\\
   JPLTEXT=                                UsrMadeChg,status_flag, \\
   JPLTEXT=                               \
 edit_reqd,ed_error_count,ed_warning_count,err_img,err_msg_dl )
   JPLTEXT=                        }
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=        else
   JPLTEXT=        {
   JPLTEXT=                if (cfg$websphere)
   JPLTEXT=                {
   JPLTEXT=                        obj_retval = \\
   JPLTEXT=                                sm_obj_call (amend_payroll_hndl,\
 "u_a_pay_time", active_empid, pp, ppyear, active_role, \\
   JPLTEXT=                                w1hrcode, w1day_1, w1day_2,\
 w1day_3, w1day_4, w1day_5, w1day_6, w1day_7, \\
   JPLTEXT=                                w1key, w1cost, w1shift, w1envcode,\
 w1telework, w1base, w1occ, w1poscode, w1mentoring, w1taskcode, w1pp,\
 w1hrdesc, \\
   JPLTEXT=                                w1costdesc, w2hrcode, w2day_1,\
 w2day_2, w2day_3, w2day_4, w2day_5, w2day_6, w2day_7, w2key, \\
   JPLTEXT=                                w2cost, w2shift, w2envcode,\
 w2telework, w2base, w2occ, w2poscode, w2mentoring, w2taskcode, w2pp,\
 w2hrdesc, \\
   JPLTEXT=                                w2costdesc, mgr_id, login_id,\
 status_dtg, status_array, prior_tna_data, \\
   JPLTEXT=                               \
 w1dyncost1,w1dyncost2,w1dyncost3,w1dyncost4,w1dyncost5,w1dyncost6,\\
   JPLTEXT=                               \
 w1dyncost7,w1dyncost8,w1dyncost9,w1dyncost10,w1dyncost11, \\
   JPLTEXT=                               \
 w1dyncost12,w1dyncost13,w1dyncost14,w1dyncost15,w1dyncost16,w1dyncost17,\\
   JPLTEXT=                               \
 w1dyncost18,w1dyncost19,w1dyncost20,w1dyncost21, \\
   JPLTEXT=                               \
 w2dyncost1,w2dyncost2,w2dyncost3,w2dyncost4,w2dyncost5,w2dyncost6,\\
   JPLTEXT=                               \
 w2dyncost7,w2dyncost8,w2dyncost9,w2dyncost10,w2dyncost11, \\
   JPLTEXT=                               \
 w2dyncost12,w2dyncost13,w2dyncost14,w2dyncost15,w2dyncost16,w2dyncost17,\\
   JPLTEXT=                               \
 w2dyncost18,w2dyncost19,w2dyncost20,w2dyncost21,w1amend_hrsprim_rownum, \\
   JPLTEXT=                               \
 w1hrsprim_rownum,w2amend_hrsprim_rownum,w2hrsprim_rownum,             \\
   JPLTEXT=                               \
 w1empbureaucode,w2empbureaucode,w1empsbureaucode,w2empsbureaucode,UsrMadeChg,\
status_flag, \\
   JPLTEXT=                               \
 edit_reqd,ed_error_count,ed_warning_count,err_img,err_msg_dl )
   JPLTEXT=
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=        retval = ut_service_error()
   JPLTEXT=
   JPLTEXT=        save_flag = retval
   JPLTEXT=        del_row_flag = gbl$FALSE
   JPLTEXT=
   JPLTEXT=        if (ed_error_count > 0 )
   JPLTEXT=        {
   JPLTEXT=                validation_error = gbl$TRUE
   JPLTEXT=
   JPLTEXT=                call change_status_array("UNV", 1, 14)
   JPLTEXT=                orig_status = status_array[14]
   JPLTEXT=
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        //Display concurrency message.
   JPLTEXT=
   JPLTEXT=        if ( retval == gbl$DBMS_DATA_CONCURRENCY )  || ( retval ==\
 gbl$DBMS_PREVIOUSLY_TRANSMITTED )
   JPLTEXT=        {
   JPLTEXT=                call do_select()
   JPLTEXT=                call sm_n_clear_array("err_msg_dl")
   JPLTEXT=                call sm_n_clear_array("err_img")
   JPLTEXT=                ed_error_count = 0
   JPLTEXT=                err_msg_idx    = 0
   JPLTEXT=                validation_error = gbl$FALSE
   JPLTEXT=
   JPLTEXT=
   JPLTEXT=                if ( retval == gbl$DBMS_DATA_CONCURRENCY )
   JPLTEXT=                {
   JPLTEXT=                        call ut5_msg_warning("Timesheet data not\
 saved to the database" ## \\
   JPLTEXT=                        " because dependent data was changed.%N" ##\
 \\
   JPLTEXT=                        "The latest data has been retrieved from\
 the" ## \\
   JPLTEXT=                        " database for your convenience.(3)")
   JPLTEXT=                }
   JPLTEXT=                else if ( retval == gbl$DBMS_PREVIOUSLY_TRANSMITTED\
 )
   JPLTEXT=                {
   JPLTEXT=                     previously_transmitted_flag = gbl$TRUE
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=        else if (retval)
   JPLTEXT=        {
   JPLTEXT=             // Service call error encountered - Notify user
   JPLTEXT=             // and retrieve last saved timesheet.
   JPLTEXT=             if (week_num == 2)
   JPLTEXT=             {
   JPLTEXT=                  // Need to clear these flags to prevent
   JPLTEXT=                  // change week save processing when
   JPLTEXT=                  // do_select executes.
   JPLTEXT=                  //
   JPLTEXT=                  security_flag = gbl$FALSE
   JPLTEXT=                  save_flag  = gbl$FALSE
   JPLTEXT=                  status_flag  = gbl$FALSE
   JPLTEXT=                  edit_reqd  = gbl$FALSE
   JPLTEXT=             }
   JPLTEXT=
   JPLTEXT=                call do_select()
   JPLTEXT=
   JPLTEXT=                call sm_n_clear_array("err_msg_dl")
   JPLTEXT=                call sm_n_clear_array("err_img")
   JPLTEXT=                ed_error_count = 0
   JPLTEXT=                err_msg_idx    = 0
   JPLTEXT=
   JPLTEXT=                validation_error = gbl$FALSE
   JPLTEXT=
   JPLTEXT=                timestamp_format = "n%0h::%0M::%0s"
   JPLTEXT=                call sm_n_putfield ("error_time", sm_sdtime\
 (timestamp_format))
   JPLTEXT=                timestamp_format = "n%0m/%0d/%4y"
   JPLTEXT=                call sm_n_putfield ("error_date", sm_sdtime\
 (timestamp_format))
   JPLTEXT=
   JPLTEXT=                call ut5_msg_warning("%NTimesheet data not saved to\
 the database" ## \\
   JPLTEXT=                  " due to an unexpected system error.%N" ## \\
   JPLTEXT=                  "If problem persists print this screen and \
 contact the Help Desk." ## \\
   JPLTEXT=                  "%NThe lastest data has been retrieved from"   ##\
 \\
   JPLTEXT=                  " the database. %N :error_date :error_time ")
   JPLTEXT=        }
   JPLTEXT=        else
   JPLTEXT=        {
   JPLTEXT=                // No error, continue with timesheet processing.
   JPLTEXT=
   JPLTEXT=                retval = ut_parse_focus_error()
   JPLTEXT=                //Re-sync the err_msg_idx.
   JPLTEXT=                num_occ = @widget("err_sync")->num_occurrences
   JPLTEXT=                if ( num_occ > 0 )
   JPLTEXT=                {
   JPLTEXT=                        call set_err_idx(num_occ)
   JPLTEXT=                }
   JPLTEXT=                else
   JPLTEXT=                {
   JPLTEXT=                        call set_err_idx("1")
   JPLTEXT=                }
   JPLTEXT=
   JPLTEXT=                //Re-size error widgets.
   JPLTEXT=
   JPLTEXT=                if ( (ed_error_count > 0) || (ed_warning_count > 0)\
 )
   JPLTEXT=                {
   JPLTEXT=                        @widget("err_img")->length = 13
   JPLTEXT=                }
   JPLTEXT=                if ( ed_error_count > 0 )
   JPLTEXT=                {
   JPLTEXT=                        @widget("err_msg_dl")->memo9 =\
 "--qt-msg-warn-fg"
   JPLTEXT=                }
   JPLTEXT=                else
   JPLTEXT=                {
   JPLTEXT=                        @widget("err_msg_dl")->memo9 =\
 "--qt-msg-info-fg"
   JPLTEXT=                }
   JPLTEXT=
   JPLTEXT=        }
   JPLTEXT=        orig_status = status_array[14]
   JPLTEXT=
   JPLTEXT=        save_flag = 0
   JPLTEXT=        status_flag = 0
   JPLTEXT=        edit_reqd = 0
   JPLTEXT=
   JPLTEXT=        if !(retval) && (temp_save_flag == 1) && (display_save_msg)
   JPLTEXT=        {
   JPLTEXT=                call ut5_msg_information("Hourly Timesheet data\
 saved.")
   JPLTEXT=        }
   JPLTEXT=        if (!retval)
   JPLTEXT=        {
   JPLTEXT=                // Clear flags when save is successful
   JPLTEXT=                add_del_flag = 0
   JPLTEXT=                del_row_flag = 0
   JPLTEXT=        }
   JPLTEXT=        call get_dyncostdescs()
   JPLTEXT=        call get_favorites_payroll_data()
   JPLTEXT=        call select_leavebal()
   JPLTEXT=        call select_comments()
   JPLTEXT=        return (retval)
   JPLTEXT=} //end proc update_time()
   JPLTEXT=
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine:   update_payroll_status
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine updates payroll data.
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2000-09-10      D. Bodlien / E. Ren     CR20000398
   JPLTEXT=//                      Add processing for amendments.
   JPLTEXT=//      2004-02-18      M. Ryan         CR20030587
   JPLTEXT=//                      Removed excess display code
   JPLTEXT=//      2006-05-04      T. Peace        PR20060258
   JPLTEXT=//                      Clear error message array
   JPLTEXT=//      2006-06-14      T. Peace        PR20060357
   JPLTEXT=//                      Save the original status after the database
   JPLTEXT=//                      update is performed.  Also clear indicator\
 that
   JPLTEXT=//                      a data change was made in the non-displayed\
 week.
   JPLTEXT=//      2008-01-28      T. Peace        PR20080001
   JPLTEXT=//                      Prevent updates to timesheets already\
 transmitted to FPPS.
   JPLTEXT=//      2016-07-19      E. Ramos        PR20160268
   JPLTEXT=//                      Removed references to the flag\
 other_wk_chg_flag which is now obsolete.
   JPLTEXT=//      2023-06-02      A. McCormick    DFCT0013320
   JPLTEXT=//                      Code changes needed to allow the default\
 timesheet to act independently from the current timesheet and
   JPLTEXT=//                      prevent the default from loading a holiday,\
 or allowing specific hour codes from being saved on the Default or Pre-Print.
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=proc update_payroll_status ()
   JPLTEXT={
   JPLTEXT=        vars retval(10)
   JPLTEXT=
   JPLTEXT=        if (status_flag)
   JPLTEXT=        {
   JPLTEXT=                if (!amendment)
   JPLTEXT=                {
   JPLTEXT=                        retval = ut_update_status ()
   JPLTEXT=                }
   JPLTEXT=                else
   JPLTEXT=                {
   JPLTEXT=                        public a_update_status.jpl
   JPLTEXT=                        retval = a_update_status ()
   JPLTEXT=                        if (cfg$DEVELOPMENT)
   JPLTEXT=                            unload a_update_status.jpl
   JPLTEXT=                }
   JPLTEXT=                if ( retval == gbl$DBMS_DATA_CONCURRENCY )   || (\
 retval == gbl$DBMS_PREVIOUSLY_TRANSMITTED )
   JPLTEXT=                {
   JPLTEXT=                        call do_select()
   JPLTEXT=                        call sm_n_clear_array("err_msg_dl")
   JPLTEXT=                        call sm_n_clear_array("err_img")
   JPLTEXT=                        ed_error_count = 0
   JPLTEXT=                        err_msg_idx    = 0
   JPLTEXT=                        validation_error = gbl$FALSE
   JPLTEXT=                        if ( retval == gbl$DBMS_DATA_CONCURRENCY )
   JPLTEXT=                        {
   JPLTEXT=                             call ut5_msg_warning("Hourly Timesheet\
 status not saved to the database" ## \\
   JPLTEXT=                                " because dependent data was\
 changed.%N" ## \\
   JPLTEXT=                                "The latest data has been retrieved\
 from the" ## \\
   JPLTEXT=                                " database for your\
 convenience.(4)")
   JPLTEXT=                }
   JPLTEXT=                        else if ( retval ==\
 gbl$DBMS_PREVIOUSLY_TRANSMITTED )
   JPLTEXT=                        {
   JPLTEXT=                             previously_transmitted_flag = gbl$TRUE
   JPLTEXT=                        }
   JPLTEXT=                }
   JPLTEXT=                status_flag = retval
   JPLTEXT=
   JPLTEXT=                orig_status = status_array[14]
   JPLTEXT=
   JPLTEXT=
   JPLTEXT=        }
   JPLTEXT=        UsrMadeChg = 0
   JPLTEXT=        UsrMadeStatusChg = 0
   JPLTEXT=        return (retval)
   JPLTEXT=} //end proc update_payroll_status()
   JPLTEXT=
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine:   update_payroll
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine updates payroll data.
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2000-09-10      D. Bodlien / E. Ren     CR20000398
   JPLTEXT=//                      Add processing for amendments.
   JPLTEXT=//      2002-04-16      D. Maynard      PR20020291
   JPLTEXT=//                      Clear the error message array when before
   JPLTEXT=//                      validation adn edits.
   JPLTEXT=//      2005-04-04      A. McCormick    CR20040387
   JPLTEXT=//                      Added call to set_leave_defaults
   JPLTEXT=//      2005-05-31      E. Ramos        CR20050039
   JPLTEXT=//                      Add support for cost structure components.
   JPLTEXT=//      2006-04-20      T. Peace        PR20060258
   JPLTEXT=//                      Process both the timesheet and status\
 change within a single database transaction.
   JPLTEXT=//      2006-07-12      T. Peace        PR20060465
   JPLTEXT=//                      Relocated "Payroll Data Saved" message from\
 proc update_payroll to proc update_time.
   JPLTEXT=//      2007-02-21      T. Peace        PR20060893
   JPLTEXT=//                      Add payroll_flag indicator on calls to\
 load_default_leave procedure.
   JPLTEXT=//      2007-10-02      T. Peace        PR20070477
   JPLTEXT=//                      Clear status_flag before processing save\
 payroll request.
   JPLTEXT=//      2007-11-08      T. Peace        CR20070158
   JPLTEXT=//                      Set UsrMadeChg flag to ensure timesheet is\
 always saved when Save function is selected.
   JPLTEXT=//       2008-04-24     T. Peace         PR20080212
   JPLTEXT=//                      Set status_flag for SVC call if status is\
 changing to UNV as result of a deletion of a timesheet row.
   JPLTEXT=//      2011-05-02      A. McCormick    PR20110185
   JPLTEXT=//                      Set the status_flag to FALSE if not loaded\
 from a default / preprint.
   JPLTEXT=//      2021-06-04      E. Ramos        ENHC0036192
   JPLTEXT=//                      Populate status column (for BOR).
   JPLTEXT=//      2024-08-15      A. McCormick    ENHC0037149
   JPLTEXT=//                      Added code needed to move Timesheet\
 Comments to this screen.
   JPLTEXT=//      2025-01-23      A. McCormick    DFCT0014136
   JPLTEXT=//                      Relocated the comment save functionality to\
 before the Timesheet save functionality
   JPLTEXT=//                      in case E-PAY-045 is enabled in the\
 instance the edit check will see the comment exists
   JPLTEXT=//                      before triggering an error to the\
 operators.
   JPLTEXT=//      2025-01-23      A. McCormick    DFCT0014136
   JPLTEXT=//                      Moved the Comment save messages here.
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=proc update_payroll ()
   JPLTEXT={
   JPLTEXT=        UsrMadeChg = 1
   JPLTEXT=        if ( security_flag )
   JPLTEXT=        {
   JPLTEXT=                call set_leave_defaults()
   JPLTEXT=                edit_reqd = gbl$FALSE
   JPLTEXT=
   JPLTEXT=                if !(default_preprint)
   JPLTEXT=                   status_flag = 0
   JPLTEXT=
   JPLTEXT=                // Set default leave codes (disabled fields are not\
 transmitted from the browser to the server)
   JPLTEXT=                call load_default_leave(gbl$TRUE)
   JPLTEXT=
   JPLTEXT=                // For BOR, populate the status column on rows\
 where there is a cost key value (BOR).
   JPLTEXT=                // If there is no cost key value in a row clear all\
 the component values
   JPLTEXT=                call do_populate_status_bor()
   JPLTEXT=
   JPLTEXT=                if ( do_valid_screen() )
   JPLTEXT=                {
   JPLTEXT=                        return (-1)
   JPLTEXT=                }
   JPLTEXT=
   JPLTEXT=                if (del_row_flag) && (status_array[14] !=\
 orig_status)
   JPLTEXT=                {
   JPLTEXT=                        status_flag = gbl$TRUE
   JPLTEXT=                }
   JPLTEXT=
   JPLTEXT=                //Save time data.
   JPLTEXT=
   JPLTEXT=                if(default_preprint >= 2)
   JPLTEXT=                {
   JPLTEXT=                        if ( update_default_preprint() )
   JPLTEXT=                        {
   JPLTEXT=                                return (-1)
   JPLTEXT=                        }
   JPLTEXT=                }
   JPLTEXT=                else
   JPLTEXT=                {
   JPLTEXT=                        if ( update_comments() )
   JPLTEXT=                        {
   JPLTEXT=                                return (-1)
   JPLTEXT=                        }
   JPLTEXT=
   JPLTEXT=                        if ( update_time() )
   JPLTEXT=                        {
   JPLTEXT=                                return (-1)
   JPLTEXT=                        }
   JPLTEXT=                }
   JPLTEXT=
   JPLTEXT=// PR20060258
   JPLTEXT=// Remove call to update payroll status.
   JPLTEXT=// Must be performed within update_time EJB call above
   JPLTEXT=// to ensure status is updated within the same
   JPLTEXT=// DB transaction.
   JPLTEXT=//
   JPLTEXT=                if (del_emp_flag == 1)
   JPLTEXT=                        call ut5_msg_information("Selected Employee\
 Comment row deleted.")
   JPLTEXT=
   JPLTEXT=                if (del_tk_flag == 1)
   JPLTEXT=                        call ut5_msg_information("Selected\
 Timekeeper Comment row deleted.")
   JPLTEXT=
   JPLTEXT=                if (comment_save_flag == gbl$TRUE)
   JPLTEXT=                        call ut5_msg_information("Comment data\
 saved.")
   JPLTEXT=
   JPLTEXT=//                call ut5_msg_information("Hourly Timesheet data\
 saved.")
   JPLTEXT=        }
   JPLTEXT=        else
   JPLTEXT=        {
   JPLTEXT=                unauthorized_action = gbl$TRUE
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        return (0)
   JPLTEXT=} //end proc update_payroll()
   JPLTEXT=
