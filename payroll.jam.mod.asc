   JPLTEXT=//      Routine:   set_status_options
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine sets status change options. It returns
   JPLTEXT=//              0 if the screen can be modified.
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2000-08-14      D. Maynard
   JPLTEXT=//                      Add code to keep one from validating or
   JPLTEXT=//                      certifying oneself.
   JPLTEXT=//                      Add bool# vars to clean up "if" statement.
   JPLTEXT=//      2000-09-28      D. Maynard
   JPLTEXT=//                      Add new bool conditions to setting status\
 options.
   JPLTEXT=//      2000-10-09      D. Maynard
   JPLTEXT=//                      Add boolean conditions for employee and\
 administrator.
   JPLTEXT=//      2000-10-12      D. Maynard
   JPLTEXT=//                      Move contents of set_status_options to
   JPLTEXT=//                      common.lib jpl: choose_options.jpl
   JPLTEXT=//      2000-09-10      D. Bodlien / E. Ren     CR20000398
   JPLTEXT=//                      Add processing for amendments.
   JPLTEXT=//      2004-02-18      M. Ryan         CR20030587
   JPLTEXT=//                      Removed excess display code
   JPLTEXT=//      2006-11-09      T. Peace        PR20060680
   JPLTEXT=//                      Prevent Administrators from downgrading
   JPLTEXT=//                      status of released timesheets.
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=proc set_status_options ()
   JPLTEXT={
   JPLTEXT=        vars retval(10)
   JPLTEXT=
   JPLTEXT=        if (tna_data[idx$ACTIVE_ROLE] == gbl$ROLE_ADMINISTRATOR && \
\\
   JPLTEXT=        status_array[14] == 'REL' )
   JPLTEXT=        {
   JPLTEXT=             retval = 0
   JPLTEXT=        }
   JPLTEXT=        else
   JPLTEXT=        {
   JPLTEXT=                if (!amendment)
   JPLTEXT=                {
   JPLTEXT=                      retval = 0
   JPLTEXT=                        public choose_options.jpl
   JPLTEXT=                        retval = choose_status_options()
   JPLTEXT=                        if (cfg$DEVELOPMENT)
   JPLTEXT=                            unload choose_options.jpl
   JPLTEXT=                }
   JPLTEXT=                else
   JPLTEXT=                {
   JPLTEXT=                        public a_choose_options.jpl
   JPLTEXT=                        retval = a_choose_status_options ()
   JPLTEXT=                        if (cfg$DEVELOPMENT)
   JPLTEXT=                            unload a_choose_options.jpl
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=        return (retval)
   JPLTEXT=} //end proc set_status_options()
