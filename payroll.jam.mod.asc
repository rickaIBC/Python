   JPLTEXT=        vars v_txt_list_of_hrscodes
   JPLTEXT=        vars v_sioreq
   JPLTEXT=        vars week_num
   JPLTEXT=
   JPLTEXT=        protect_status = 0
   JPLTEXT=        display_save_msg = gbl$TRUE
   JPLTEXT=
   JPLTEXT=        ts_comments_cnt = 0
   JPLTEXT=        remarks_cnt = 0
   JPLTEXT=
   JPLTEXT=        @screen("@current")->html_template = "payroll.htm"
   JPLTEXT=
   JPLTEXT=        //Display pay period.
   JPLTEXT=
   JPLTEXT=        if !(default_preprint >= 2)
   JPLTEXT=        {
   JPLTEXT=                if (!amendment)
   JPLTEXT=                {
   JPLTEXT=                        call ut_dis_pay_periods(future)
   JPLTEXT=                }
   JPLTEXT=                else
   JPLTEXT=                {
   JPLTEXT=                        public a_dis_pay_periods.jpl
   JPLTEXT=                        call a_dis_pay_periods ()
   JPLTEXT=                        if (cfg$DEVELOPMENT)
   JPLTEXT=                            unload a_dis_pay_periods.jpl
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        if ( sm_is_bundle("grp_status") )
   JPLTEXT=        {
   JPLTEXT=                back_dl->hidden = PV_NO
   JPLTEXT=
   JPLTEXT=        }
   JPLTEXT=        if ( grp_future_flag )
   JPLTEXT=        {
   JPLTEXT=                back_dl->hidden = PV_NO
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        if ( grp_amendment_flag )
   JPLTEXT=        {
   JPLTEXT=                back_dl->hidden = PV_NO
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        for week_num = 1 while week_num <=2
   JPLTEXT=        {
   JPLTEXT=                //Set visibility based on configuration settings.
   JPLTEXT=
   JPLTEXT=                if ( !cfg$MIS_TASK_CODE )
   JPLTEXT=                {
   JPLTEXT=                        w:(week_num)taskcode->hidden = PV_YES
   JPLTEXT=                        w:(week_num)taskcode->validation_func = ""
   JPLTEXT=                }
   JPLTEXT=
   JPLTEXT=                if ( cfg$ACCOUNT_TYPE == gbl$ACCOUNT_NFC )
   JPLTEXT=                {
   JPLTEXT=                        w:(week_num)shift->hidden = PV_YES
   JPLTEXT=                        w:(week_num)base->hidden = PV_YES
   JPLTEXT=                        w:(week_num)envcode->hidden = PV_YES
   JPLTEXT=                        w:(week_num)telework->hidden = PV_YES
   JPLTEXT=                        w:(week_num)poscode->hidden = PV_YES
   JPLTEXT=                        w:(week_num)occ->hidden = PV_YES
   JPLTEXT=                        w:(week_num)shift->validation_func = ""
   JPLTEXT=                        w:(week_num)base->validation_func = ""
   JPLTEXT=                        w:(week_num)envcode->validation_func = ""
   JPLTEXT=                        w:(week_num)telework->validation_func = ""
   JPLTEXT=                        w:(week_num)poscode->validation_func = ""
   JPLTEXT=                        w:(week_num)occ->validation_func = ""
   JPLTEXT=                }
