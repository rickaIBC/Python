 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine:   do_save
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine is executed when the SAVE button is\
 clicked on.
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2014-09-24      E. Ramos        PR20140482
   JPLTEXT=//                      Created function.
   JPLTEXT=//                      Avoid downgrading status of timesheet if\
 the SAVE button is pressed but no changes have been made.
   JPLTEXT=//      2014-10-15      E. Ramos        PR20140482
   JPLTEXT=//                      Reset the UsrMadeChg flag after saving the\
 timesheet.
   JPLTEXT=//      2014-11-05      E. Ramos        PR20140482 REWORK 2
   JPLTEXT=//                      Ensure totals are displayed when data is\
 not saved.
   JPLTEXT=//      2016-06-20      E. Ramos        CR20160062
   JPLTEXT=//                      Show week 1 and week 2 on the same screen.
   JPLTEXT=//      2023-03-08      E. Ramos        DFCT0013147
   JPLTEXT=//                      Reset UsrMadeChg only if\
 data_changed_message==0
   JPLTEXT=//      2025-04-15      T. Peace        DFCT0014160
   JPLTEXT=//                      Ensure timesheet comments and leavbal\
 totals are
   JPLTEXT=//                      reloaded after a save operation is\
 performed.
   JPLTEXT=//
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=proc do_save()
   JPLTEXT={
   JPLTEXT=        vars i w
   JPLTEXT=
   JPLTEXT=        call ut_dbms_sql ("BLMcode,BLMcode2", \\
   JPLTEXT=                                "SELECT code,code2 FROM\
 cost_element_ref_list WHERE reference_id = 'SUBACTIVITY-SBY' ORDER BY code")
   JPLTEXT=
   JPLTEXT=        if (UsrMadeChg == 1)
   JPLTEXT=        {
   JPLTEXT=                call update_payroll()
   JPLTEXT=                // Reset the flag once the time sheet has been\
 saved
   JPLTEXT=                if (!data_changed_message)
   JPLTEXT=                {
   JPLTEXT=                        UsrMadeChg = 0
   JPLTEXT=                        UsrMadeStatusChg = "0"  // just being\
 careful
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=        else
   JPLTEXT=        {
   JPLTEXT=                call get_daily_totals()
   JPLTEXT=                call select_leavebal()
   JPLTEXT=                call select_comments()
   JPLTEXT=        }
   JPLTEXT=}
