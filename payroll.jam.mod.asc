   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=proc valid_mentoring (fld_no, fld_val, fld_occ, fld_flag)
   JPLTEXT={
   JPLTEXT=        vars retval = 0, t_val(10)
   JPLTEXT=
   JPLTEXT=        if ( !flag_changes(fld_no, fld_val, fld_occ, fld_flag) )
   JPLTEXT=        {
   JPLTEXT=                return (0)
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        //Pad the field.
   JPLTEXT=
   JPLTEXT=        t_val = ut_squeeze(fld_val, ut$SQUEEZE_LEAD_TRAIL, "")
   JPLTEXT=        t_val = ut_substitute(t_val, "'", "")
   JPLTEXT=        t_val = ut_substitute(t_val, '"', '')
   JPLTEXT=
   JPLTEXT=        if ( @length(t_val) == 0 )
   JPLTEXT=        {
   JPLTEXT=                @field_num(fld_no)[fld_occ] = ''
   JPLTEXT=                return (0)
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        if ( !ut_dbms_exists("select count(*) from ref_mentoring\
 where code = ':t_val' and status = 'A' ") )
   JPLTEXT=        {
   JPLTEXT=
   JPLTEXT=                //Record not found.
   JPLTEXT=
   JPLTEXT=                call ut5_msg_warning("Invalid mentoring code\
 ':t_val'.")
   JPLTEXT=                retval = 1
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        if (fld_val != t_val)
   JPLTEXT=                @field_num(fld_no)[fld_occ] = t_val
   JPLTEXT=
   JPLTEXT=        return (retval)
   JPLTEXT=} //end proc valid_mentoring()
   JPLTEXT=
