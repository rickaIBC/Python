 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine: figure_dates
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine figures out the mon/day combination to\
 be displayed next to day of week label.
   JPLTEXT=//              This change was requested by BPD.
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2000-12-13      Fran Haibach
   JPLTEXT=//                      Created
   JPLTEXT=//      2000-09-10      D. Bodlien / E. Ren     CR20000398
   JPLTEXT=//                      Add processing for amendments.
   JPLTEXT=//      2004-02-18      M. Ryan         CR20030587
   JPLTEXT=//                      Removed excess display code & added\
 column_title.
   JPLTEXT=//      2004-06-09      Ed Bowes        CR20040112
   JPLTEXT=//                      Filled in memo7 text with full name of day
   JPLTEXT=//                      that corresponds to the column_title being
   JPLTEXT=//                      assigned.  This is in the interest of being
   JPLTEXT=//                      508 compliant and friendly.
   JPLTEXT=//      2005-10-19      A. McCormick    PR20050946
   JPLTEXT=//                      Added code to set the memo7 field on the\
 cost components - 508 Compliance
   JPLTEXT=//      2009-04-13      E. Ramos        CR20010062
   JPLTEXT=//                      Support data entry for future pay periods.
   JPLTEXT=//      2016-06-20      E. Ramos        CR20160062
   JPLTEXT=//                      Show week 1 and week 2 on the same screen.
   JPLTEXT=//      2016-10-18      I. Lane         PR20160423
   JPLTEXT=//                      Changed parameters to be used for memo7\
 property of the
   JPLTEXT=//                      week day widget to use ":(week_day)"\
 instead of "1" to ensure that JAWS
   JPLTEXT=//                      accuratley identifies the weekday that the\
 cursor is on in the timesheet.
   JPLTEXT=//      2024-07-31      M. Luke         ENHC0037148
   JPLTEXT=//                      Updated to make day column headers have the\
 full day name without the dates.
   JPLTEXT=//      2025-01-31      J. Grossman     DFCT0014147
   JPLTEXT=//                      revert to short day + date
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=proc figure_dates()
   JPLTEXT={
   JPLTEXT=  vars i
   JPLTEXT=  vars week_num
   JPLTEXT=
   JPLTEXT=        if cfg$PAYDATES == 1
   JPLTEXT=        {
   JPLTEXT=                if (!amendment)
   JPLTEXT=                {
   JPLTEXT=                        if ( startpay_period == gbl$PAY_PREVIOUS )
   JPLTEXT=                        {
   JPLTEXT=                                startday =\
 tna_data[idx$PAY_PREV_START]
   JPLTEXT=                        }
   JPLTEXT=                        if ( startpay_period == gbl$PAY_CURRENT )
   JPLTEXT=                        {
   JPLTEXT=                                startday = currstartday
   JPLTEXT=                        }
   JPLTEXT=                        if (startpay_period == gbl$PAY_CORRECTION)
   JPLTEXT=                        {
   JPLTEXT=                                startday =\
 tna_data[idx$PAY_PREV_START]
   JPLTEXT=                        }
   JPLTEXT=                        if (future)
   JPLTEXT=                        {
   JPLTEXT=                                startday =\
 future_tna_data[idx$PAY_FUTURE_START]
   JPLTEXT=                        }
   JPLTEXT=                }
   JPLTEXT=                else
   JPLTEXT=                {
   JPLTEXT=                        startday = currstartday
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=
   JPLTEXT=        vars day_week_name  [7] (10) = {"Sunday", "Monday",\
 "Tuesday","Wednesday", "Thursday", "Friday", "Saturday"}
   JPLTEXT=        vars day_week_short [7] (3) =\
 {"Sun","Mon","Tue","Wed","Thu","Fri","Sat"}
   JPLTEXT=
   JPLTEXT=        for week_num = 1 while week_num <= 2
   JPLTEXT=        {
   JPLTEXT=                for week_day = 1 while week_day <= 7
   JPLTEXT=                {
   JPLTEXT=                        date_template = @date(startday) +\
 (7*(week_num-1)) + (week_day-1)
   JPLTEXT=                        w:(week_num)day_:(week_day) ->column_title\
 = day_week_short[week_day] ## "<br>:date_template"
   JPLTEXT=                        w:(week_num)day_:(week_day) ->memo7      =\
 day_week_name[week_day] ## " :date_template"
   JPLTEXT=                }
   JPLTEXT=
   JPLTEXT=
   JPLTEXT=                if(cfg$COST_COMPONENTS_FOR_ENTRY)
   JPLTEXT=                {
   JPLTEXT=                        for i = 1 while i <= 21
   JPLTEXT=                        {
   JPLTEXT=                                w:(week_num)dyncost:(i)->memo7 =\
 dyncost_label[i]
   JPLTEXT=                        }
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=
   JPLTEXT=        return 0
   JPLTEXT=} //end figure_dates()
   JPLTEXT=
