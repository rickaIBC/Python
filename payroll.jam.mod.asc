 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine:   show_import_button
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine determines if the import button is\
 visible
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2012-08-06      A. McCormick    CR20120016
   JPLTEXT=//                      Created function.
   JPLTEXT=//      2013-09-23      E. Ramos        CR20130159
   JPLTEXT=//                      Do not present the IMPORT button if there\
 is no data to load for the user and pay period,
   JPLTEXT=//                      taking into account the list of hours codes\
 configured in cfg$INPUT_HRSPRIM_HRSCODES_FILTER.
   JPLTEXT=//      2014-10-22      E. Ramos        CR20140149
   JPLTEXT=//                      Support the value 2 for the\
 cfg$INPUT_HRSPRIM setting.
   JPLTEXT=//                      Support the value "ALL" for the\
 cfg$INPUT_HRSPRIM_FILTER setting.
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=proc show_import_button ()
   JPLTEXT={
   JPLTEXT=        vars sql_stmt, import_cnt, retval
   JPLTEXT=
   JPLTEXT=        if (cfg$INPUT_HRSPRIM==0) || (amendment)
   JPLTEXT=                return 0
   JPLTEXT=
   JPLTEXT=        sql_stmt = "select count(*) from input_hrsprim where empid\
 = ':active_empid' and ppyear = :ppyear and pp = :pp"
   JPLTEXT=
   JPLTEXT=        if cfg$INPUT_HRSPRIM_HRSCODES_FILTER != "ALL"
   JPLTEXT=          sql_stmt = sql_stmt ## " and hrscode in (" ##\
 cfg$INPUT_HRSPRIM_HRSCODES_FILTER ## ")"
   JPLTEXT=
   JPLTEXT=        if ( ut_dbms_exists(sql_stmt) )
   JPLTEXT=                retval = 1
   JPLTEXT=        else
   JPLTEXT=                retval = 0
   JPLTEXT=
   JPLTEXT=        // If data to import for the time sheet exists but a single\
 import is allowed, check if
   JPLTEXT=        // data has already been imported to this time sheet
   JPLTEXT=        if (retval==1 && cfg$INPUT_HRSPRIM==2)
   JPLTEXT=        {
   JPLTEXT=                sql_stmt = "select 1 from ppstat where empid =\
 ':active_empid' and ppyear = :ppyear and pp = :pp and dateimport is not null"
   JPLTEXT=                // If data has already been imported to this time\
 sheet then the import button should not be displayed
   JPLTEXT=                if ( ut_dbms_exists(sql_stmt) )
   JPLTEXT=                        retval = 0
   JPLTEXT=        }
   JPLTEXT=        return retval
   JPLTEXT=}
   JPLTEXT=
