 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine: get_component_totals
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine calculates the configured component\
 totals.
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2023-08-23      M. Luke         ENHC0036699
   JPLTEXT=//                      Created.
   JPLTEXT=//      2023-09-27      M. Luke         ENHC0036699-R2
   JPLTEXT=//                      Updated proc to clear compLabel on each\
 execution and
   JPLTEXT=//                      set grid occurences = number of unique\
 component names.
   JPLTEXT=//                      Call removeHTMLtags on the component\
 labels.
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=proc get_component_totals ()
   JPLTEXT={
   JPLTEXT=        vars i(10), j(10), cur_tot(10), wk_tot(10), daysTotal(10)
   JPLTEXT=        vars week_num
   JPLTEXT=        vars week_days_occ
   JPLTEXT=        vars compValue
   JPLTEXT=
   JPLTEXT=        if (cfg$SHOW_PAYROLL_COMPONENT_SUMS[1] != "")
   JPLTEXT=        {
   JPLTEXT=                // Clear any previous gird values.
   JPLTEXT=                call sm_n_clear_array("sumComponents")
   JPLTEXT=                call sm_n_clear_array("w1sumTotals")
   JPLTEXT=                call sm_n_clear_array("w2sumTotals")
   JPLTEXT=                call sm_n_clear_array("sumTotals")
   JPLTEXT=
   JPLTEXT=                // If the site is configured to summarize by\
 components, unhide grid and set column titles.
   JPLTEXT=                compSummaryGrid->hidden = PV_NO
   JPLTEXT=                compLabel = ""
   JPLTEXT=                for i = 1 while cfg$SHOW_PAYROLL_COMPONENT_SUMS[i]\
 != ""
   JPLTEXT=                {
   JPLTEXT=                        if (i > 1)
   JPLTEXT=                                compLabel = compLabel ## ", "
   JPLTEXT=                        compLabel = compLabel ##\
 removeHTMLtags(dyncost_label[cfg$SHOW_PAYROLL_COMPONENT_SUMS[i]])
   JPLTEXT=                }
   JPLTEXT=
   JPLTEXT=                // sumComponents->column_title = compLabel
   JPLTEXT=
   JPLTEXT=                // Loop over week 1 and 2 row totals
   JPLTEXT=                for week_num = 1 while week_num<=2
   JPLTEXT=                {
   JPLTEXT=                        week_days_occ = get_week_days_occ(week_num)
   JPLTEXT=                        for i = 1 while (i <= week_days_occ)
   JPLTEXT=                        {
   JPLTEXT=                                daysTotal =\
 w:(week_num)daystotal[i]
   JPLTEXT=                                if (daysTotal != "")
   JPLTEXT=                                {
   JPLTEXT=                                        // Create a combined\
 component value based on the configuration.
   JPLTEXT=                                        compValue = ""
   JPLTEXT=                                        for j = 1 while\
 cfg$SHOW_PAYROLL_COMPONENT_SUMS[j] != ""
   JPLTEXT=                                        {
   JPLTEXT=                                                if (j > 1)
   JPLTEXT=                                                        compValue =\
 compValue ## ","
   JPLTEXT=                                                compValue =\
 compValue ## w:(week_num)dyncost:(cfg$SHOW_PAYROLL_COMPONENT_SUMS[j])[i]
   JPLTEXT=                                        }
   JPLTEXT=
   JPLTEXT=                                        if (compValue == "" ||\
 compValue == ",")
   JPLTEXT=                                                compValue =\
 "<Blank>"
   JPLTEXT=
   JPLTEXT=                                        cur_tot =\
 ut_hhmm2mm(w:(week_num)daystotal[i])
   JPLTEXT=                                        wk_tot =\
 ut_hhmm2mm(w:(week_num)daystotal[i])
   JPLTEXT=                                        // Loop over the existing\
 component totals to see if there is an existing one.
   JPLTEXT=                                        for j = 1 while j <=\
 sumComponents->num_occurrences
   JPLTEXT=                                        {
   JPLTEXT=                                                if\
 (sumComponents[j] = compValue)
   JPLTEXT=                                                {
   JPLTEXT=                                                        cur_tot =\
 cur_tot + ut_hhmm2mm(sumTotals[j])
   JPLTEXT=                                                        wk_tot =\
 wk_tot + ut_hhmm2mm(w:(week_num)sumTotals[j])
   JPLTEXT=                                                        break
   JPLTEXT=                                                }
   JPLTEXT=                                        }
   JPLTEXT=
   JPLTEXT=                                        if (j >\
 sumComponents->num_occurrences)
   JPLTEXT=                                                sumComponents[j] =\
 compValue
   JPLTEXT=
   JPLTEXT=                                        sumTotals[j] =\
 ut_formatdaystotal(ut_mm2hhmm (cur_tot))
   JPLTEXT=                                        w:(week_num)sumTotals[j] =\
 ut_formatdaystotal(ut_mm2hhmm (wk_tot))
   JPLTEXT=                                        if (cfg$DEVELOPMENT)
   JPLTEXT=                                                call\
 ut_log_error("get_component_totals:: week_num (:week_num) week-row (:i)\
 compValue (:compValue) comp-row (:j)")
   JPLTEXT=                                }
   JPLTEXT=                        }
   JPLTEXT=                }
   JPLTEXT=
   JPLTEXT=                if (compSummaryGrid->num_occurrences == 0)
   JPLTEXT=                        compSummaryGrid->onscreen_rows = 1
   JPLTEXT=                else
   JPLTEXT=                        compSummaryGrid->onscreen_rows =\
 compSummaryGrid->num_occurrences
   JPLTEXT=        }
   JPLTEXT=}
   JPLTEXT=
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine: set_comp_desc
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine fills in the component descriptions in\
 the week grids.
   JPLTEXT=//              A call to dyncost_prepare(), called from\
 refresh_screen(), is required
   JPLTEXT=//              before calling set_comp_desc().
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2023-12-15      M. Luke         ENHC0036698
   JPLTEXT=//                      Created.
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=proc set_comp_desc()
   JPLTEXT={
   JPLTEXT=        vars rtn_desc, idx(10), found(10)
   JPLTEXT=        vars cIdx(10), compId(10), row(10), week_num(10)
   JPLTEXT=
   JPLTEXT=        for cIdx = 1 while dyncost_id[cIdx] != '' && cIdx <= 5
   JPLTEXT=        {
   JPLTEXT=                compId = dyncost_id[cIdx]
   JPLTEXT=                // If one of the first 5 component description\
 columns is configured for display.
   JPLTEXT=                //              dyncost_vis_desc_len < 0 - show\
 full desc.
   JPLTEXT=                //              dyncost_vis_desc_len = 0 - do not\
 display desc.
   JPLTEXT=                //              dyncost_vis_desc_len > 0 - show\
 desc up to max number of chars
   JPLTEXT=                if (dyncost_vis_desc_len[cIdx] != 0)
   JPLTEXT=                {
   JPLTEXT=                        for week_num = 1 while week_num <= 2
   JPLTEXT=                        {
   JPLTEXT=                                // Loop over the rows in the week\
 grid.
   JPLTEXT=                                for row = 1 while row <=\
 w:(week_num)hrcode->num_occurrences
   JPLTEXT=                                {
   JPLTEXT=                                        // Loop over pre-loaded\
 dynacost codes and find the description for the ones in the week grids.
   JPLTEXT=                                        found = 0
   JPLTEXT=                                        for idx = 1 while ((idx <=\
 dyncostid->num_occurrences) && !found)
   JPLTEXT=                                        {
   JPLTEXT=                                                if (dyncostid[idx]\
 == dyncost_ref[cIdx] && dyncostcode[idx] ==\
 w:(week_num)dyncost:(compId)[row])
   JPLTEXT=                                                {
   JPLTEXT=                                                        found = 1
   JPLTEXT=                                                       \
 w:(week_num)dyncostdesc:(compId)[row] = dyncostdesc[idx]
   JPLTEXT=                                                }
   JPLTEXT=                                        }
   JPLTEXT=                                }
   JPLTEXT=                        }
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=        return 0
   JPLTEXT=}
   JPLTEXT=
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine:   select_leavebal
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine performs the selection for the leave\
 balance section of the screen
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2024-08-15      A. McCormick    ENHC0037149
   JPLTEXT=//                      Created from leave.jam
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=proc select_leavebal()
   JPLTEXT={
   JPLTEXT=        vars i(100)
   JPLTEXT=
   JPLTEXT=        if(ppyear == 0)
   JPLTEXT=                return 0
   JPLTEXT=
   JPLTEXT=        if (cfg$websphere)
   JPLTEXT=        {
   JPLTEXT=                update_flag = 'Y'
   JPLTEXT=
   JPLTEXT=                obj_retval = sm_obj_call (leave_hndl,\
 "cal_balance", tna_data, pp, ppyear, update_flag)
   JPLTEXT=
   JPLTEXT=                obj_retval = sm_obj_call (leave_hndl,\
 "get_balance", \\
   JPLTEXT=                        tna_data, active_empid, pp, ppyear, lvtype,\
 balforwdhrs, \\
   JPLTEXT=                        accruehrs, availhrs, ytd, usedhrs, ytd1,\
 balhrs, \\
   JPLTEXT=                        uselose_date, uselose_bal, annuhrs,\
 sickhrs, \\
   JPLTEXT=                        prev_annuhrs, prev_sickhrs, in_days,\
 accrue_check, \\
   JPLTEXT=                        hrscd, advance_type, advance_balance,\
 advance_accrued, \\
   JPLTEXT=                        advance_owed, advance_ytd, advance_hrcode,\
 display_alc_grid, show_on_screen)
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        if ( (tna_data[idx$ACTIVE_TYPE] == gbl$EMPTYPE_PART )     \
 && \\
   JPLTEXT=             ((tna_data[idx$ACTIVE_PAYPLAN] == gbl$EMP_PAYPLAN_CE)\
 || \\
   JPLTEXT=              (tna_data[idx$ACTIVE_PAYPLAN] == gbl$EMP_PAYPLAN_CY)\
 ))
   JPLTEXT=        {
   JPLTEXT=                annuhrs = "00::00"
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        call sm_n_copyarray("bi_balance", "balforwdhrs")
   JPLTEXT=        call sm_n_copyarray("balforwd_dl", "balforwdhrs")
   JPLTEXT=        call sm_n_copyarray("balforwd_slt", "balforwdhrs")
   JPLTEXT=
   JPLTEXT=        usedannuhrs = ut_mm2hhmm(usedannuhrs)
   JPLTEXT=        usedsickhrs = ut_mm2hhmm(usedsickhrs)
   JPLTEXT=        usedannuytd= ut_mm2hhmm(usedannuytd)
   JPLTEXT=        usedsickytd= ut_mm2hhmm(usedsickytd)
   JPLTEXT=
   JPLTEXT=        for i = 1 while i <= hrscd->num_occurrences
   JPLTEXT=        {
   JPLTEXT=                if(hrscd[i] == cfg$LEAVE_FMLA_CODE)
   JPLTEXT=                {
   JPLTEXT=                        family_leave_occ = i
   JPLTEXT=                        i = hrscd->num_occurrences + 1
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        // To correctly remove the lines from the leave balance\
 grid any leave balance codes
   JPLTEXT=        // that the forwards_balance = 'Y' and the show_on_screen =\
 'N' should have the highest
   JPLTEXT=        // sequence numbers assigned to them.
   JPLTEXT=
   JPLTEXT=        for i = hrscd->num_occurrences while i > 0 step -1
   JPLTEXT=        {
   JPLTEXT=                if (show_on_screen[i] == 'N')
   JPLTEXT=                {
   JPLTEXT=                //Setting the parameters for the dynamic labels to\
 NULL so they can be removed from the grid
   JPLTEXT=                        lvtype[i] = ""
   JPLTEXT=                        balforwd_dl[i] = ""
   JPLTEXT=                        balforwd_slt[i] = ""
   JPLTEXT=                        balforwdhrs[i] = ""
   JPLTEXT=                        accruehrs[i] = ""
   JPLTEXT=                        ytd[i] = ""
   JPLTEXT=                        availhrs[i] = ""
   JPLTEXT=                        usedhrs[i] = ""
   JPLTEXT=                        ytd1[i] = ""
   JPLTEXT=                        bi_balance[i] = ""
   JPLTEXT=                        balhrs[i] = ""
   JPLTEXT=                        in_days[i] = ""
   JPLTEXT=                        hrscd[i] = ""
   JPLTEXT=                        call sm_i_doccur("lvtype",i,10)
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=}
   JPLTEXT=
