 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine:   do_delete_one_emp_cmt
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              Delete the record highlighted in the employee's\
 grid widget
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2024-08-15      A. McCormick    ENHC0037149
   JPLTEXT=//                      Created from time_comments.jam
   JPLTEXT=//      2025-01-23      A. McCormick    DFCT0014136
   JPLTEXT=//                      Set the UsrMadeChg, status and edit flags\
 when needed.
   JPLTEXT=//                      Replaced call to update_comments with\
 update_payroll.
   JPLTEXT=//      2025-04-15      T. Peace        DFCT0014160
   JPLTEXT=//                      For amendments, set status to UNV if
   JPLTEXT=//                      no comments remain after the delete.
   JPLTEXT=//                      Only necessay if edit E-PAY-045 is active.
   JPLTEXT=//                      Test that row selected has data.
   JPLTEXT=//                      Ensure comments and leavbal totals are
   JPLTEXT=//                      reloaded after the delete operation.
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=proc do_delete_one_emp_cmt()
   JPLTEXT={
   JPLTEXT=        vars grid_occ(10), retval(10)
   JPLTEXT=        vars amend_comments_cnt(10)
   JPLTEXT=        vars edit045_cnt(10)
   JPLTEXT=
   JPLTEXT=        call sm_n_clear_array("err_msg_dl")
   JPLTEXT=        call sm_n_clear_array("err_img")
   JPLTEXT=        err_msg_idx = 1
   JPLTEXT=
   JPLTEXT=        UsrMadeChg = gbl$TRUE
   JPLTEXT=        if(amendment)
   JPLTEXT=        {
   JPLTEXT=                status_flag = gbl$TRUE
   JPLTEXT=                edit_reqd = gbl$TRUE
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        grid_occ = @widget("employee_grid")->grid_current_occ
   JPLTEXT=
   JPLTEXT=        // Exit if an empty row is selected
   JPLTEXT=        if (emp_comment[:grid_occ] == "")
   JPLTEXT=        {
   JPLTEXT=             call select_leavebal()
   JPLTEXT=             call select_comments()
   JPLTEXT=             return
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        call sm_i_doccur("emp_comment", grid_occ,1)
   JPLTEXT=
   JPLTEXT=        del_emp_flag = 1
   JPLTEXT=        if(amendment)
   JPLTEXT=        {
   JPLTEXT=                edit045_cnt = ut_is_edit_active("E-PAY-045")
   JPLTEXT=                amend_comments_cnt =\
 ut_amend_comments_count(":active_empid",ppyear,pp,prior_tna_data[idx$PAY_CORR\
_YEAR],prior_tna_data[idx$PAY_CORR_PP],prior_tna_data[idx$PAY_CURR_SEQ])
   JPLTEXT=
   JPLTEXT=                // If last comment is being removed then we need to
   JPLTEXT=                // downgrade status to UNV if E-PAY-045 is active.
   JPLTEXT=                // (note the physical delete won't happen until\
 update_payroll
   JPLTEXT=                // executes, so testing for amend_comments_cnt == 1\
 here.)
   JPLTEXT=                if (orig_status != "UNV"    &&  \\
   JPLTEXT=                    edit045_cnt  > 0        &&  \\
   JPLTEXT=                    amend_comments_cnt == 1)
   JPLTEXT=                {
   JPLTEXT=                        UsrMadeChg = gbl$TRUE
   JPLTEXT=                        status_flag = gbl$TRUE
   JPLTEXT=                        call change_status_array("UNV", 1, 14)
   JPLTEXT=                        wstatus_type_om = 'UNV'
   JPLTEXT=                        call  change_status()
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        call update_payroll()
   JPLTEXT=
   JPLTEXT=        return(retval)
   JPLTEXT=
   JPLTEXT=}
   JPLTEXT=
  \
