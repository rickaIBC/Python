 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine: valid_hrscode
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine validates the hours code.
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2001-01-30      D. Bodlien
   JPLTEXT=//                      Change procedure valid_hrscode to always\
 select from the database.
   JPLTEXT=//      2007-03-15      E. Ramos        PR20070036 rework
   JPLTEXT=//                      Handle 'obsolete' flag on hrscodes.
   JPLTEXT=//      2008-09-09      T. Peace        PR20080582
   JPLTEXT=//                      Eliminate SQL error when validating user
   JPLTEXT=//                      entered quote character in hrscode field.
   JPLTEXT=//      2016-06-20      E. Ramos        CR20160062
   JPLTEXT=//                      Show week 1 and week 2 on the same screen.
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=proc valid_hrscode (fld_no, fld_val, fld_occ, fld_flag)
   JPLTEXT={
   JPLTEXT=        vars retval = 0, t_val(10), idx(10)
   JPLTEXT=        vars fld_len(10), i(10)
   JPLTEXT=        vars week_num
   JPLTEXT=
   JPLTEXT=        if ( !flag_changes(fld_no, fld_val, fld_occ, fld_flag) )
   JPLTEXT=        {
   JPLTEXT=                return (0)
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        // Verify user hasn't entered quotes in hrscode field.
   JPLTEXT=        v_hrcode = fld_val
   JPLTEXT=        fld_len = sm_n_dlength ("v_hrcode")
   JPLTEXT=        if fld_len > 0
   JPLTEXT=        {
   JPLTEXT=             for i = 1 while i < (fld_len + 1)
   JPLTEXT=             {
   JPLTEXT=                 if (fld_val(:i,1) = "'") || (fld_val(:i,1) = '"')
   JPLTEXT=                 {
   JPLTEXT=                      call ut5_msg_warning("Invalid hour code '" ##\
 fld_val ## "'.  Quote Characters are not permitted.")
   JPLTEXT=                      retval = 1
   JPLTEXT=                      return (retval)
   JPLTEXT=                      break
   JPLTEXT=                 }
   JPLTEXT=             }
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        //Pad the hrcode.
   JPLTEXT=
   JPLTEXT=        week_num = GetWeekNumberFromFieldNumber(fld_no)
   JPLTEXT=
   JPLTEXT=        t_val = ut_squeeze(fld_val, ut$SQUEEZE_LEAD_TRAIL, "")
   JPLTEXT=        if ( t_val == "" )
   JPLTEXT=        {
   JPLTEXT=                w:(week_num)hrdesc[fld_occ] = ""
   JPLTEXT=                return (0)
   JPLTEXT=        }
   JPLTEXT=        l_hrcode = ""
   JPLTEXT=        call ut_dbms_sql ("l_hrcode, l_hrdesc", \\
   JPLTEXT=                "select hrcode, hrdesc from hrscodes" ## \\
   JPLTEXT=                " where trim(hrcode) =':t_val' AND obsolete = 'N'\
 ")
   JPLTEXT=        idx = ut_object_search(t_val, "gLDB_HRCODE_STRIP", 1,\
 gLDB_HRCODE_OCC)
   JPLTEXT=        if (l_hrcode == "")
   JPLTEXT=        {
   JPLTEXT=
   JPLTEXT=                //Record not found.
   JPLTEXT=
   JPLTEXT=                call ut5_msg_warning("Invalid hour code\
 ':fld_val'.")
   JPLTEXT=                retval = 1
   JPLTEXT=        }
   JPLTEXT=        else
   JPLTEXT=        {
   JPLTEXT=                week_num = GetWeekNumberFromFieldNumber(fld_no)
   JPLTEXT=                w:(week_num)hrcode [fld_occ] = l_hrcode
   JPLTEXT=                w:(week_num)hrdesc [fld_occ] = l_hrdesc
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        return (retval)
   JPLTEXT=} //end proc valid_hrscode()
   JPLTEXT=
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine: GetWeekNumberFromFieldNumber(fld_no)
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine returns the week number for the given\
 field.
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2016-06-20      E. Ramos        CR20160062
   JPLTEXT=//                      Created.
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=string proc GetWeekNumberFromFieldNumber(fld_no)
   JPLTEXT=  vars fld_name
   JPLTEXT=  fld_name = @field_num(fld_no)->name
   JPLTEXT=  if ( fld_name(2,1)=="1" || fld_name(2,1)=="2")
   JPLTEXT=    return fld_name(2,1)
   JPLTEXT=return ""
   JPLTEXT=
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine: valid_envhazard
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine validates the environmental hazard\
 differential code.
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2011-07-25      A. McCormick    PR20110295
   JPLTEXT=//                      Modified validiation function to take the\
 quotes out before SQL.
   JPLTEXT=//      2011-08-23      A. McCormick    PR20110436
   JPLTEXT=//                      Moved quote substitution prior to length\
 check
   JPLTEXT=//      2011-09-16      A. McCormick    PR20110436 - rework
   JPLTEXT=//                      Replace value on screen if the\
 substitutions clear the value.
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=proc valid_envhazard (fld_no, fld_val, fld_occ, fld_flag)
   JPLTEXT={
   JPLTEXT=        vars retval = 0, t_val(10)
   JPLTEXT=
   JPLTEXT=        if ( !flag_changes(fld_no, fld_val, fld_occ, fld_flag) )
   JPLTEXT=        {
   JPLTEXT=                return (0)
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        //Pad the hrcode.
   JPLTEXT=
   JPLTEXT=        t_val = ut_squeeze(fld_val, ut$SQUEEZE_LEAD_TRAIL, "")
   JPLTEXT=        t_val = ut_substitute(t_val, "'", "")
   JPLTEXT=        t_val = ut_substitute(t_val, '"', '')
   JPLTEXT=        if ( @length(t_val) == 0 )
   JPLTEXT=        {
   JPLTEXT=                @field_num(fld_no)[fld_occ] = ''
   JPLTEXT=                return (0)
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        if ( !ut_dbms_exists("select count(*) from envhazcodes\
 where envhazarddiffcode = ':t_val'") )
   JPLTEXT=        {
   JPLTEXT=
   JPLTEXT=                //Record not found.
   JPLTEXT=
   JPLTEXT=                call ut5_msg_warning("Invalid environmental/hazard\
 differential code ':t_val'.")
   JPLTEXT=                retval = 1
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        if (fld_val != t_val)
   JPLTEXT=                @field_num(fld_no)[fld_occ] = t_val
   JPLTEXT=
   JPLTEXT=        return (retval)
   JPLTEXT=} //end proc valid_envhazard()
   JPLTEXT=
   JPLTEXT=
