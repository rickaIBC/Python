 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine:   comment_security
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine controls the security of the comment\
 sections.
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2024-08-15      A. McCormick    ENHC0037149
   JPLTEXT=//                      Created.
   JPLTEXT=//      2025-02-06      A. McCormick    DFCT0014157
   JPLTEXT=//                      Modified comment security in the event the\
 Timekeeper is not
   JPLTEXT=//                      part of the Amendment process so to still\
 display the Timekeeper
   JPLTEXT=//                      Comments to the Timekeeper and Certifier,\
 as needed.
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=proc comment_security()
   JPLTEXT={
   JPLTEXT=        vars sql_stmt(255)
   JPLTEXT=
   JPLTEXT=        emp_comment->attributes = ''
   JPLTEXT=        tk_comment ->attributes = ''
   JPLTEXT=
   JPLTEXT=        if(ts_status == '')
   JPLTEXT=        {
   JPLTEXT=                call get_timesheet_status()
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        //Employee specific comment buttons
   JPLTEXT=        call ut_web_link_repost("emp_delete_dl",\
 "do_delete_one_emp_cmt")
   JPLTEXT=        call ut_web_link_repost("emp_deleteall_dl",\
 "do_delete_all_emp_cmt")
   JPLTEXT=
   JPLTEXT=        //Timekeeper specific comment buttons
   JPLTEXT=        call ut_web_link_repost("tk_delete_dl",\
 "do_delete_one_tk_cmt")
   JPLTEXT=        call ut_web_link_repost("tk_deleteall_dl",\
 "do_delete_all_tk_cmt")
   JPLTEXT=        call ut_protect_field("description")
   JPLTEXT=
   JPLTEXT=        if (tna_data [idx$ACTIVE_ROLE] == gbl$ROLE_EMPLOYEE)
   JPLTEXT=        {
   JPLTEXT=                call ut_protect_field("tk_comment")
   JPLTEXT=                tk_delete_dl->hidden = PV_YES
   JPLTEXT=                tk_deleteall_dl->hidden = PV_YES
   JPLTEXT=
   JPLTEXT=                if(ts_status == 'UNV' || ts_status == 'VER')
   JPLTEXT=                {
   JPLTEXT=                        call ut_unprotect_field("emp_comment")
   JPLTEXT=                        emp_delete_dl->hidden = PV_NO
   JPLTEXT=                        emp_deleteall_dl->hidden = PV_NO
   JPLTEXT=                }
   JPLTEXT=                else
   JPLTEXT=                {
   JPLTEXT=                        call ut_protect_field("emp_comment")
   JPLTEXT=                        emp_delete_dl->hidden = PV_YES
   JPLTEXT=                        emp_deleteall_dl->hidden = PV_YES
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=        else if(( !amendment && tna_data [idx$ACTIVE_ROLE] ==\
 gbl$ROLE_TIMEKEEPER ) || \\
   JPLTEXT=                ( amendment && ut_bitop_and( cfg$AMENDMENT_ROLE,\
 tna_data[idx$ACTIVE_ROLE])) || \\
   JPLTEXT=                ( cfg$MIS_NO_TKROLE > 0 && tna_data\
 [idx$ACTIVE_ROLE] == gbl$ROLE_CERTIFIER) )
   JPLTEXT=        {
   JPLTEXT=                call ut_protect_field("emp_comment")
   JPLTEXT=                emp_delete_dl->hidden = PV_YES
   JPLTEXT=                emp_deleteall_dl->hidden = PV_YES
   JPLTEXT=
   JPLTEXT=                if(ts_status == 'UNV' || ts_status == 'VER' ||\
 ts_status == 'VAL' || cfg$MIS_NO_TKROLE > 0)
   JPLTEXT=                {
   JPLTEXT=                        call ut_unprotect_field("tk_comment")
   JPLTEXT=                        tk_delete_dl->hidden = PV_NO
   JPLTEXT=                        tk_deleteall_dl->hidden = PV_NO
   JPLTEXT=                }
   JPLTEXT=                else
   JPLTEXT=                {
   JPLTEXT=                        call ut_protect_field("tk_comment")
   JPLTEXT=                        tk_delete_dl->hidden = PV_YES
   JPLTEXT=                        tk_deleteall_dl->hidden = PV_YES
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=        else
   JPLTEXT=        {
   JPLTEXT=                call ut_protect_field("emp_comment")
   JPLTEXT=                call ut_protect_field("tk_comment")
   JPLTEXT=
   JPLTEXT=                emp_delete_dl->hidden = PV_YES
   JPLTEXT=                emp_deleteall_dl->hidden = PV_YES
   JPLTEXT=                tk_delete_dl->hidden = PV_YES
   JPLTEXT=                tk_deleteall_dl->hidden = PV_YES
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        return (0)
   JPLTEXT=}
