 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine:          do_populate_status_bor
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine populates the value OPEN in the status\
 column (dyncost13)
   JPLTEXT=//              in all the rows where a key value is present, and\
 clears the status column
   JPLTEXT=//              in those rows where no key value is present.
   JPLTEXT=//              This routine simply exits when\
 cfg$COST_COMPONENTS_COST_KEY_BOR = 0.
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2021-06-04      E. Ramos        ENHC0036192
   JPLTEXT=//                      Created.
   JPLTEXT=//      2021-06-25      E. Ramos        ENHC0036192 REWORK
   JPLTEXT=//                      For BOR, clear all component values from\
 timesheet lines where the cost key is blank.
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=proc do_populate_status_bor()
   JPLTEXT={
   JPLTEXT=        vars week_num, i, j, num_occ
   JPLTEXT=
   JPLTEXT=        if (cfg$COST_COMPONENTS_COST_KEY_BOR == 0)
   JPLTEXT=                return
   JPLTEXT=
   JPLTEXT=        for week_num = 1 while week_num <= 2
   JPLTEXT=        {
   JPLTEXT=                num_occ = ut_max( w:(week_num)key->num_occurrences,\
 w:(week_num)dyncost13->num_occurrences)
   JPLTEXT=                num_occ = ut_max( num_occ,\
 w:(week_num)hrcode->num_occurrences)
   JPLTEXT=
   JPLTEXT=                for i = 1 while i <= num_occ
   JPLTEXT=                {
   JPLTEXT=                        if (cfg$DEVELOPMENT)
   JPLTEXT=                                call ut_log_error("enhc0036192\
 do_populate_status_bor week_num (:week_num) i (:i)")
   JPLTEXT=
   JPLTEXT=                        for j = 1 while j <= 21
   JPLTEXT=                        {
   JPLTEXT=                                if(dyncost_show_on_screen[j] ==\
 'Y')
   JPLTEXT=                                {
   JPLTEXT=                                        tvar = tvar ##\
 w:(week_num)dyncost:(j)[i]
   JPLTEXT=                                }
   JPLTEXT=                        }
   JPLTEXT=
   JPLTEXT=                        tvar = ut_squeeze(tvar,\
 ut$SQUEEZE_LEAD_TRAIL, "")
   JPLTEXT=
   JPLTEXT=                        if ((w:(week_num)key[i] != "") &&\
 (@length(tvar) > 0))
   JPLTEXT=                        {
   JPLTEXT=                                w:(week_num)dyncost13[i] = "OPEN"
   JPLTEXT=                        }
   JPLTEXT=                        else
   JPLTEXT=                        {
   JPLTEXT=                                for j = 1 while j <= 21
   JPLTEXT=                                {
   JPLTEXT=                                        // Clear values of all the\
 components in the row
   JPLTEXT=                                        w:(week_num)dyncost:(j)[i]\
 = ""
   JPLTEXT=                                }
   JPLTEXT=                        }
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=}
   JPLTEXT=
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine:   update_default_preprint
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine updates payroll data for Default and\
 Pre-Print timesheets.
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2023-06-02      A. McCormick    DFCT0013320
   JPLTEXT=//                      Created update_default_preprint() and\
 default_preprint_hrscodes()
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=proc update_default_preprint ()
   JPLTEXT={
   JPLTEXT=        vars retval(10),num_occ(10)
   JPLTEXT=        vars w1_row_cnt(10)
   JPLTEXT=        vars w2_row_cnt(10)
   JPLTEXT=        vars temp_save_flag(10)
   JPLTEXT=        vars timestamp_format(20)
   JPLTEXT=        vars ts_type_lbl
   JPLTEXT=
   JPLTEXT=        temp_save_flag = save_flag
   JPLTEXT=        if ( !security_flag ) && ( !save_flag )  && (!status_flag)\
 && (!edit_reqd)
   JPLTEXT=        {
   JPLTEXT=                if ( status_flag )
   JPLTEXT=                {
   JPLTEXT=                        unauthorized_action = gbl$TRUE
   JPLTEXT=                }
   JPLTEXT=                return (0)
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        if ( pp == gbl$SCHD_DFLT )
   JPLTEXT=                ts_type_lbl = "Default Timesheet"
   JPLTEXT=        else if ( pp == gbl$SCHD_PRE )
   JPLTEXT=                ts_type_lbl = "Pre-print Timesheet"
   JPLTEXT=
   JPLTEXT=        UsrMadeChg = gbl$TRUE
   JPLTEXT=        status_flag = gbl$TRUE
   JPLTEXT=        edit_reqd = gbl$TRUE
   JPLTEXT=
   JPLTEXT=        // Prepare component values to be saved (i.e. components\
 with formatting = 10, which means they are substrings of other components)
   JPLTEXT=        call dyncost_prepare_save()
   JPLTEXT=
   JPLTEXT=        w1_row_cnt = @widget("w1hrcode") -> num_occurrences
   JPLTEXT=        w2_row_cnt = @widget("w2hrcode") -> num_occurrences
   JPLTEXT=
   JPLTEXT=        if (w1_row_cnt > 100) || (w2_row_cnt > 100 )
   JPLTEXT=        {
   JPLTEXT=                // empty rows found log empid,pp-yr,counts
   JPLTEXT=                call ut_log_error(":ts_type_lbl - ROWS > 100 for\
 :+active_empid in :ppyear - :pp wk1==> :w1_row_cnt, wk2==> :w2_row_cnt ")
   JPLTEXT=                call ut_log_error(":ts_type_lbl - wk1==> :w1hrcode \
 wk2==> :w2hrcode ")
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        if (cfg$websphere)
   JPLTEXT=        {
   JPLTEXT=                obj_retval = \\
   JPLTEXT=                        sm_obj_call (payroll_hndl,\
 "update_default_preprint", active_empid, pp, ppyear, active_role, \\
   JPLTEXT=                        w1hrcode, w1day_1, w1day_2, w1day_3,\
 w1day_4, w1day_5, w1day_6, w1day_7, w1key, w1cost, w1shift, \\
   JPLTEXT=                        w1envcode, w1telework, w1base, w1occ,\
 w1poscode, w1mentoring,  w1taskcode, w1pp, w1hrdesc, w1costdesc, \\
   JPLTEXT=                        w2hrcode, w2day_1, w2day_2, w2day_3,\
 w2day_4, w2day_5, w2day_6, w2day_7, w2key, w2cost, w2shift, \\
   JPLTEXT=                        w2envcode, w2telework, w2base, w2occ,\
 w2poscode, w2mentoring, w2taskcode, w2pp, w2hrdesc, w2costdesc, \\
   JPLTEXT=                        mgr_id, login_id, status_dtg,\
 cur_status_dtg, status_array, tna_data, \\
   JPLTEXT=                        w1dyncost1, w1dyncost2, w1dyncost3,\
 w1dyncost4, w1dyncost5, w1dyncost6, w1dyncost7, \\
   JPLTEXT=                        w1dyncost8, w1dyncost9, w1dyncost10,\
 w1dyncost11, w1dyncost12, w1dyncost13, w1dyncost14, \\
   JPLTEXT=                        w1dyncost15, w1dyncost16,\
 w1dyncost17,w1dyncost18, w1dyncost19, w1dyncost20, w1dyncost21, \\
   JPLTEXT=                        w2dyncost1, w2dyncost2, w2dyncost3,\
 w2dyncost4, w2dyncost5, w2dyncost6, w2dyncost7, \\
   JPLTEXT=                        w2dyncost8, w2dyncost9, w2dyncost10,\
 w2dyncost11, w2dyncost12, w2dyncost13, w2dyncost14, \\
   JPLTEXT=                        w2dyncost15, w2dyncost16, w2dyncost17,\
 w2dyncost18, w2dyncost19, w2dyncost20, w2dyncost21,\\
   JPLTEXT=                        UsrMadeChg, status_flag,\
 edit_reqd,ed_error_count,ed_warning_count,err_img,err_msg_dl )
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        retval = ut_service_error()
   JPLTEXT=
   JPLTEXT=        save_flag = retval
   JPLTEXT=        del_row_flag = gbl$FALSE
   JPLTEXT=
   JPLTEXT=        if (ed_error_count > 0 )
   JPLTEXT=        {
   JPLTEXT=                validation_error = gbl$TRUE
   JPLTEXT=
   JPLTEXT=                call change_status_array("UNV", 1, 14)
   JPLTEXT=                orig_status = status_array[14]
   JPLTEXT=
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        //Display concurrency message.
   JPLTEXT=
   JPLTEXT=        if ( retval == gbl$DBMS_DATA_CONCURRENCY )  || ( retval ==\
 gbl$DBMS_PREVIOUSLY_TRANSMITTED )
   JPLTEXT=        {
   JPLTEXT=                call do_select()
   JPLTEXT=                call sm_n_clear_array("err_msg_dl")
   JPLTEXT=                call sm_n_clear_array("err_img")
   JPLTEXT=                ed_error_count = 0
   JPLTEXT=                err_msg_idx    = 0
   JPLTEXT=                validation_error = gbl$FALSE
   JPLTEXT=
   JPLTEXT=                if ( retval == gbl$DBMS_DATA_CONCURRENCY )
   JPLTEXT=                {
   JPLTEXT=                        call ut5_msg_warning(":ts_type_lbl data not\
 saved to the database" ## \\
   JPLTEXT=                        " because dependent data was changed.%N" ##\
 \\
   JPLTEXT=                        "The latest data has been retrieved from\
 the" ## \\
   JPLTEXT=                        " database for your convenience.(13)")
   JPLTEXT=                }
   JPLTEXT=                else if ( retval == gbl$DBMS_PREVIOUSLY_TRANSMITTED\
 )
   JPLTEXT=                {
   JPLTEXT=                     previously_transmitted_flag = gbl$TRUE
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=        else if (retval)
   JPLTEXT=        {
   JPLTEXT=             // Service call error encountered - Notify user
   JPLTEXT=             // and retrieve last saved timesheet.
   JPLTEXT=             if (week_num == 2)
   JPLTEXT=             {
   JPLTEXT=                  // Need to clear these flags to prevent
   JPLTEXT=                  // change week save processing when
   JPLTEXT=                  // do_select executes.
   JPLTEXT=                  //
   JPLTEXT=                  security_flag = gbl$FALSE
   JPLTEXT=                  save_flag  = gbl$FALSE
   JPLTEXT=                  status_flag  = gbl$FALSE
   JPLTEXT=                  edit_reqd  = gbl$FALSE
   JPLTEXT=             }
   JPLTEXT=
   JPLTEXT=                call do_select()
   JPLTEXT=
   JPLTEXT=                call sm_n_clear_array("err_msg_dl")
   JPLTEXT=                call sm_n_clear_array("err_img")
   JPLTEXT=                ed_error_count = 0
   JPLTEXT=                err_msg_idx    = 0
   JPLTEXT=
   JPLTEXT=                validation_error = gbl$FALSE
   JPLTEXT=
   JPLTEXT=                timestamp_format = "n%0h::%0M::%0s"
   JPLTEXT=                call sm_n_putfield ("error_time", sm_sdtime\
 (timestamp_format))
   JPLTEXT=                timestamp_format = "n%0m/%0d/%4y"
   JPLTEXT=                call sm_n_putfield ("error_date", sm_sdtime\
 (timestamp_format))
   JPLTEXT=
   JPLTEXT=                call ut5_msg_warning("%N:ts_type_lbl data not saved\
 to the database" ## \\
   JPLTEXT=                  " due to an unexpected system error.%N" ## \\
   JPLTEXT=                  "If problem persists print this screen and \
 contact the Help Desk." ## \\
   JPLTEXT=                  "%NThe lastest data has been retrieved from"   ##\
 \\
   JPLTEXT=                  " the database. %N :error_date :error_time ")
   JPLTEXT=        }
   JPLTEXT=        else
   JPLTEXT=        {
   JPLTEXT=                // No error, continue with timesheet processing.
   JPLTEXT=
   JPLTEXT=                retval = ut_parse_focus_error()
   JPLTEXT=                //Re-sync the err_msg_idx.
   JPLTEXT=                num_occ = @widget("err_sync")->num_occurrences
   JPLTEXT=                if ( num_occ > 0 )
   JPLTEXT=                {
   JPLTEXT=                        call set_err_idx(num_occ)
   JPLTEXT=                }
   JPLTEXT=                else
   JPLTEXT=                {
   JPLTEXT=                        call set_err_idx("1")
   JPLTEXT=                }
   JPLTEXT=
   JPLTEXT=                //Re-size error widgets.
   JPLTEXT=
   JPLTEXT=                if ( (ed_error_count > 0) || (ed_warning_count > 0)\
 )
   JPLTEXT=                {
   JPLTEXT=                        @widget("err_img")->length = 13
   JPLTEXT=                }
   JPLTEXT=
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        save_flag = 0
   JPLTEXT=        status_flag = 0
   JPLTEXT=        edit_reqd = 0
   JPLTEXT=
   JPLTEXT=        if !(retval) && (temp_save_flag == 1) && (display_save_msg)
   JPLTEXT=        {
   JPLTEXT=                call ut5_msg_information(":ts_type_lbl data\
 saved.")
   JPLTEXT=        }
   JPLTEXT=        if (!retval)
   JPLTEXT=        {
   JPLTEXT=                // Clear flags when save is successful
   JPLTEXT=                add_del_flag = 0
   JPLTEXT=                del_row_flag = 0
   JPLTEXT=        }
   JPLTEXT=        call get_dyncostdescs()
   JPLTEXT=        call get_favorites_payroll_data()
   JPLTEXT=        return (retval)
   JPLTEXT=} //end proc update_default_preprint()
   JPLTEXT=
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=//      Routine:   default_preprint_hrscodes
   JPLTEXT=//
   JPLTEXT=//      Description:
   JPLTEXT=//              This routine determines if there are hour codes on\
 the current timesheet
   JPLTEXT=//              that cannot be saved as part of the Default or\
 Pre-Print timesheets.
   JPLTEXT=//
   JPLTEXT=//      Last Modified:
   JPLTEXT=//      2023-06-02      A. McCormick    DFCT0013320
   JPLTEXT=//                      Created update_default_preprint() and\
 default_preprint_hrscodes()
   JPLTEXT=//
  \
 JPLTEXT=////////////////////////////////////////////////////////////////////
   JPLTEXT=
   JPLTEXT=proc default_preprint_hrscodes()
   JPLTEXT={
   JPLTEXT=        vars i,j,k,idx
   JPLTEXT=        vars l_hrcode
   JPLTEXT=        vars retval = 0
   JPLTEXT=
   JPLTEXT=        // Loop through the hour codes on week 1 and week 2 and\
 compare them to the list of
   JPLTEXT=        // hour codes that can be used on the default or pre-print\
 timesheet. If any hour
   JPLTEXT=        // codes are found that cannot be used then display error\
 and do not save timesheet.
   JPLTEXT=
   JPLTEXT=        for i = 1 while i <= w1hrcode->num_occurrences
   JPLTEXT=        {
   JPLTEXT=                j = 0
   JPLTEXT=                k = 0
   JPLTEXT=                l_hrcode = ut_squeeze (w1hrcode[i],\
 ut$SQUEEZE_LEAD_TRAIL, "")
   JPLTEXT=                j = ut_object_search (l_hrcode,\
 "gLDB_HRCODE_STRIP", 1, gLDB_HRCODE_OCC)
   JPLTEXT=                if (j != 0)
   JPLTEXT=                {
   JPLTEXT=                        k = (ut_bitop_and (gLDB_EDIT_BITS_3 [j],\
 gbl$EDIT_BITS_DEFAULT_PREPRINT))
   JPLTEXT=
   JPLTEXT=                        if k == 0
   JPLTEXT=                        {
   JPLTEXT=                                call ut5_msg_warning("Week 1 - Hour\
 Code :l_hrcode cannot be saved to a Default or Pre-Print timesheet.")
   JPLTEXT=                                retval = -1
   JPLTEXT=                        }
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=
   JPLTEXT=        for i = 1 while i <= w2hrcode->num_occurrences
   JPLTEXT=        {
   JPLTEXT=                j = 0
   JPLTEXT=                k = 0
   JPLTEXT=                l_hrcode = ut_squeeze (w2hrcode[i],\
 ut$SQUEEZE_LEAD_TRAIL, "")
   JPLTEXT=                j = ut_object_search (l_hrcode,\
 "gLDB_HRCODE_STRIP", 1, gLDB_HRCODE_OCC)
   JPLTEXT=                if (j != 0)
   JPLTEXT=                {
   JPLTEXT=                        k = (ut_bitop_and (gLDB_EDIT_BITS_3 [j],\
 gbl$EDIT_BITS_DEFAULT_PREPRINT))
   JPLTEXT=
   JPLTEXT=                        if k == 0
   JPLTEXT=                        {
   JPLTEXT=                                call ut5_msg_warning("Week 2 - Hour\
 Code :l_hrcode cannot be saved to a Default or Pre-Print timesheet.")
   JPLTEXT=                                retval = -1
   JPLTEXT=                        }
   JPLTEXT=                }
   JPLTEXT=        }
   JPLTEXT=
   JPLTEXT=        return (retval)
   JPLTEXT=}
   JPLTEXT=
